    --zxr10ptpDomain

        -- ptp domain delay mechanism
        zxr10PTPConfigDDM OBJECT-TYPE
              SYNTAX  INTEGER {
                  e2e(1),
                  p2p(2)
                       }
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "The ptp domain delay mechanism."
              ::= { zxr10ptpDomain 1 }

        -- ptp domain number
        zxr10PTPConfigDomainNum OBJECT-TYPE
              SYNTAX  Integer32 (0..255)
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "The ptp domain number."
              ::= { zxr10ptpDomain 2 }

        -- ptp announce interval
        zxr10PTPConfigAnnounceInterval OBJECT-TYPE
              SYNTAX  Integer32 (-4..4)
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "The ptp announce interval,the range is -1..4 for t8000\M6k
                      and 89;-4..4 for upp3081 and 15k."
              ::= { zxr10ptpDomain 3 }

        -- ptp announce timeout
        zxr10PTPConfigAnnounceTimeout OBJECT-TYPE
              SYNTAX  Integer32 (2..10)
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "The ptp announce timeout."
              ::= { zxr10ptpDomain 4 }

        -- ptp sync interval
        zxr10PTPConfigSyncInterval OBJECT-TYPE
              SYNTAX  Integer32 (-8..1)
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "The ptp sync interval.for t8000\M6k the range is -7..1,
                      for 89 is -6..1,for 15k is -7..1,upp3081 does not support."
              ::= { zxr10ptpDomain 5 }

        -- ptp delay-req-interval
        zxr10PTPConfigDelayReqInterval OBJECT-TYPE
              SYNTAX  Integer32 (-7..5)
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "The ptp delay req interval,for t8000\M6k and 15k the range is
                      -7..1,for 89 the range is -5..5,upp3081 does not support."
              ::= { zxr10ptpDomain 6 }

        -- ptp pdelay-req-interval
        zxr10PTPConfigPDelayReqInterval OBJECT-TYPE
              SYNTAX  Integer32 (-4..5)
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "The ptp pdelay req interval,
                      for t8000\M6k and 89 the range is 0..5,
                      for 15k the range is -4..5,upp3081 does not support."
              ::= { zxr10ptpDomain 7 }


