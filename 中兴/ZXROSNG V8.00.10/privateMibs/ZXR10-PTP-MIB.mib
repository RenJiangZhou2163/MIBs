    --zxr10ptpIntfConfTable,the ptp intf configure table

        zxr10ptpIntfConfTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10ptpIntfConfEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "A table containing PTP port configure
                      information."
              ::= { zxr10ptp 4 }

        zxr10ptpIntfConfEntry OBJECT-TYPE
              SYNTAX  Zxr10ptpIntfConfEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "Information of PTP port."
              INDEX   {
                  zxr10PTPPortConfigName
                      }
              ::= { zxr10ptpIntfConfTable 1 }

        Zxr10ptpIntfConfEntry ::=
            SEQUENCE {
                         zxr10PTPPortIndex
                             Integer32,
                         zxr10PTPPortConfigName
                             DisplayString,
                         zxr10PTPPortConfigEnable
                             INTEGER,
                         zxr10PTPPortConfigState
                             INTEGER,
                         zxr10PTPPortConfigSyncInterval
                             Integer32,
                         zxr10PTPPortConfigDelayReqInterval
                             Integer32,
                         zxr10PTPPortConfigMultiDomainEnable
                             INTEGER,
                         zxr10PTPPortConfigDelayMech
                             INTEGER,
                         zxr10PTPPortNetProtocol
                             INTEGER,
                         zxr10PTPPortPacketMechanism
                             INTEGER,
                         zxr10PTPShowPortState
                             INTEGER,
                         zxr10PTPPortConfigTransEnable
                             INTEGER,
                         zxr10PTPPortUnSymmetry
                             Unsigned32,
                         zxr10PTPPortUnSign
                             INTEGER,
                         zxr10PTPPortVlanId
                             Integer32,
                         zxr10PTPPortSourceName
                             DisplayString,
                         zxr10PTPPortPDelayReqInterval
                           Integer32,
                         zxr10PTPPortNum
                             Integer32,
                         zxr10PTPPortConfigRowStatus
                             RowStatus
                             }

        zxr10PTPPortIndex OBJECT-TYPE
            SYNTAX           Integer32(1..2147483647)
            MAX-ACCESS not-accessible
            STATUS     obsolete
            DESCRIPTION
                   "Object which specifies a unique entry in the
                    Zxr10PtpIntfConfEntry."
            ::= { zxr10ptpIntfConfEntry 1 }

        zxr10PTPPortConfigName OBJECT-TYPE
            SYNTAX  DisplayString (SIZE (1..32))
