            zxr10MplsTeInterfaceObjects OBJECT IDENTIFIER ::= { zxr10MplsTeMIB 8  }
            zxr10MplsIpExplicitObjects OBJECT IDENTIFIER ::= { zxr10MplsTeMIB 9  }
            zxr10MplsPathOptObjects OBJECT IDENTIFIER ::= { zxr10MplsTeMIB 10 }
            zxr10MplsAdminGroupListObjects OBJECT IDENTIFIER ::= { zxr10MplsTeMIB 11 }
            zxr10MplsHotStandbyLspObjects  OBJECT IDENTIFIER ::= { zxr10MplsTeMIB 12 }


        -- MPLS Tunnel scalars.
         zxr10MplsTunnelEnable OBJECT-TYPE
            SYNTAX INTEGER { false(0), true(1) }
            MAX-ACCESS read-only
            STATUS obsolete
            DESCRIPTION
                "If this object is true, then it enables the zxr10MplsTunnel."
            ::= { zxr10MplsTeScalars 1 }


         zxr10MplsTunnelConfiguredNum OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The number of tunnels configured on this device. A
                tunnel is considered configured if the
                zxr10MplsTunnelRowStatus is active(1)."
            ::= { zxr10MplsTeScalars 2 }


         zxr10MplsTunnelTEDistProto OBJECT-TYPE
            SYNTAX INTEGER {
                other (0),
                ospf (1),
                isis (2),
                both (3)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
