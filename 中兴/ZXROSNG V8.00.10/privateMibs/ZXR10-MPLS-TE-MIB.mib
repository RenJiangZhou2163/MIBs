            ::= { zxr10MplsTeScalars 8 }

         zxr10MplsTunnelGrRestartTime OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS obsolete
            DESCRIPTION
                "Graceful-restart time 0~300000."
            ::= { zxr10MplsTeScalars 9 }

         zxr10MplsTunnelSignallingRefreshInterval OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS obsolete
            DESCRIPTION
                "RefreshInterval 5000~65535000"
            ::= { zxr10MplsTeScalars 10 }

         zxr10MplsTunnelSignallingRefreshMisses OBJECT-TYPE
            SYNTAX Unsigned32
            MAX-ACCESS read-only
            STATUS obsolete
            DESCRIPTION
                "SignallingRefreshMisses 2~10"
            ::= { zxr10MplsTeScalars 11 }

       --added
         zxr10MplsVersion OBJECT-TYPE
