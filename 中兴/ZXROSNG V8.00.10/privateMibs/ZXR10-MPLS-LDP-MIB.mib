-- *******************************************************************
--   Trap Definitions
-- *******************************************************************

zxr10LdpNotify  OBJECT IDENTIFIER ::= { zxr10mplsLdpMIB 15 }

zxr10LdpSesUp NOTIFICATION-TYPE
    OBJECTS  { zxr10LdpRtrid,
               zxr10LdpSessState,
               zxr10LdpOutIfName,
               zxr10LdpInstanceName,
               zxr10LdpRtridAddrType }
    STATUS   current
    DESCRIPTION
        "The SNMP trap that is generated when the value of 'zxr10MplsLdpSesState'
     enters the 'operational(5) state."
    ::= { zxr10LdpNotify 1 }

zxr10LdpSesDown NOTIFICATION-TYPE
    OBJECTS  { zxr10LdpRtrid,
               zxr10LdpSessState, 
               zxr10LdpSesDownReason,
               zxr10LdpOutIfName,
               zxr10LdpInstanceName,
               zxr10LdpRtridAddrType } 
    STATUS   current
    DESCRIPTION
        "The SNMP trap that is generated when the value of 'zxr10MplsLdpSesState'
    leaves the 'operational(5) state. The value of zxr10LdpSesDownIf is 
    one of the neighbor's interface. It is the interface associated with
    the last neighbor when zxr10LdpSesDownReason is allAdjacenciesDown (3)."
    ::= { zxr10LdpNotify 2 }

zxr10LdpLspUp NOTIFICATION-TYPE
    OBJECTS  { zxr10LdpLspFec, 
               zxr10LdpLspFecLen, 
               zxr10LdpInstanceName,
               zxr10LdpLspFecAddrType }
    STATUS   current
    DESCRIPTION
        "The SNMP trap that is generated when an LSP comes up."
    ::= { zxr10LdpNotify 3 }

zxr10LdpLspDown NOTIFICATION-TYPE
    OBJECTS  { zxr10LdpLspFec,
               zxr10LdpLspFecLen,
               zxr10LdpLspDownReason,
               zxr10LdpInstanceName }
    STATUS   current
    DESCRIPTION
        "The SNMP trap that is generated when the LSP goes down."
    ::= { zxr10LdpNotify 4 }

zxr10LdpLspPathUp NOTIFICATION-TYPE
    OBJECTS  { zxr10LdpLspFec,
               zxr10LdpLspFecLen,
               zxr10LdpOutIfName,
               zxr10LdpInstanceName,
               zxr10LdpLspFecAddrType }
    STATUS   current
    DESCRIPTION
        "An mplsLspPathUp trap signifies that the specified path mplsPathName
         for the specified LSP mplsLspName came up."
    ::= { zxr10LdpNotify 5 }
         
zxr10LdpLspPathDown NOTIFICATION-TYPE
    OBJECTS  { zxr10LdpLspFec,
               zxr10LdpLspFecLen,
               zxr10LdpOutIfName,
               zxr10LdpInstanceName,
               zxr10LdpLspFecAddrType }
    STATUS   current
    DESCRIPTION
        "An mplsLspPathDown trap signifies that the specified path mplsPathName
         for the specified LSP mplsLspName went down."
    ::= { zxr10LdpNotify 6 }
    
zxr10LdpLspChange NOTIFICATION-TYPE
    OBJECTS  { zxr10LdpLspFec, 
               zxr10LdpLspFecLen, 
               zxr10LdpInstanceName,
               zxr10LdpLspFecAddrType }
    STATUS   current
    DESCRIPTION
        "An mplsLspChange trap signifies that the specified LSP has switched traffic 
        to the new active path 'toLspPath'. The LSP maintains up state before and 
        after the switch over."
    ::= { zxr10LdpNotify 7 }

    
