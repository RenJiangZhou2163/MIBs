ZXR10-MPLS-LDP-MIB DEFINITIONS ::= BEGIN  
     
IMPORTS
       OBJECT-TYPE, 
       MODULE-IDENTITY,  
       NOTIFICATION-TYPE,             
       Counter32,Counter64,Unsigned32,enterprises,IpAddress
             FROM SNMPv2-SMI
       DisplayString,TimeStamp
             FROM SNMPv2-TC 

       MplsLdpIdentifier
             FROM MPLS-TC-STD-MIB                          --  [RFC3811]

       InetAddressPrefixLength,
       InetAddressType,
       InetAddress
             FROM INET-ADDRESS-MIB;                         --  [RFC3291]
             
             
zxr10mplsLdpMIB MODULE-IDENTITY
         LAST-UPDATED "201511260900Z" 
         ORGANIZATION "Multiprotocol Label Switching (mpls)
                       Working Group"
         CONTACT-INFO
             "Joan Cucchiara (jcucchiara@crescentnetworks.com)
              Crescent Networks

              Hans Sjostrand (hans@ipunplugged.com)
              ipUnplugged

              James V. Luciani (jluciani@crescentnetworks.com)
              Crescent Networks

              Working Group Chairs:
              George Swallow,   email: swallow@cisco.com
              Vijay Srinivasan, email: vijay@cosinecom.com
              MPLS Working Group, email: mpls@uu.net
         "
         DESCRIPTION
             "This MIB contains managed object definitions for the
             'Multiprotocol Label Switching, Label Distribution
             Protocol, LDP' document."
         REVISION "201511260900Z"
         DESCRIPTION
             "modify: change MAX-ACCESS from read-only to not-accessible
                      of below nodes in zxr10LdpBfdTable:
                   zxr10LdpBfdAddrType
                   zxr10LdpBfdDstAddr
                   zxr10LdpBfdPrefixLength
             "
         REVISION "201401090900Z"
         DESCRIPTION
             "add: zxr10LdpIgpSyncIntTable
              "
         REVISION "201406261200Z"
         DESCRIPTION
             "modify: change DEFVAL from number to defined word 
                      of below tables or nodes:
                   zxr10LdpGr zxr10LdpLabelRetentionMode
                   zxr10LdpLspControlMode zxr10LdpGlblAutoCfgMode
                   zxr10LdpV6GlblAutoCfgMode zxr10LdpV4IntfLblDistributeMode
                   zxr10LdpV4IntfGtsm zxr10LdpV6IntfLblDistributeMode
             "    
         REVISION "201403051200Z"
         DESCRIPTION
             "modify: change zxr10LdpConnWithDod STATUS to 'obsolete'
             "    
         REVISION "201402271200Z"
         DESCRIPTION
             "modify: modify word spell fault of below 
                      tables or nodes decription:
                   zxr10LdpLabelAdvertise zxr10LdpInstCfgEntry
                   zxr10LdpFecAddr zxr10LdpGrSessNeighborLiveTime
                   zxr10LdpInterfaceEntry zxr10LdpLabelAdvertiseEntry
                   zxr10LdpBfdSrcAddr
             "
         REVISION "201401071200Z"
         DESCRIPTION
             "add: zxr10LdpV6TrafficCntAcl in zxr10LdpInstCfgTable
                   zxr10LdpV6GlblAutoCfgMode in zxr10LdpInstCfgTable
                   zxr10LdpV6DisAutoCfgIntTable
              modify: modify zxr10LdpTrafficCntAcl node description
                      modify zxr10LdpGlblAutoCfgMode node description
              "
         REVISION "201312231200Z"
         DESCRIPTION
             "add: zxr10LdpPeerBfdDelay,zxr10LdpPeerBfdTime
                   in zxr10LdpPeerBfdTable
             "    
         REVISION "201311041200Z"
         DESCRIPTION
             "add: zxr10LdpBfdSrcAddr,zxr10LdpBfdProtectBind
                   in zxr10LdpBfdTable"
         REVISION "201310281200Z"
         DESCRIPTION
             "modify: modify word spell fault of below 
                      tables or nodes decription:
                   zxr10LdpGrTable zxr10LdpGrEntry
                   zxr10LdpGrSessMaxRecoveryTime zxr10LdpMd5PwdFlag
                   zxr10LdpSessTable zxr10LdpSessHelloNumber
                   zxr10LdpSessTcpRetry zxr10LdpDirectHelloHoldTime
                   zxr10LdpDirectHelloInterTime zxr10LdpInstanceNo
                   zxr10LdpInstNo zxr10LdpV4IntfLdpInstNo
                   zxr10LdpV6IntfLdpInstNo zxr10LdpGtsmLdpInstNo
                   zxr10LdpPeerBfdLdpInstNo zxr10LdpV4DisACIntfInstNo
             "
         REVISION "201307261200Z"
         DESCRIPTION
             "add: zxr10LdpGlblAutoCfgMode in zxr10LdpInstCfgTable
                   zxr10LdpV4DisAutoCfgIntTable
             "
         REVISION "201307091200Z"
         DESCRIPTION
             "add: zxr10LdpLostIngressOctets in zxr10LdpFecTable
                   zxr10LdpLostIngressPackets in zxr10LdpFecTable
                   zxr10LdpLostTransitOctets in zxr10LdpFecTable
                   zxr10LdpLostTransitPackets in zxr10LdpFecTable
             "
         REVISION "201305081200Z"
         DESCRIPTION
             "add: zxr10LdpTrafficCntAcl in zxr10LdpInstCfgTable
              modify: change SYNTAX from Counter32 to Counter64
                      of below nodes in zxr10LdpFecTable:
                      zxr10LdpIngressOctets
                      zxr10LdpIngressPackets
                      zxr10LdpTransitOctets
                      zxr10LdpTransitPackets
             "
         REVISION "201302281200Z"
         DESCRIPTION
             "add: zxr10LdpInstCfgTable
                   zxr10LdpV4IntfCfgTable
                   zxr10LdpV6IntfCfgTable
                   zxr10LdpGtsmTable
                   zxr10LdpPeerBfdTable
             "
         REVISION "200108161200Z"
         DESCRIPTION
             "Initial revision."
         ::= { zxr10MplsLdp 1} 
     
      zte OBJECT IDENTIFIER ::= { enterprises 3902 }
      zxr10 OBJECT IDENTIFIER ::= { zte 3} 
      zxr10MplsLdp OBJECT IDENTIFIER ::= { zxr10 314}     
   
      zxr10LdpLsrAttrObjects OBJECT IDENTIFIER ::={ zxr10mplsLdpMIB 14}
      

--********************************************         
--difinition of objects ::  
--********************************************   

zxr10LdpTrapVarsObject  OBJECT IDENTIFIER ::= { zxr10mplsLdpMIB 5 }

zxr10LdpLspFec OBJECT-TYPE
            SYNTAX        InetAddress
            MAX-ACCESS    accessible-for-notify
            STATUS        current
            DESCRIPTION
            "The LSP FEC in IP address format."
            ::= { zxr10LdpTrapVarsObject 1 }

zxr10LdpLspFecLen OBJECT-TYPE
            SYNTAX        InetAddressPrefixLength
            MAX-ACCESS    accessible-for-notify
            STATUS        current
            DESCRIPTION
            "The length in bits of the LSP FEC prefix."
            ::= { zxr10LdpTrapVarsObject 2 }
        
zxr10LdpLspDownReason OBJECT-TYPE
            SYNTAX        INTEGER { 
                            null (0),
                            topologyChanged (1), 
                            receivedWithdrawl (2),
                            neighborDown (3),
                            filterChanged (4),
                            bfdSessionDown (5),
                            unknown (6) }
            MAX-ACCESS    accessible-for-notify
            STATUS        current
            DESCRIPTION
            "The reason why the LSP went down. Can be one of the following:
            the topology changed, the neighbor withdrew the label, 
            the neighbor went down, the filter changed, the BFD session
            went down or the reason is unknown."
            ::= { zxr10LdpTrapVarsObject 3 }

zxr10LdpRtrid OBJECT-TYPE
            SYNTAX        InetAddress
            MAX-ACCESS    accessible-for-notify
            STATUS        current
            DESCRIPTION
            "The router id of the sending router."
            ::= { zxr10LdpTrapVarsObject 4 }

zxr10LdpSesDownReason OBJECT-TYPE
            SYNTAX        INTEGER { 
                            null (0),
                            holdExpired (1), 
                            connectionExpired (2),
                            allAdjacenciesDown (3),
                            badTLV (4),
                            badPDU (5),
                            connectionError (6),
                            connectionReset (7),
                            peerSentNotification (8),
                            unexpectedEOF (9),
                            clearneighbor (10),
                            initError (11),
                            gracefulRestartAbort (12),
                            cliCommand (13),
                            unknown (14) }
            MAX-ACCESS    accessible-for-notify
            STATUS        current
            DESCRIPTION
            "The reason why the session transitioned to nonexistent state. 
            Can be one of the following:
            unknown reason, hold time expired, connection time expired, 
            all adjacencies down,  received bad tlv, received bad pdu, 
            connection error, connection reset, received notification from peer, 
            received unexpected end-of-file, clear mpls ldp, 
            error during initialization, graceful restart was aborted or client
            command."
            ::= { zxr10LdpTrapVarsObject 5 }

zxr10LdpOutIfName  OBJECT-TYPE
            SYNTAX     DisplayString
            MAX-ACCESS accessible-for-notify
            STATUS     current
            DESCRIPTION
                    "The name of the interface."
            ::= { zxr10LdpTrapVarsObject 6}

zxr10LdpLspFecAddrType OBJECT-TYPE
            SYNTAX        InetAddressType
            MAX-ACCESS    accessible-for-notify
            STATUS        current
            DESCRIPTION
            "The Internet address type of the LSP FEC ."
            ::= { zxr10LdpTrapVarsObject 7 }

zxr10LdpRtridAddrType OBJECT-TYPE
            SYNTAX        InetAddressType
            MAX-ACCESS    accessible-for-notify
            STATUS        current
            DESCRIPTION
            "The Internet address type of the router id ."
            ::= { zxr10LdpTrapVarsObject 8 }
            
--********************************************         
--definition of tables::  
--********************************************    

--*************zxr10LdpFecTable*************** 
zxr10LdpFecTable OBJECT-TYPE
            SYNTAX     SEQUENCE OF Zxr10LdpFecEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION "Fec Table. This table contains
                    one entry per fec table."
        
            ::= { zxr10mplsLdpMIB 6 }

zxr10LdpFecEntry OBJECT-TYPE
            SYNTAX     Zxr10LdpFecEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                "The parameter of a fec entity containing statistics 
                information about a particular LDP FEC."
            INDEX { zxr10LdpFecIndex }
            ::= { zxr10LdpFecTable 1 }

Zxr10LdpFecEntry ::= SEQUENCE {
            zxr10LdpFecIndex          Unsigned32,
            zxr10LdpFecType           INTEGER,
            zxr10LdpFecAddrType       InetAddressType,
            zxr10LdpFecAddr           InetAddress,
            zxr10LdpFecAddrLength     InetAddressPrefixLength,
            zxr10LdpFecState          INTEGER,
            zxr10LdpIngressOctets     Counter64,
            zxr10LdpIngressPackets    Counter64,
            zxr10LdpTransitOctets     Counter64,
            zxr10LdpTransitPackets    Counter64,
            zxr10LdpFecLspUp          TimeStamp,
            zxr10LdpFecLspDown        TimeStamp,
            zxr10LdpFecLspDownReason  INTEGER,
            zxr10LdpLostIngressOctets   Counter64,
            zxr10LdpLostIngressPackets  Counter64,
            zxr10LdpLostTransitOctets   Counter64,
            zxr10LdpLostTransitPackets  Counter64   }

zxr10LdpFecIndex OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION "The index which uniquely identifies this entry."
            ::= { zxr10LdpFecEntry 1 }

zxr10LdpFecType OBJECT-TYPE
            SYNTAX      INTEGER { 
                            indirect (0),
                            direct (1),
                            me (2),
                            invalid (3) }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
            "The type of this LDP FEC. If the value of this object
                is 'indirect (0)' then the FEC type described by this
                row is for address prefixes and not included in local 
                address list. If the value of this object is 'direct 
                (1)' then the FEC type described by this row is for 
                address prefixes and included in local address list. 
                If the value of this object is 'me(2)' then the FEC 
                type described by this row is for a host address. If 
                the value of this object is 'invalid(3)' then the FEC 
                value is 'FFFF'."
            ::= { zxr10LdpFecEntry 2 }
            
zxr10LdpFecAddrType  OBJECT-TYPE
         SYNTAX      InetAddressType
         MAX-ACCESS  read-only
         STATUS      current
         DESCRIPTION
             "The value of this object is the type of the
             Internet address.  The value of this object,
             decides how the value of the zxr10LdpFecAddr object
             is interpreted."
         REFERENCE
             "RFC3036, Section 3.4.1. FEC TLV."
         ::= { zxr10LdpFecEntry 3 }
            
zxr10LdpFecAddr OBJECT-TYPE
            SYNTAX      InetAddress
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION "LDP FEC address."
            ::= { zxr10LdpFecEntry 4 }

zxr10LdpFecAddrLength OBJECT-TYPE
            SYNTAX      InetAddressPrefixLength
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION "The LDP FEC length (in bits)."
            ::= { zxr10LdpFecEntry 5 }
            
zxr10LdpFecState OBJECT-TYPE
            SYNTAX      INTEGER {
                    ok(1),
                    egress(2),
                    noOutLabel(3),
                    noInLabel(4)
                }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The state of this LDP FEC. If the FEC exists with the 
                 local popular tag and the outgoing label then the value 
                 of this object is 'ok(1)'. If the FEC exists with the 
                 local explicit or implicit null tag then the value of 
                 this object is 'egress(2)'. If the FEC exists with the 
                 local popluar tag only then the value of this object is 
                 'noOutLabel(3)'. If the FEC exists without local tag then 
                 the value of this object is 'noInLabel(4)'."
            ::= { zxr10LdpFecEntry 6 } 

zxr10LdpIngressOctets OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The number of octets of traffic originated on 
                 this box, forwarded over the current LDP FEC. The 
                 number reported is not realtime and may be subject 
                 to several minutes delay. The delay is controllable 
                 by ldp statistics gathering interval, which by 
                 default is 5 minutes."
            ::= { zxr10LdpFecEntry 7 }
            
zxr10LdpIngressPackets OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The number of packets of traffic originated on 
                 this box, forwarded over the current LDP FEC. The 
                 number reported is not realtime and may be subject 
                 to several minutes delay. The delay is controllable 
                 by ldp statistics gathering interval, which by 
                 default is 5 minutes."
            ::= { zxr10LdpFecEntry 8 }  
            
zxr10LdpTransitOctets OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The number of octets of traffic destined to this FEC, 
                 originated on a different box, forwarded over the 
                 current LDP FEC. The number reported is not realtime 
                 and may be subject to several minutes delay. The delay 
                 is controllable by ldp statistics gathering interval, 
                 which by default is 5 minutes."
            ::= { zxr10LdpFecEntry 9 }  
            
zxr10LdpTransitPackets OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The number of packets of traffic destined to this FEC, 
                 originated on a different box, forwarded over the 
                 current LDP FEC. The number reported is not realtime 
                 and may be subject to several minutes delay. The delay 
                 is controllable by ldp statistics gathering interval, 
                 which by default is 5 minutes."
            ::= { zxr10LdpFecEntry 10 }

zxr10LdpFecLspUp OBJECT-TYPE
            SYNTAX      TimeStamp
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The value of sysUpTime at the time of the most recent 
                 change in state of LSP about FEC from down to up.
                 If no such changes have occurred since the last
                 re-initialization of the local management subsystem,
                 then this object contains a zero value."
            ::= { zxr10LdpFecEntry 11 }

zxr10LdpFecLspDown OBJECT-TYPE
            SYNTAX      TimeStamp
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The value of sysUpTime at the time of the most recent 
                 change in state of LSP about FEC from up to down.
                 If no such changes have occurred since the last
                 re-initialization of the local management subsystem,
                 then this object contains a zero value."
            ::= { zxr10LdpFecEntry 12 }

zxr10LdpFecLspDownReason OBJECT-TYPE
            SYNTAX        INTEGER { 
                            null (0),
                            topologyChanged (1), 
                            receivedWithdrawl (2),
                            neighborDown (3),
                            filterChanged (4),
                            bfdSessionDown (5),
                            unknown (6) }
            MAX-ACCESS    read-only
            STATUS        current
            DESCRIPTION
            "The reason why the LSP went down about the most recent 
                change in state of LSP from up to down. 
                Can be one of the following:
            the topology changed, the neighbor withdrew the label, 
            the neighbor went down, the filter changed, the BFD session
            went down or the reason is unknown. 
            If no such changes have occurred since the last
                re-initialization of the local management subsystem,
                then this object contains a zero value."
            ::= { zxr10LdpFecEntry 13 }
            
zxr10LdpLostIngressOctets OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The number of octets of traffic lost on 
                 this box, forwarded over the current LDP FEC. The 
                 number reported is not realtime and may be subject 
                 to several minutes delay. The delay is controllable 
                 by ldp statistics gathering interval, which by 
                 default is 5 minutes."
            ::= { zxr10LdpFecEntry 14 }
            
zxr10LdpLostIngressPackets OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The number of packets of traffic lost on 
                 this box, forwarded over the current LDP FEC. The 
                 number reported is not realtime and may be subject 
                 to several minutes delay. The delay is controllable 
                 by ldp statistics gathering interval, which by 
                 default is 5 minutes."
            ::= { zxr10LdpFecEntry 15 }  
            
zxr10LdpLostTransitOctets OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The number of lost octets of traffic destined to this FEC, 
                 originated on a different box, forwarded over the 
                 current LDP FEC. The number reported is not realtime 
                 and may be subject to several minutes delay. The delay 
                 is controllable by ldp statistics gathering interval, 
                 which by default is 5 minutes."
            ::= { zxr10LdpFecEntry 16 }  
            
zxr10LdpLostTransitPackets OBJECT-TYPE
            SYNTAX      Counter64
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The number of lost packets of traffic destined to this FEC, 
                 originated on a different box, forwarded over the 
                 current LDP FEC. The number reported is not realtime 
                 and may be subject to several minutes delay. The delay 
                 is controllable by ldp statistics gathering interval, 
                 which by default is 5 minutes."
            ::= { zxr10LdpFecEntry 17 }

--*************zxr10LdpBfdTable*************** 
zxr10LdpBfdTable OBJECT-TYPE
            SYNTAX     SEQUENCE OF Zxr10LdpBfdEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                    "Bfd Table. This table contains
                    one entry per bfd table."
            ::= { zxr10mplsLdpMIB 7 } 
            
zxr10LdpBfdEntry OBJECT-TYPE
            SYNTAX     Zxr10LdpBfdEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                    "The parameter of a bfd entity."
            INDEX { zxr10LdpBfdAddrType, zxr10LdpBfdDstAddr, zxr10LdpBfdPrefixLength }
            ::= { zxr10LdpBfdTable 1 }
            
Zxr10LdpBfdEntry ::= SEQUENCE {
                zxr10LdpBfdAddrType    
                    InetAddressType,
                zxr10LdpBfdDstAddr    
                    InetAddress,
                zxr10LdpBfdPrefixLength
                    InetAddressPrefixLength,  
                zxr10LdpBfdMinTxInterval
                    Unsigned32, 
                zxr10LdpBfdMinRxInterval
                    Unsigned32,   
                zxr10LdpBfdMult
                    Unsigned32,   
                zxr10LdpBfdLspEffect
                    INTEGER, 
                zxr10LdpBfdIfUp 
                    INTEGER,
                zxr10LdpBfdSrcAddr    
                    InetAddress,
                zxr10LdpBfdProtectBind
                    INTEGER             
                }   
zxr10LdpBfdAddrType  OBJECT-TYPE
            SYNTAX     InetAddressType
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                    " The value of this object is the type of the
                      Internet address.  The value of this object,
                      decides how the value of the zxr10LdpBfdDstAddr
                      object is interpreted."
            ::= { zxr10LdpBfdEntry 1 } 
                
zxr10LdpBfdDstAddr  OBJECT-TYPE
            SYNTAX     InetAddress
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                    " The address about FEC related to BFD."
            ::= { zxr10LdpBfdEntry 2 } 
            
zxr10LdpBfdPrefixLength  OBJECT-TYPE
            SYNTAX     InetAddressPrefixLength
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                    "The mask length of the FEC related to BFD."
            ::= { zxr10LdpBfdEntry 3 }
            
zxr10LdpBfdMinTxInterval  OBJECT-TYPE
            SYNTAX     Unsigned32
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "The time span that two control package is send."
            ::= { zxr10LdpBfdEntry 4 }
            
zxr10LdpBfdMinRxInterval  OBJECT-TYPE
            SYNTAX     Unsigned32
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "The time span that two control package is received."
            ::= { zxr10LdpBfdEntry 5 }
            
zxr10LdpBfdMult  OBJECT-TYPE
            SYNTAX     Unsigned32
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "If this parameter multiply zxr10LdpBfdMinRxInterval 
                    a control message is no received it means the lsp has 
                    some problem."
            ::= { zxr10LdpBfdEntry 6 }
                                  
zxr10LdpBfdLspEffect  OBJECT-TYPE
            SYNTAX     INTEGER{true(1),false(0)}
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "A fec needs bfd protection."
            ::= { zxr10LdpBfdEntry 7 }   
            
zxr10LdpBfdIfUp  OBJECT-TYPE
            SYNTAX     INTEGER{false(0),true(1)}
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "If the bfd is up."
            ::= { zxr10LdpBfdEntry 8 }  

zxr10LdpBfdSrcAddr  OBJECT-TYPE
            SYNTAX     InetAddress
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    " The source address specified with package of LSP PING."
            ::= { zxr10LdpBfdEntry 9 }

zxr10LdpBfdProtectBind  OBJECT-TYPE
            SYNTAX     INTEGER{false(1),true(2)}
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "FRR or ECMP protect bind flag.
                     If the value is 1,then not support protect bind.
                     If the value is 2,then support protect bind."
            ::= { zxr10LdpBfdEntry 10 }
--*************zxr10LdpGrTable***************   
zxr10LdpGrTable OBJECT-TYPE
            SYNTAX     SEQUENCE OF Zxr10LdpGrEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                    "GR Table. This table contains
                    one entry per GR table."
            ::= { zxr10mplsLdpMIB 8 }  
            
zxr10LdpGrEntry OBJECT-TYPE
            SYNTAX     Zxr10LdpGrEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                    "The parameter of a GR entity about session."
            INDEX { zxr10LdpGrSessLocalId, zxr10LdpGrSessPeerId }
            ::= { zxr10LdpGrTable 1 }  
            
Zxr10LdpGrEntry ::= SEQUENCE {
                zxr10LdpGrSessLocalId  
                    MplsLdpIdentifier,
                zxr10LdpGrSessPeerId  
                    MplsLdpIdentifier,
                zxr10LdpGrSessRestart    
                    INTEGER,
                zxr10LdpGrSessNeighborLiveTime
                    Unsigned32,  
                zxr10LdpGrSessMaxRecoveryTime
                    Unsigned32, 
                zxr10LdpSessIsInGr
                    INTEGER,   
                zxr10LdpGrSessState
                    INTEGER,   
                zxr10LdpGrSessWaitTime
                    Unsigned32                                   
                }   

zxr10LdpGrSessLocalId  OBJECT-TYPE
            SYNTAX     MplsLdpIdentifier
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "The identifier of local lsr used to identify a session relating GR."
            ::= { zxr10LdpGrEntry 1 } 

zxr10LdpGrSessPeerId  OBJECT-TYPE
            SYNTAX     MplsLdpIdentifier
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
