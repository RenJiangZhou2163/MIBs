        "The fifth outgoing label of this LSP."
    ::= { zxr10LspmIPv4LabelFwdEntry 13 }

    -- 1.3.6.1.4.1.3902.3.6000.401.2.4.1.14
    zxr10LspmIPv4LabelFwdSpecialFlag OBJECT-TYPE
        SYNTAX INTEGER
        {
        master(1),
        slave(2),
        none(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
