        ::= { zxr10LspOutsegTable 1 }

    Zxr10LspOutsegEntry ::=
        SEQUENCE { 
            zxr10LspOutsegInstName
                DisplayString,
            zxr10LspOutsegIfName
                DisplayString,
            zxr10LspOutsegNexthop
                IpAddress,
            zxr10LspOutsegLabel
                Unsigned32,
            zxr10LspOutsegForceFlag
                INTEGER
        }

    -- 1.3.6.1.4.1.3902.3.6000.401.2.3.1.1
