            zxr10LspmIPv4LabelFwdOutLabel2
                Unsigned32,
            zxr10LspmIPv4LabelFwdOutLabel3
                Unsigned32,
            zxr10LspmIPv4LabelFwdOutLabel4
                Unsigned32,
            zxr10LspmIPv4LabelFwdOutLabel5
                Unsigned32,
            zxr10LspmIPv4LabelFwdSpecialFlag
                INTEGER,
            zxr10LspmIPv4LabelFwdOriNextHop
                IpAddress
        }

