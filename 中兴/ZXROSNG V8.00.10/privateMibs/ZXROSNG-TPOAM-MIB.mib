--- TP OAM global object  
--- ***************************************************************************
       
    zxTPOAMChannelType OBJECT-TYPE
            SYNTAX     Unsigned32
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "The identity of Y.1731 packet in G-ACH Label
                    (default:32762)."
            ::= { zxTPOAMModules 1 }
            
    zxTPOAMMel OBJECT-TYPE
            SYNTAX     Unsigned32(0..7)
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "The value of MEL field in OAM packets(default: 7)."
            ::= { zxTPOAMModules 2 }
            
    zxTPOAMGlobalEnable OBJECT-TYPE
            SYNTAX     INTEGER {disable(0),enable(1)}
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "The global enable switch(default: enable)."
            ::= { zxTPOAMModules 3 }
            
    zxTPOAMTc OBJECT-TYPE
            SYNTAX     Unsigned32(0..7)
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "The value of TC field in the label of GAL(default:7)."
            ::= { zxTPOAMModules 4 }
            
    zxTPOAMRdiToSF OBJECT-TYPE
            SYNTAX     INTEGER {disable(0),enable(1)}
            MAX-ACCESS read-only
            STATUS     current
            DESCRIPTION
                    "This switch is used to transform the RDI alarm to SF alarm
                    (default:disable)."
            ::= { zxTPOAMModules 5 }
                            
