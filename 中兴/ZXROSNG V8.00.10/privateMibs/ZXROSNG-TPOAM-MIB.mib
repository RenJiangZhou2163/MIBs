--- ****************************************************************************

    zxTPOAMTunnelEntityStaConfigTable  OBJECT-TYPE
        SYNTAX  SEQUENCE OF ZxTPOAMTunnelEntityStaConfigEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "The configuration table in the instance of TUNNEL."
        ::= { zxTPOAMModules 7 }

    zxTPOAMTunnelEntityStaConfigEntry  OBJECT-TYPE
        SYNTAX  ZxTPOAMTunnelEntityStaConfigEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "An entry containing management information applicable to a 
             particular instance of TUNNEL."
        INDEX  { zxTPOAMTunnelEntityStaCTunnelID,
                 zxTPOAMTunnelEntityStaCLspid }
        ::= { zxTPOAMTunnelEntityStaConfigTable 1 }

    ZxTPOAMTunnelEntityStaConfigEntry ::=
        SEQUENCE {
            zxTPOAMTunnelEntityStaCTunnelID       Unsigned32,
            zxTPOAMTunnelEntityStaCLspid          Unsigned32,
            zxTPOAMTunnelEntityStaCMegIndex       Unsigned32,
            zxTPOAMTunnelEntityStaCPropagate      INTEGER
     }

    zxTPOAMTunnelEntityStaCTunnelID  OBJECT-TYPE
        SYNTAX  Unsigned32(1..131072)
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "The TUNNEL ID only applies to the bidirectional static TUNNEL.
             The range of this parameter depend on the device."
        ::= { zxTPOAMTunnelEntityStaConfigEntry 1 }

    zxTPOAMTunnelEntityStaCLspid  OBJECT-TYPE
        SYNTAX  Unsigned32(1..65535)
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "The identity of the static LSP"
        ::= { zxTPOAMTunnelEntityStaConfigEntry 2 }

    zxTPOAMTunnelEntityStaCMegIndex  OBJECT-TYPE
        SYNTAX  Unsigned32(0..65535)
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "MEG index corresponding to this entity.0 is invalid and it means 
            that have not created MEG."
        ::= { zxTPOAMTunnelEntityStaConfigEntry 3 }
        
    zxTPOAMTunnelEntityStaCPropagate  OBJECT-TYPE
        SYNTAX  INTEGER {disable(0),enable(1)}
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The propagating switch of the entity"
        ::= { zxTPOAMTunnelEntityStaConfigEntry 4 }
