        STATUS     current
        DESCRIPTION "The end IPv6 address of current range." 
        ::= { zxr10IPv6AddrPoolRangeEntry 3 }
        
    zxr10IPv6AddrPoolRangeVirtualRouteField   OBJECT-TYPE
        SYNTAX     DisplayString(SIZE(1..32))
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION "VRF name which binded with current range." 
        ::= { zxr10IPv6AddrPoolRangeEntry 4 }        
        
    zxr10IPv6AddrPoolRangeFreeIPNumH   OBJECT-TYPE
        SYNTAX     Counter64
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION "The high 8 octets of IPv6 address number which is not 
                     used." 
        ::= { zxr10IPv6AddrPoolRangeEntry 5 }
        
    zxr10IPv6AddrPoolRangeFreeIPNumL   OBJECT-TYPE
        SYNTAX     Counter64
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION "The lower 8 octets of IPv6 address number which is not 
                    used." 
        ::= { zxr10IPv6AddrPoolRangeEntry 6 }
        
    zxr10IPv6AddrPoolRangeUsedIPNumH   OBJECT-TYPE
        SYNTAX     Counter64
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION "The high 8 octets of IPv6 address number which is 
                    used." 
        ::= { zxr10IPv6AddrPoolRangeEntry 7 }  
        
    zxr10IPv6AddrPoolRangeUsedIPNumL   OBJECT-TYPE
        SYNTAX     Counter64
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION "The lower 8 octets of IPv6 address number which is 
                     used." 
        ::= { zxr10IPv6AddrPoolRangeEntry 8 }  

