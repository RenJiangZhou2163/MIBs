        STATUS     current
        DESCRIPTION "IPv6 address pool name." 
        ::= { zxr10IPv6AddrPoolRangeEntry 1 }
        
    zxr10IPv6AddrPoolRangeStartIP   OBJECT-TYPE
        SYNTAX     Ipv6Address
