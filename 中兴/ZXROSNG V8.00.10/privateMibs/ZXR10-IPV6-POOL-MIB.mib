        zxr10IPv6AddrPoolRangeVirtualRouteField  DisplayString, 
        zxr10IPv6AddrPoolRangeFreeIPNumH         Counter64,
        zxr10IPv6AddrPoolRangeFreeIPNumL         Counter64,
        zxr10IPv6AddrPoolRangeUsedIPNumH         Counter64,
        zxr10IPv6AddrPoolRangeUsedIPNumL         Counter64
        }

    zxr10IPv6AddrPoolRangePoolName   OBJECT-TYPE
        SYNTAX     DisplayString(SIZE(1..31))
