		promptgroup(GUI_DISPLAY)
		interest(1)
		size(16)
	}
	field(TABL,DBF_STRING) {
		prompt("Table")
		promptgroup(GUI_DISPLAY)
		interest(1)
		size(16)
	}
	field(FILD,DBF_STRING) {
		prompt("Field")
		promptgroup(GUI_DISPLAY)
		interest(1)
		size(26)
	}
	field(QRY,DBF_STRING) {
		prompt("Query String")
		promptgroup(GUI_DISPLAY)
		interest(1)
		size(200)
	}
	field(OVAL,DBF_DOUBLE) {
		prompt("Old Value")
		asl(ASL0)
	}
