record(dfanout, "$(SYS):$(DEV):PID_CTRL")
{
  field(DESC, "$(DEV) PID Control fanout")
  field(OMSL, "supervisory")
  field(OUTA, "$(SYS):$(DEV):1+PID.FBON  PP NMS")
  field(OUTB, "$(SYS):$(DEV):1-PID.FBON  PP NMS")
  field(OUTC, "$(SYS):$(DEV):2+PID.FBON  PP NMS")
  field(OUTD, "$(SYS):$(DEV):2-PID.FBON  PP NMS")
  field(OUTE, "$(SYS):$(DEV):3+PID.FBON  PP NMS")
  field(OUTF, "$(SYS):$(DEV):3-PID.FBON  PP NMS")
}
