record(dfanout, "$(SYS):$(DEV):CH1_VolSet")
{
  field(DESC, "CH1 VolSet fanout")
  field(OMSL, "supervisory")
  field(OUTA, "$(SYS):$(DEV):1+XR_VolSet  PP NMS")
  field(OUTB, "$(SYS):$(DEV):1-XL_VolSet  PP NMS")
  field(EGU, "V")
  field(HOPR, "4000")
  field(LOPR, "0")
}
