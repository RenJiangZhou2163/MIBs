record(ao, "$(SYS):$(DEV):CH_A_OutputCTRL")
{
    field(DESC, "Remote Enable")
    field(DTYP, "niCtrl")
    field(SCAN, "Passive")
    field(PREC, "3")
    field(OUT,  "@Dev2/ao0")
}
