record(ai, "$(SYS):$(DEV):VolRB")
{
    field(DESC, "Get Voltage Value")
    field(DTYP, "Fug")
    field(EGU, "V")
    field(SCAN, ".2 second")
    field(PREC, "0")
    field(INP,  "#L$(L) A0 @0")
    field(EGUF, "65000")
    field(EGUL, "0")
    field(HOPR, "65000")
    field(LOPR, "0")
    field(LINR, "LINEAR")
}
record(ai, "$(SYS):$(DEV):CurRB")
{
    field(DESC, "Get Current Value")
    field(DTYP, "Fug")
    field(EGU, "mA")
    field(SCAN, ".2 second")
    field(PREC, "5")
    field(INP,  "#L$(L) A0 @1")
    field(EGUF, "2")
    field(EGUL, "0")
    field(HOPR, "2")
    field(LOPR, "0")
    field(LINR, "LINEAR")
}
