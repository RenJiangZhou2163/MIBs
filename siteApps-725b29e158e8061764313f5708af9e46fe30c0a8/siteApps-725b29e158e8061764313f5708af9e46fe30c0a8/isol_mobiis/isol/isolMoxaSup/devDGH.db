record(ai, "$(SYS):$(DEV):VolRB")
{
    field(DESC, "Get Voltage Value")
    field(DTYP, "DGH")
    field(EGU, "mV")
    field(SCAN, ".2 second")
    field(PREC, "3")
    field(INP,  "#L$(L) A0 @0")
}
record(ao, "$(SYS):$(DEV):VolSet")
{
    field(DESC, "Set Voltage Value")
    field(DTYP, "DGH")
    field(EGU, "mV")
    field(SCAN, "Passive")
    field(PREC, "3")
    field(OUT,  "#L$(L) A0 @1")
}
