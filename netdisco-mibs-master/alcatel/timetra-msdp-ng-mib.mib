       tmnxMsdpNgPeerGrpPeerMaxActSrcs          Integer32, 
       tmnxMsdpNgPeerGrpPeerMsgRcvRt            Unsigned32,
       tmnxMsdpNgPeerGrpPeerMsgRxRtTime         Unsigned32,
       tmnxMsdpNgPeerGrpPeerMsgRcvRtThd         Gauge32,
