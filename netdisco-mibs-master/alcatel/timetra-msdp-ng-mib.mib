                tmnxMsdpNgPeerGroupName, 
                tmnxMsdpNgPeerGrpPeerAddressType, 
                tmnxMsdpNgPeerGrpPeerAddress 
            }
    ::= { tmnxMsdpNgPeerGrpPeerTable 1 }

TmnxMsdpNgPeerGrpPeerEntry ::= SEQUENCE {
