    vRtrMldHostStatsMcacPolicyDrops Counter32,
    vRtrMldHostRxLocalScopePkts     Counter32,
    vRtrMldHostRxRsvdScopePkts      Counter32,
    vRtrMldHostRedirectionDrops     Counter32
}

vRtrMldHostTxGenQueries OBJECT-TYPE
