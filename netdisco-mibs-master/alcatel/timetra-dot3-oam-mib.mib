TIMETRA-DOT3-OAM-MIB DEFINITIONS ::= BEGIN

IMPORTS
        Integer32, MODULE-IDENTITY,
        OBJECT-TYPE, Counter32, Counter64,
        Unsigned32, NOTIFICATION-TYPE                 FROM SNMPv2-SMI

        RowStatus, TimeStamp,
        TruthValue, TEXTUAL-CONVENTION                FROM SNMPv2-TC

        MODULE-COMPLIANCE, OBJECT-GROUP,
        NOTIFICATION-GROUP                            FROM SNMPv2-CONF

        dot3OamEntry, dot3OamEventConfigEntry,
        dot3OamEventLogEntry, dot3OamEventLogTimestamp,
        dot3OamEventLogOui, dot3OamEventLogType,
        dot3OamEventLogLocation, dot3OamEventLogWindowHi,
        dot3OamEventLogWindowLo, dot3OamEventLogThresholdHi,
        dot3OamEventLogThresholdLo, dot3OamEventLogValue,
        dot3OamEventLogRunningTotal, dot3OamEventLogEventTotal,
        dot3OamLoopbackEntry, dot3OamPeerEntry,
        dot3OamPeerMacAddress, EightOTwoOui           FROM DOT3-OAM-MIB

        ifIndex                                       FROM IF-MIB

        CounterBasedGauge64                           FROM HCNUM-TC

        timetraSRMIBModules, tmnxSRObjs,
        tmnxSRNotifyPrefix, tmnxSRConfs               FROM TIMETRA-GLOBAL-MIB

        TmnxEnabledDisabled                           FROM TIMETRA-TC-MIB;


timetraDOT3OAMMIBModule MODULE-IDENTITY
        LAST-UPDATED    "201601010000Z"
        ORGANIZATION    "Nokia"
        CONTACT-INFO
            "Nokia SROS Support
             Web: http://www.nokia.com"
        DESCRIPTION
