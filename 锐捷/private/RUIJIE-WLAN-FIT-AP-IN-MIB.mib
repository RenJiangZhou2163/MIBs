    OBJECTS {
        ruijieAPCurrentPMMode,
        ruijieChgWorkModeAPMac
    }
    STATUS  current
    DESCRIPTION
            " This trap will be sent when the ap monitor workMode Change."
    ::= { ruijieAlarmTraps 2 }    

    
