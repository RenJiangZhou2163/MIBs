        ruijieOspfNbrOptions
            Unsigned32,
        ruijieOspfNbrPriority
            DesignatedRouterPriority,
        ruijieOspfNbrState
            INTEGER,
        ruijieOspfNbrEvents
            Counter32,
        ruijieOspfNbrLsRetransQLen
            Gauge32,
        ruijieOspfNbmaNbrStatus
            RowStatus,
        ruijieOspfNbmaNbrPermanence
            INTEGER,
        ruijieOspfNbrHelloSuppressed
