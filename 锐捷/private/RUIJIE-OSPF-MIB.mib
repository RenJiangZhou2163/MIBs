--      The OSPF Neighbor Table describes all neighbors in
--      the locality of the subject router.

ruijieOspfNeighborTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF RuijieOspfNeighborEntry
        MAX-ACCESS   not-accessible
        STATUS   current
        DESCRIPTION
           "the neighbor detail infomation ."
       REFERENCE
          "ospfNbrTable in rfc1850"           
       ::= { ruijieOspfMIBObjects  9 }

ruijieOspfNeighborEntry OBJECT-TYPE
       SYNTAX   RuijieOspfNeighborEntry
       MAX-ACCESS   not-accessible
       STATUS   current
       DESCRIPTION
          "the entry of this table."
      INDEX {   ruijieOspfNbrIpAddr, 
                ruijieOspfNbrAddressLessIndex
             }
      ::= { ruijieOspfNeighborTable 1 }
      
RuijieOspfNeighborEntry ::=
