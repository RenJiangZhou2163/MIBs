RUIJIE-VXLAN-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY,
        OBJECT-TYPE,
        NOTIFICATION-TYPE,
        OBJECT-IDENTITY,
        Integer32,
        Counter32,
        Counter64,
        Unsigned32,
        IpAddress
                FROM SNMPv2-SMI
        InetAddressIPv6,
        InetAddress,
        InetAddressType
                FROM INET-ADDRESS-MIB                   -- [RFC4001]
        RowStatus
                FROM SNMPv2-TC               
        MODULE-COMPLIANCE,
        OBJECT-GROUP,
        NOTIFICATION-GROUP
                FROM SNMPv2-CONF
        ConfigStatus,
        IfIndex
                FROM RUIJIE-TC
        ifIndex
                FROM RFC1213-MIB
        ruijieMgmt
                FROM RUIJIE-SMI
        InterfaceIndex
                FROM IF-MIB
        VlanId
                FROM Q-BRIDGE-MIB;
                
ruijieVxlanMIB    MODULE-IDENTITY
       LAST-UPDATED    "201904100000Z"
       ORGANIZATION    "Ruijie Networks Co.,Ltd."
       CONTACT-INFO
                " 
                Tel: 4008-111-000 

                E-mail: service@ruijie.com.cn"
        DESCRIPTION
                "This module defines ruijie vxlan mibs."
        REVISION      "201904100000Z"
        DESCRIPTION
                "Initial version of this MIB module."
        ::= { ruijieMgmt 161}

ruijieVxlanMIBObjects  OBJECT IDENTIFIER ::= { ruijieVxlanMIB 1 }
ruijieVxlanMIBTraps  OBJECT IDENTIFIER ::= { ruijieVxlanMIB 2 }
ruijieVxlanMIBConformance  OBJECT IDENTIFIER ::= { ruijieVxlanMIB 3 }

--****************************************************************
-- ruijieVxlanMIBObjects
--****************************************************************

--
-- ruijieVxlanVniStatisticTable
--
ruijieVxlanVniStatisticTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RuijieVxlanVniStatisticEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "List of VXLAN vni statistic objects."
        ::= { ruijieVxlanMIBObjects 1 }
    
ruijieVxlanVniStatisticEntry OBJECT-TYPE
        SYNTAX RuijieVxlanVniStatisticEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Entry contains packets statistics of a vni."
        INDEX   { 
                    ruijieVxlanVniStatisticVni
                }
        ::= { ruijieVxlanVniStatisticTable 1 }

RuijieVxlanVniStatisticEntry ::=
        SEQUENCE {
            ruijieVxlanVniStatisticVni              Unsigned32,
            ruijieVxlanVniStatisticInPackets        Counter64,
            ruijieVxlanVniStatisticOutPackets       Counter64,
            ruijieVxlanVniStatisticInBytes          Counter64,
            ruijieVxlanVniStatisticOutBytes         Counter64,
            ruijieVxlanVniStatisticInPps            Counter64,
            ruijieVxlanVniStatisticOutPps           Counter64,
            ruijieVxlanVniStatisticInBps            Counter64,
            ruijieVxlanVniStatisticOutBps           Counter64
        }

ruijieVxlanVniStatisticVni OBJECT-TYPE
        SYNTAX Unsigned32 (1..16777215)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "vni : VXLAN Network Identifier."
        ::= { ruijieVxlanVniStatisticEntry 1 }

ruijieVxlanVniStatisticInPackets OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Inbound packets in this vni."
        ::= { ruijieVxlanVniStatisticEntry 2 }
           
ruijieVxlanVniStatisticOutPackets OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Outbound packets in this vni."
        ::= { ruijieVxlanVniStatisticEntry 3 }
    
ruijieVxlanVniStatisticInBytes OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Inbound bytes in this vni."
        ::= { ruijieVxlanVniStatisticEntry 4 }

ruijieVxlanVniStatisticOutBytes OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Outbound bytes in this vni."
        ::= { ruijieVxlanVniStatisticEntry 5 }

ruijieVxlanVniStatisticInPps OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Inbound packets per second in this vni."
        ::= { ruijieVxlanVniStatisticEntry 6 }
        
ruijieVxlanVniStatisticOutPps OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Outbound packets per second in this vni."
        ::= { ruijieVxlanVniStatisticEntry 7 }
        
ruijieVxlanVniStatisticInBps OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Inbound bits per second in this vni."
        ::= { ruijieVxlanVniStatisticEntry 8 }
        
ruijieVxlanVniStatisticOutBps OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Outbound bits per second in this vni."
        ::= { ruijieVxlanVniStatisticEntry 9 }
        
--
-- ruijieVxlanStaticTnlTable
--
ruijieVxlanStaticTnlTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RuijieVxlanStaticTnlEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "List of VXLAN static tunnel objects."
        ::= { ruijieVxlanMIBObjects 2 }
        
ruijieVxlanStaticTnlEntry OBJECT-TYPE
        SYNTAX RuijieVxlanStaticTnlEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Entry contains a VXLAN static tunnel."
        INDEX   {
                    ruijieVxlanStaticTnlDestIp,
                    ruijieVxlanStaticTnlSourceIp
                }
        ::= { ruijieVxlanStaticTnlTable 1 }

RuijieVxlanStaticTnlEntry ::=
        SEQUENCE {
            ruijieVxlanStaticTnlDestIp      IpAddress,
            ruijieVxlanStaticTnlSourceIp    IpAddress,
            ruijieVxlanStaticTnlRowStatus   RowStatus
        }
        
ruijieVxlanStaticTnlDestIp OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The destination IP address of the VXLAN static tunnel."
        ::= { ruijieVxlanStaticTnlEntry 1 }
        
ruijieVxlanStaticTnlSourceIp OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The source IP address of the VXLAN static tunnel."
        ::= { ruijieVxlanStaticTnlEntry 2 }
        
ruijieVxlanStaticTnlRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "The status of this conceptual row.  The RowStatus becomes
             active on successful creation of a VXLAN static tunnel."
        DEFVAL { active }
        ::= { ruijieVxlanStaticTnlEntry 3 }
        
