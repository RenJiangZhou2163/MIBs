--
-- ruijieVxlanVtepTable
--        
ruijieVxlanVtepTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RuijieVxlanVtepEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "List of vteps' configurations."
        ::= { ruijieVxlanMIBObjects 6 }
       
ruijieVxlanVtepEntry OBJECT-TYPE
        SYNTAX RuijieVxlanVtepEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Entry contains one vtep's configurations."
        INDEX   {
                    ruijieVxlanVtepId
                }
        ::= { ruijieVxlanVtepTable 1 }
        
RuijieVxlanVtepEntry ::=
        SEQUENCE {
            ruijieVxlanVtepId                   Unsigned32,
            ruijieVxlanVtepSourceLoopbackNum    Integer32,
            ruijieVxlanVtepSourceIPv4Addr       IpAddress,
            ruijieVxlanVtepSourceIPv6Addr       InetAddressIPv6
        }

ruijieVxlanVtepId OBJECT-TYPE
        SYNTAX Unsigned32 (1..1)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Vtep's id. Now only support '1' because only one vtep is
             supported in a system."
        ::= { ruijieVxlanVtepEntry 1 }
        
ruijieVxlanVtepSourceLoopbackNum OBJECT-TYPE
        SYNTAX Integer32 (-1..2147483647)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The source loopback interface port-number of this vtep,
            the '-1' means that the source loopback interface has not
            been configured when get it, or want to delete configuration
            of the source loopback interface when set it.
            One thing to note is that the ruijieVxlanVtepSourceLoopbackNum,
            ruijieVxlanVtepSourceIPv4Addr and ruijieVxlanVtepSourceIPv6Addr
            are mutually exclusive."
        ::= { ruijieVxlanVtepEntry 2 }
        
ruijieVxlanVtepSourceIPv4Addr OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The IPv4 address of this vtep,
            the zero address means that the IPv4 address has not
            been configured when get it, or want to delete configuration
            of the IPv4 address when set it.
            One thing to note is that the ruijieVxlanVtepSourceLoopbackNum,
            ruijieVxlanVtepSourceIPv4Addr and ruijieVxlanVtepSourceIPv6Addr
            are mutually exclusive."
        ::= { ruijieVxlanVtepEntry 3 }
        
ruijieVxlanVtepSourceIPv6Addr OBJECT-TYPE
        SYNTAX InetAddressIPv6
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The IPv6 address of this vtep,
            the zero address means that the IPv6 address has not
            been configured when get it, or want to delete configuration
            of the IPv6 address when set it.
            One thing to note is that the ruijieVxlanVtepSourceLoopbackNum,
            ruijieVxlanVtepSourceIPv4Addr and ruijieVxlanVtepSourceIPv6Addr
            are mutually exclusive."
        ::= { ruijieVxlanVtepEntry 4 }

