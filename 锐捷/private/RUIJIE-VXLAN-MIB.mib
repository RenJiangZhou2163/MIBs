ruijieVxlanGroups       OBJECT IDENTIFIER ::= { ruijieVxlanMIBConformance 2 }

--
-- compliance statements
--
ruijieVxlanCompliance MODULE-COMPLIANCE
        STATUS  current
        DESCRIPTION
            ""
        MODULE -- this module
            MANDATORY-GROUPS {
                ruijieVxlanStaticTnlCfgGroup,
                ruijieVxlanTnlGroup,
                ruijieVxlanVtepCfgGroup,
                ruijieVxlanTrapTnlStatusGroup,
                ruijieVxlanVlanAssoGroup
            }
            GROUP  ruijieVxlanVniStatisticGroup
            DESCRIPTION
                "These statistics are mandatory only for those systems 
                 that count such statistics."
            GROUP  ruijieVxlanTnlVniStatisticGroup
            DESCRIPTION
                "These statistics are mandatory only for those systems 
                 that count such statistics."             
        ::= { ruijieVxlanCompliances 1 }
        
--
-- groups statements
--  
