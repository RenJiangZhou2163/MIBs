RUIJIE-AAA-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY,
        OBJECT-TYPE,
        Counter32,
        Counter64,
        Unsigned32,
        Integer32,
        IpAddress
                FROM SNMPv2-SMI
        InetAddressType, 
        InetAddress
                FROM INET-ADDRESS-MIB
        Counter
                FROM RFC1155-SMI
        TruthValue,
        DisplayString,
        RowStatus,
        MacAddress
                FROM SNMPv2-TC
        MODULE-COMPLIANCE,
        OBJECT-GROUP
                FROM SNMPv2-CONF
        ConfigStatus,
        IfIndex
                FROM RUIJIE-TC
        EnabledStatus 
                FROM P-BRIDGE-MIB       
        radiusAuthServerAddress,
        radiusAuthClientServerPortNumber
            FROM RADIUS-AUTH-CLIENT-MIB
        radiusAccServerAddress, 
        radiusAccClientServerPortNumber
            FROM RADIUS-ACC-CLIENT-MIB
        ruijieMgmt
                FROM RUIJIE-SMI;

ruijieAAAMIB MODULE-IDENTITY
        LAST-UPDATED "200203200000Z"
        ORGANIZATION "Ruijie Networks Co.,Ltd."
        CONTACT-INFO
                " 
                Tel: 4008-111-000 

                E-mail: service@ruijie.com.cn"
        DESCRIPTION
                "This module defines ruijie AAA(802.1x) mibs."
        REVISION      "200203200000Z"
        DESCRIPTION
                "Initial version of this MIB module."
        ::= { ruijieMgmt 19}

ruijieAAAMIBObjects OBJECT IDENTIFIER ::= { ruijieAAAMIB 1 }

ruijieDot1xAuthObjects OBJECT IDENTIFIER ::= { ruijieAAAMIBObjects 1 }
ruijieAAAServerObjects OBJECT IDENTIFIER ::=    { ruijieAAAMIBObjects 2 }
ruijieAuthUserObjects OBJECT IDENTIFIER ::=  { ruijieAAAMIBObjects 3 }
ruijieAuthModeObjects OBJECT IDENTIFIER ::=  { ruijieAAAMIBObjects 4 }
ruijieClientProbeObjects OBJECT IDENTIFIER ::=  { ruijieAAAMIBObjects 5 }

--     
-- ruijieDot1xAuth Groupfor 802.1x global settings.
--
ruijieDot1xAuthStatus OBJECT-TYPE
        SYNTAX EnabledStatus
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "The enable(1)/disable(2) control used by the  
            802.1x global settings."
        DEFVAL { disabled }
        ::= { ruijieDot1xAuthObjects 1 }

ruijieDot1xAuthObjectsQuietPeriod OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "The value, in seconds, of the quietPeriod constant
             currently in use by the Authenticator PAE state
             machine."
        DEFVAL { 60 }
        ::= { ruijieDot1xAuthObjects 2 } 

ruijieDot1xAuthObjectsTxPeriod OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "The value, in seconds, of the txPeriod constant
             currently in use by the Authenticator PAE state
             machine."
        DEFVAL { 30 }
        ::= { ruijieDot1xAuthObjects 3 } 

ruijieDot1xAuthObjectsSuppTimeout OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "The value, in seconds, of the suppTimeout constant
             currently in use by the Backend Authentication state
             machine."
        DEFVAL { 30 }
        ::= { ruijieDot1xAuthObjects 4 } 

ruijieDot1xAuthObjectsServerTimeout OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        "The value, in seconds, of the serverTimeout constant
         currently in use by the Backend Authentication state
         machine."
        DEFVAL { 30 }
    ::= { ruijieDot1xAuthObjects 5 }

ruijieDot1xAuthObjectsMaxReq OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "The value of the maxReq constant currently in use by
             the Backend Authentication state machine."
        DEFVAL { 2 }
        ::= { ruijieDot1xAuthObjects 6 } 

ruijieDot1xAuthObjectsReAuthPeriod OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "The value, in seconds, of the reAuthPeriod constant
             currently in use by the Reauthentication Timer state
             machine."
        DEFVAL { 3600 }
        ::= { ruijieDot1xAuthObjects 7 } 

ruijieDot1xAuthObjectsMaxReauth OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "The value of the reAuthMax constant currently in use by
             the Authenticator PAE state machine."
        DEFVAL { 2 }
        ::= { ruijieDot1xAuthObjects 8 } 
        
ruijieDot1xAuthObjectsReAuthEnable OBJECT-TYPE
        SYNTAX EnabledStatus
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "The enable(1)/disable(2) control used by the Reauthentication
             Timer state machine."
        DEFVAL { 2 }
        ::= { ruijieDot1xAuthObjects 9 } 
        
ruijieDot1xAuthObjectsConfigTable OBJECT-TYPE
         SYNTAX SEQUENCE OF RuijieDot1xAuthConfigEntry
         MAX-ACCESS not-accessible
         STATUS current
         DESCRIPTION
           "A table that contains the configuration objects for the
              Authenticator PAE associated with each MAC address.
              An entry appears in this table for each MAC address that 
              may authenticate access to itself."
          ::= { ruijieDot1xAuthObjects 10 }
        
ruijieDot1xAuthObjectsConfigEntry OBJECT-TYPE
        SYNTAX RuijieDot1xAuthConfigEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
         "The configuration information for an Authenticator
              PAE."
       INDEX { ruijieDot1xAuthObjectsConfigFdbId,ruijieDot1xAuthObjectsConfigAddr }
       ::= { ruijieDot1xAuthObjectsConfigTable 1 }

RuijieDot1xAuthConfigEntry ::=
        SEQUENCE {
        ruijieDot1xAuthObjectsConfigFdbId
                 Unsigned32,
        ruijieDot1xAuthObjectsConfigAddr
                 MacAddress,
         ruijieDot1xAuthObjectsPaeState
                 INTEGER,
         ruijieDot1xAuthObjectsBackendAuthState
                 INTEGER,
         ruijieDot1xAuthObjectsAuthControlledPortStatus
                 INTEGER,
        ruijieDot1xAuthObjectsKeyTxEnabled
                 TruthValue,        
  ruijieDot1xAuthObjectsIfIndex
     IfIndex
         }
ruijieDot1xAuthObjectsConfigFdbId OBJECT-TYPE
        SYNTAX       Unsigned32
        MAX-ACCESS   read-only
        STATUS       current
        DESCRIPTION
            "The VID of vlan which the address blongs to."
    ::= { ruijieDot1xAuthObjectsConfigEntry 1 }
    
ruijieDot1xAuthObjectsConfigAddr OBJECT-TYPE
        SYNTAX  MacAddress
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The MAC address associated with this configurations."
        ::= { ruijieDot1xAuthObjectsConfigEntry 2 }

ruijieDot1xAuthObjectsPaeState OBJECT-TYPE
        SYNTAX INTEGER {
               initialize(1),
               disconnected(2),
               connecting(3),
               authenticating(4),
               authenticated(5),
               aborting(6),
               held(7),
               forceAuth(8),
               forceUnauth(9)
               }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The current value of the Authenticator PAE state
             machine."
        ::= { ruijieDot1xAuthObjectsConfigEntry 3 }

ruijieDot1xAuthObjectsBackendAuthState OBJECT-TYPE
        SYNTAX INTEGER {
               request(1),
               response(2),
               success(3),
               fail(4),
               timeout(5),
               idle(6),
               initialize(7)
               }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The current state of the Backend Authentication
             state machine."
        ::= { ruijieDot1xAuthObjectsConfigEntry 4 }

ruijieDot1xAuthObjectsAuthControlledPortStatus OBJECT-TYPE
        SYNTAX INTEGER {
               authorized(1),
               unauthorized(2)
               }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The current value of the controlled Port
             status parameter for the Port."
        ::= { ruijieDot1xAuthObjectsConfigEntry 5 }

ruijieDot1xAuthObjectsKeyTxEnabled OBJECT-TYPE
        SYNTAX TruthValue
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The value of the keyTransmissionEnabled constant
             currently in use by the Authenticator PAE state
             machine."
        ::= { ruijieDot1xAuthObjectsConfigEntry 6 }
                
ruijieDot1xAuthObjectsIfIndex OBJECT-TYPE
        SYNTAX IfIndex
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The interface index associated with this configurations"
        ::= { ruijieDot1xAuthObjectsConfigEntry 7 }
        
                
ruijieDot1xAuthObjectsStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RuijieDot1xAuthStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table that contains the statistics objects for the
             Authenticator PAE associated with each MAC address.
             An entry appears in this table for each MAC address 
             that may authenticate access to itself."
        ::= { ruijieDot1xAuthObjects 11 }

ruijieDot1xAuthStatsEntry OBJECT-TYPE
        SYNTAX RuijieDot1xAuthStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The statistics information for an Authenticator PAE."
        INDEX { ruijieDot1xAuthObjectsStatsFdbId,ruijieDot1xAuthObjectsStatsAddr }
        ::= { ruijieDot1xAuthObjectsStatsTable 1 }

RuijieDot1xAuthStatsEntry ::=
        SEQUENCE {
        ruijieDot1xAuthObjectsStatsFdbId
                Unsigned32,
        ruijieDot1xAuthObjectsStatsAddr
                MacAddress,
        ruijieDot1xAuthObjectsEapolFramesRx
                Counter32,
        ruijieDot1xAuthObjectsEapolFramesTx
                Counter32,
        ruijieDot1xAuthObjectsEapolRuijieFramesRx
                Counter32,
        ruijieDot1xAuthObjectsEapolLogoffFramesRx
                Counter32,
        ruijieDot1xAuthObjectsEapolRespIdFramesRx
                Counter32,
        ruijieDot1xAuthObjectsEapolRespFramesRx
                Counter32,
        ruijieDot1xAuthObjectsEapolReqIdFramesTx
                Counter32,
        ruijieDot1xAuthObjectsEapolReqFramesTx
                Counter32,
        ruijieDot1xAuthObjectsInvalidEapolFramesRx
                Counter32,
        ruijieDot1xAuthObjectsEapLengthErrorFramesRx
                Counter32,
        ruijieDot1xAuthObjectsLastEapolFrameVersion
                Unsigned32,
        ruijieDot1xAuthObjectsLastEapolFrameSource
                MacAddress
            }
ruijieDot1xAuthObjectsStatsFdbId OBJECT-TYPE
        SYNTAX       Unsigned32
        MAX-ACCESS   read-only
        STATUS       current
        DESCRIPTION
            "The VID of vlan which the address blongs to."
        ::= { ruijieDot1xAuthStatsEntry 1 }

ruijieDot1xAuthObjectsStatsAddr OBJECT-TYPE
        SYNTAX  MacAddress
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The MAC address associated with this states."
        ::= { ruijieDot1xAuthStatsEntry 2 }

ruijieDot1xAuthObjectsEapolFramesRx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of valid EAPOL frames of any type
             that have been received by this Authenticator."
        ::= { ruijieDot1xAuthStatsEntry 3 }

ruijieDot1xAuthObjectsEapolFramesTx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of EAPOL frames of any type
             that have been transmitted by this Authenticator."
        ::= { ruijieDot1xAuthStatsEntry 4 }

ruijieDot1xAuthObjectsEapolRuijieFramesRx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of EAPOL Ruijie frames that have
             been received by this Authenticator."
        ::= { ruijieDot1xAuthStatsEntry 5 }

ruijieDot1xAuthObjectsEapolLogoffFramesRx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of EAPOL Logoff frames that have
             been received by this Authenticator."
        ::= { ruijieDot1xAuthStatsEntry 6 }

ruijieDot1xAuthObjectsEapolRespIdFramesRx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of EAP Resp/Id frames that have
             been received by this Authenticator."
        ::= { ruijieDot1xAuthStatsEntry 7 }

ruijieDot1xAuthObjectsEapolRespFramesRx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of valid EAP Response frames
             (other than Resp/Id frames) that have been
             received by this Authenticator."
        ::= { ruijieDot1xAuthStatsEntry 8 }

ruijieDot1xAuthObjectsEapolReqIdFramesTx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "The number of EAP Req/Id frames that have been
        transmitted by this Authenticator."
    ::= { ruijieDot1xAuthStatsEntry 9 }

ruijieDot1xAuthObjectsEapolReqFramesTx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "The number of EAP Request frames
         (other than Rq/Id frames) that have been
         transmitted by this Authenticator."
    ::= { ruijieDot1xAuthStatsEntry 10 }

ruijieDot1xAuthObjectsInvalidEapolFramesRx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "The number of EAPOL frames that have been
         received by this Authenticator in which the
         frame type is not recognized."
    ::= { ruijieDot1xAuthStatsEntry 11 }

ruijieDot1xAuthObjectsEapLengthErrorFramesRx OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "The number of EAPOL frames that have been received
         by this Authenticator in which the Packet Body
        Length field is invalid."
    ::= { ruijieDot1xAuthStatsEntry 12 }

ruijieDot1xAuthObjectsLastEapolFrameVersion OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "The protocol version number carried in the
         most recently received EAPOL frame."
    ::= { ruijieDot1xAuthStatsEntry 13 }

ruijieDot1xAuthObjectsLastEapolFrameSource OBJECT-TYPE
        SYNTAX MacAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "The source MAC address carried in the
         most recently received EAPOL frame."
    ::= { ruijieDot1xAuthStatsEntry 14 }
    
ruijieDot1xCurrentUserNumber OBJECT-TYPE
        SYNTAX Counter
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION 
             "Numbers of users who attached to the system ,regardless of their state."
        ::= { ruijieDot1xAuthObjects 12 }      

ruijieDot1xCurrentAuthenticatedUserNumber OBJECT-TYPE
        SYNTAX Counter
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION 
                "Numbers of user who attached to the system and their current state is
                 authorized"
        ::= { ruijieDot1xAuthObjects 13 }           

ruijieDot1xAccountStatus OBJECT-TYPE
        SYNTAX EnabledStatus
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "The enable(1)/disable(2) control used by the  
             accounting function."
        DEFVAL { 2 }
        ::= { ruijieDot1xAuthObjects 14 }

ruijieAuthIfTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF RuijieAuthIfEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "The table of the authenticated status of interface."
        ::= { ruijieDot1xAuthObjects 15 }

ruijieAuthIfEntry OBJECT-TYPE
        SYNTAX  RuijieAuthIfEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "The entry of MAC address which can be authenticated."
        INDEX   { ruijieAuthIf}
        ::= { ruijieAuthIfTable 1 }

RuijieAuthIfEntry ::=
        SEQUENCE {
        ruijieAuthIf
                IfIndex,           
        ruijieAuthIfStatus
                EnabledStatus
        }
ruijieAuthIf OBJECT-TYPE
        SYNTAX  IfIndex
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The port number of the port/trunk to which frames received from a
              specific port/trunk and destined for a specific MAC address."
        ::= { ruijieAuthIfEntry 1 }

ruijieAuthIfStatus OBJECT-TYPE
        SYNTAX EnabledStatus
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The authentication status of a port, can be opened by setting its
            value to Enabled(1), or closed by setting its value to Disabled(2)."
        DEFVAL { 2 }
        ::= { ruijieAuthIfEntry 2 }

ruijieAuthenticationMode OBJECT-TYPE
        SYNTAX  INTEGER {
                eap(1),       
                chap(2),
                pap(3)
                }
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "Mode used for authentication"
        ::= { ruijieDot1xAuthObjects 16 }        

ruijieDot1xAccountUpdateStatus OBJECT-TYPE
        SYNTAX  EnabledStatus
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "Account Update Status"
        ::= { ruijieDot1xAuthObjects 17 }  

ruijieDot1xAcctInterimInterval OBJECT-TYPE
        SYNTAX  Unsigned32 (60..31536000)
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "Account Interim Interval(seconds), should be multiples of 60"
        ::= { ruijieDot1xAuthObjects 18 }               
       
ruijieDot1xEapolTagEnabled OBJECT-TYPE
        SYNTAX  EnabledStatus
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "Eapol Tag Enabled"
        ::= { ruijieDot1xAuthObjects 19 }            

ruijieDot1xIfUserMaxTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF RuijieDot1xIfUserMaxEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "The table of the max dot1x user number on authenticated interface."
        ::= { ruijieDot1xAuthObjects 20 }

ruijieDot1xIfUserMaxEntry OBJECT-TYPE
        SYNTAX  RuijieDot1xIfUserMaxEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "The entry of max dot1x user number on authenticated interface."
        INDEX   { ruijieDot1xIfUserMaxIndex}
        ::= { ruijieDot1xIfUserMaxTable 1 }

RuijieDot1xIfUserMaxEntry ::=
        SEQUENCE {
        ruijieDot1xIfUserMaxIndex
                IfIndex,           
        ruijieDot1xIfUserMaxNum
                Unsigned32
        }
ruijieDot1xIfUserMaxIndex OBJECT-TYPE
        SYNTAX  IfIndex
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The interface index which dot1x authentication is enabled."
        ::= { ruijieDot1xIfUserMaxEntry 1 }

ruijieDot1xIfUserMaxNum OBJECT-TYPE
        SYNTAX Unsigned32 (1..1000000)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The max dot1x user num on the interface, this is valid only 
            when dot1x authentication is enable on that interface.
            Number 0 indicates no user number limits enabled on the interface."
        ::= { ruijieDot1xIfUserMaxEntry 2 }

ruijieDot1xPseudoSrcmac OBJECT-TYPE
        SYNTAX EnabledStatus
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION 
            "Enable(1)/disable(2) use pseudo mac as the source mac of 802.1x
            authentication frame."
        DEFVAL { enabled }
        ::= { ruijieDot1xAuthObjects 21 } 

ruijieDot1xUserMIB OBJECT IDENTIFIER ::= { ruijieDot1xAuthObjects 22 }

ruijieDot1xUserTrapsObjects OBJECT IDENTIFIER ::= { ruijieDot1xUserMIB 1 }
ruijieDot1xUserTraps OBJECT IDENTIFIER ::= { ruijieDot1xUserMIB 2 }

ruijieDot1xUserMac OBJECT-TYPE
        SYNTAX MacAddress
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "802.1X user's MAC address."
        ::= { ruijieDot1xUserTrapsObjects 1 }

ruijieDot1xUserName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "802.1X user's name"
        ::= { ruijieDot1xUserTrapsObjects 2 }

ruijieDot1xUserIp OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "802.1X user's IPv4 address."
        ::= { ruijieDot1xUserTrapsObjects 3 }

ruijieDot1xUserIpv6 OBJECT-TYPE
        SYNTAX InetAddress
        MAX-ACCESS accessible-for-notify
        STATUS current  
        DESCRIPTION
            "802.1x user's IPv6 address."
        ::= { ruijieDot1xUserTrapsObjects 4 }

ruijieDot1xUserWlanId OBJECT-TYPE
        SYNTAX INTEGER (1..4095)
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "The wlan id that 802.1X user associated with."
        ::= { ruijieDot1xUserTrapsObjects 5 }

ruijieDot1xUserVlanId OBJECT-TYPE
        SYNTAX INTEGER (1..4094)
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "The vlan id that 801.1X user belongs to."
        ::= { ruijieDot1xUserTrapsObjects 6 }

ruijieDot1xUserSsid OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "The SSID that 802.1X user associated with."
        ::= { ruijieDot1xUserTrapsObjects 7 }

ruijieDot1xUserApMac OBJECT-TYPE
        SYNTAX MacAddress
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "The MAC address of the AP that 802.1X user associated with."
        ::= { ruijieDot1xUserTrapsObjects 8 }

ruijieDot1xUserTerminalType OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "802.1X user's terminal type."
        ::= { ruijieDot1xUserTrapsObjects 9 }

ruijieDot1xUserOperType OBJECT-TYPE
        SYNTAX INTEGER {
            online(1),
            offline(2),
            authenfail(3)
        }
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "Operation type of 802.1X user's trap."
        ::= { ruijieDot1xUserTrapsObjects 10 }

ruijieDot1xUserTerminateCause OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "Terminate cause of 802.1X user."
        ::= { ruijieDot1xUserTrapsObjects 11 }

ruijieDot1xUserReplyMessage OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "Message of authentication failure of a 802.1X user."
        ::= { ruijieDot1xUserTrapsObjects 12 }
        
ruijieDot1xUserIfIndex OBJECT-TYPE
        SYNTAX INTEGER (1..4095)
        MAX-ACCESS accessible-for-notify
        STATUS current
        DESCRIPTION
            "The interface id that 802.1X user belongs to."
        ::= { ruijieDot1xUserTrapsObjects 13 }

ruijieDot1xUserMgmtTrap NOTIFICATION-TYPE
        OBJECTS {
            ruijieDot1xUserMac,
            ruijieDot1xUserName,
            ruijieDot1xUserIp,
            ruijieDot1xUserIpv6,
            ruijieDot1xUserWlanId,
            ruijieDot1xUserVlanId,
            ruijieDot1xUserSsid,
            ruijieDot1xUserApMac,
            ruijieDot1xUserTerminalType,
            ruijieDot1xUserOperType,
            ruijieDot1xUserTerminateCause,
            ruijieDot1xUserReplyMessage,
            ruijieDot1xUserIfIndex
        }
        STATUS current
        DESCRIPTION
            "802.1X wireless user online/offline trap."
        ::= { ruijieDot1xUserTraps 1 }

ruijieDot1xWiredUserTrap NOTIFICATION-TYPE
        OBJECTS {
            ruijieDot1xUserMac,
            ruijieDot1xUserName,
            ruijieDot1xUserIfIndex,
            ruijieDot1xUserVlanId,
            ruijieDot1xUserIp,
            ruijieDot1xUserIpv6,
            ruijieDot1xUserOperType,
            ruijieDot1xUserTerminateCause
        }
        STATUS current
        DESCRIPTION
            "802.1X wired user online/offline/authentication trap."
        ::= { ruijieDot1xUserTraps 2 }

--**********************************************************************
-- about dot1x online user table info 
--**********************************************************************    
ruijieDot1xOnlineUserTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RuijieDot1xOnlineUserEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
            "Detail information of dot1x online user."
        ::= { ruijieDot1xUserMIB 3 }

ruijieDot1xOnlineUserEntry OBJECT-TYPE
    SYNTAX RuijieDot1xOnlineUserEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The entry of ruijieDot1xOnlineUserTable."
    INDEX { ruijieDot1xOnlineUserID }
    ::= { ruijieDot1xOnlineUserTable 1 }

RuijieDot1xOnlineUserEntry ::=  
    SEQUENCE {
        ruijieDot1xOnlineUserID                 INTEGER,
        ruijieDot1xOnlineUserName               DisplayString,
        ruijieDot1xOnlineUserMacAddr            MacAddress,
        ruijieDot1xOnlineUserIfIndex            INTEGER,
        ruijieDot1xOnlineUserVlanId             INTEGER,
        ruijieDot1xOnlineUserIp                 IpAddress,
        ruijieDot1xOnlineUserIpv6               InetAddress
    }

ruijieDot1xOnlineUserID OBJECT-TYPE
    SYNTAX INTEGER
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The ID of Dot1xOnlineuser that is uniqueness."
    ::= {ruijieDot1xOnlineUserEntry 1 }

ruijieDot1xOnlineUserName OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS read-only
