                ruijieAceExtIfAnyDestIp TruthValue,
                ruijieAceExtDestIp IpAddress,
                ruijieAceExtIfAnyDestWildCard TruthValue,
                ruijieAceExtDestIpWildCard IpAddress,
                ruijieAceExtIfAnyDestMacAddr TruthValue,
                ruijieAceExtDestMacAddr MacAddress,
                ruijieAceExtIfAnyEtherLikeType TruthValue,
                ruijieAceExtEtherLikeType Integer32,
                ruijieAceExtIfAnyIpProtocolField TruthValue,
                ruijieAceExtIpProtocolField Integer32,
                ruijieAceExtSourceProtocolPort Integer32,
                ruijieAceExtDestProtocolPort Integer32,
                ruijieAceExtIfAnyProtocolType TruthValue,
                ruijieAceExtProtocolType Integer32,
                ruijieAceExtFlowAction INTEGER,
                ruijieAceExtEntryStauts RowStatus,
                ruijieAceExtTimeRangeName DisplayString,
                ruijieAceExtSourcePortOp INTEGER,
                ruijieAceExtSourceProtocolPortRange Integer32,
                ruijieAceExtDestPortOp INTEGER,
                ruijieAceExtDestProtocolPortRange Integer32,
                ruijieAceExtIfAnyCos TruthValue,
                ruijieAceExtCos Integer32,
                ruijieAceExtIfAnyIpPrec TruthValue,
                ruijieAceExtIpPrec Integer32,
                ruijieAceExtIfAnyDscp TruthValue,
                ruijieAceExtDscp Integer32,
                ruijieAceExtIfAnySourceMacAddrWildCard TruthValue,
                ruijieAceExtSourceMacAddrWildCard MacAddress,
                ruijieAceExtIfAnyDestMacAddrWildCard TruthValue,
                ruijieAceExtDestMacAddrWildCard MacAddress,
                ruijieAceExtIfAnyTcpFlag TruthValue,
                ruijieAceExtTcpFlag Integer32,
                ruijieAceExtIfAnySourceIp6 TruthValue,
                ruijieAceExtSourceIp6 OCTET STRING,
                ruijieAceExtIfAnySourceIp6WildCard TruthValue,
                ruijieAceExtSourceIp6WildCard OCTET STRING,
                ruijieAceExtIfAnyDestIp6 TruthValue,
                ruijieAceExtDestIp6 OCTET STRING,
                ruijieAceExtIfAnyDestIp6WildCard TruthValue,
                ruijieAceExtDestIp6WildCard OCTET STRING
        }

