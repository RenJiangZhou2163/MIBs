ruijieAclIfTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RuijieAclIfEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "list of ACL interface objects."
        ::= { ruijieAclMIBObjects 3 }
    
ruijieAclIfEntry OBJECT-TYPE
        SYNTAX RuijieAclIfEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Entry ACL interface information."
        INDEX { ruijieAclIfIndex }
        ::= { ruijieAclIfTable 1 }

RuijieAclIfEntry ::=
        SEQUENCE {
            ruijieAclIfIndex IfIndex,
            ruijieAclIfMaxEntryNum Integer32,
            ruijieAclIfCurruntEntryNum Integer32,
            ruijieIfInAclName  DisplayString,
            ruijieIfOutAclName DisplayString,
            ruijieAclIf6MaxEntryNum Integer32,
            ruijieAclIf6CurruntEntryNum Integer32,
            ruijieIf6InAclName  DisplayString,
            ruijieIf6OutAclName DisplayString
        }

ruijieAclIfIndex OBJECT-TYPE
        SYNTAX IfIndex
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            " "
        ::= { ruijieAclIfEntry 1 }

ruijieAclIfMaxEntryNum OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
               "Max num of ACL entry(ACE) in a interface."
        ::= { ruijieAclIfEntry 2 }

ruijieAclIfCurruntEntryNum OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
               "Currunt valid num of ACL entry(ACE) in a interface."
        ::= { ruijieAclIfEntry 3 }
        
ruijieIfInAclName OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..32))
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Access list name of ACL of this interface match for input. 
            It indicate that this interface doesn't match any ACL for input 
            if this string is null"
        ::= { ruijieAclIfEntry  4 }        
        
ruijieIfOutAclName OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..32))
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Access list name of ACL of this interface match for output. 
            It indicate that this interface doesn't match any ACL for output 
            if this string is null
            
            This relation function apply only L3 interface."
        ::= { ruijieAclIfEntry  5 }  
        
ruijieAclIf6MaxEntryNum OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
               "Max num of ACL6 entry(ACE) in a interface."
