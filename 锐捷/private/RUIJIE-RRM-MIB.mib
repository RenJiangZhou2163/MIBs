RUIJIE-RRM-MIB DEFINITIONS ::= BEGIN

IMPORTS
        IpAddress,
        OBJECT-TYPE,
        MODULE-IDENTITY,
        NOTIFICATION-TYPE,
        Integer32,
        Unsigned32
                FROM SNMPv2-SMI
        MacAddress,
        DisplayString,
        TruthValue
                FROM SNMPv2-TC
        NOTIFICATION-GROUP,
        MODULE-COMPLIANCE,
        OBJECT-GROUP
                FROM SNMPv2-CONF
        ruijieMgmt
                FROM RUIJIE-SMI;

--********************************************************************
--  MODULE IDENTITY
--********************************************************************

ruijieRrmMIB MODULE-IDENTITY
    LAST-UPDATED "200912150000Z"
    ORGANIZATION "Ruijie Networks Co.,Ltd."
    CONTACT-INFO
        "
    Tel: 4008-111-000

    E-mail: service@ruijie.com.cn"
    DESCRIPTION
        "This module defines wlan rrm mibs."
    REVISION      "200912150000Z"
    DESCRIPTION
        "Initial version of this MIB module."
    ::= { ruijieMgmt 63 }


--********************** define the Rrm root nodes ***************************
ruijieRrmMIBObjects     OBJECT IDENTIFIER     ::= { ruijieRrmMIB 1 }
ruijieRrmMIBTraps       OBJECT IDENTIFIER     ::= { ruijieRrmMIB 2 }
ruijieRrmMIBConformance OBJECT IDENTIFIER     ::= { ruijieRrmMIB 3 }


--***************** define the ruijieRrmMIBObjects nodes *********************
ruijieRrmObjectsGroup   OBJECT IDENTIFIER      ::= { ruijieRrmMIBObjects 1 }
ruijieRrmObjectsDot11a  OBJECT IDENTIFIER      ::= { ruijieRrmMIBObjects 2 }
ruijieRrmObjectsDot11b  OBJECT IDENTIFIER      ::= { ruijieRrmMIBObjects 3 }
ruijieRrmObjectsAP     	OBJECT IDENTIFIER      ::= { ruijieRrmMIBObjects 4 }

--***************** define the ruijieRrmObjectsDot11a nodes *****************
ruijieRrmDCADot11a      OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11a 1 }
ruijieRrmTPCDot11a      OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11a 2 }
ruijieRrmCHDDot11a      OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11a 3 }
ruijieRrmGroupDot11a    OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11a 4 }
ruijieRrmProfileDot11a  OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11a 5 }
ruijieRrmMonitorDot11a  OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11a 6 }
ruijieRrmFactoryDot11a  OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11a 7 }

--***************** define the ruijieRrmObjectsDot11a nodes *****************
ruijieRrmDCADot11b      OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11b 1 }
ruijieRrmTPCDot11b      OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11b 2 }
ruijieRrmCHDDot11b      OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11b 3 }
ruijieRrmGroupDot11b    OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11b 4 }
ruijieRrmProfileDot11b  OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11b 5 }
ruijieRrmMonitorDot11b  OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11b 6 }
ruijieRrmFactoryDot11b  OBJECT IDENTIFIER      ::= { ruijieRrmObjectsDot11b 7 }


--********************************************************************
--                                                                  **
--                           Rf group name                          **
--                                                                  **
--********************************************************************

ruijieRrmRFNetworkName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(1..19))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "RF Network Group Name to which this Ruijie Switch belongs.
         Airespace Switches on a network form a RF Network Group as
         well as a Mobility Group. RF Network Groups does the channel
         and power management of AP while Mobility Group does load
         balancing and hand off for clients."
    ::= { ruijieRrmObjectsGroup 1 }


--********************************************************************
--                                                                  **
--                       AP Profile Display                         **
--                                                                  **
--********************************************************************
ProfileState ::= INTEGER { pass (1), fail (0) }

ruijieRrmAPIfSlotId OBJECT-TYPE
    SYNTAX 			INTEGER(1..32)
    MAX-ACCESS 	read-only
    STATUS 			current
    DESCRIPTION
        		"Number of Radio Interfaces on the Airespace AP.
         		Currently maximum two interfaces are supported.
         		One would be of type 802.11a and other of type
         		802.11b/g."
    ::= {  ruijieRrmObjectsAP 1 }

ruijieRrmAPName OBJECT-TYPE
    SYNTAX 			DisplayString (SIZE(1..32))
    MAX-ACCESS 	read-only
    STATUS 			current
    DESCRIPTION
        		"Name assigned to this AP. If an AP is not configured its
         		factory default name will be ap:<last three byte of
         		MACAddress> eg. ap:af:12:be"
    ::= { ruijieRrmObjectsAP 2 }


--********************************************************************
-- * Begin of ruijieRrmAPIfProfileThresholdConfigTable
--********************************************************************

ruijieRrmAPIfProfileThresholdConfigTable OBJECT-TYPE
		SYNTAX 			SEQUENCE OF RuijieRrmAPIfProfileThresholdConfigEntry
		MAX-ACCESS 	not-accessible
		STATUS 			current
		DESCRIPTION
        		"Table of attributes for various thresholds to be
         		set on each Airespace AP Interface for Load
        		performance profile , interference performance
        		profile and Noise performance profile."
		::= { ruijieRrmObjectsAP 3 }

ruijieRrmAPIfProfileThresholdConfigEntry OBJECT-TYPE
		SYNTAX 			RuijieRrmAPIfProfileThresholdConfigEntry
 		MAX-ACCESS 	not-accessible
  	STATUS 			current
  	DESCRIPTION
         		"An entry (conceptual row) in the Phy Thresholds
          	Table. Entries in this MIB are indexed by
        		ruijieRrmAPDot3MacAddress and ruijieRrmAPIfSlotId"
   	INDEX { ruijieRrmAPIfThresholdMacAddr, ruijieRrmAPIfThresholdRadioType }
    ::= { ruijieRrmAPIfProfileThresholdConfigTable 1 }

RuijieRrmAPIfProfileThresholdConfigEntry ::=
     	SEQUENCE {
     				ruijieRrmAPIfThresholdMacAddr    MacAddress,
            ruijieRrmAPIfThresholdRadioType  INTEGER,
            ruijieRrmAPIfThresholdName DisplayString,
            ruijieRrmAPIfForeignInterferenceThreshold INTEGER,
            ruijieRrmAPIfForeignNoiseThreshold        INTEGER,
            ruijieRrmAPIfRFUtilizationThreshold       INTEGER,
            ruijieRrmAPIfThroughputThreshold          Unsigned32,
            ruijieRrmAPIfMobilesThreshold             INTEGER,
            ruijieRrmAPIfForeignGlobalConfig          INTEGER,
            ruijieRrmAPIfNoiseGlobalConfig            INTEGER,
            ruijieRrmAPIfRFUtilizationGlobalConfig    INTEGER,
            ruijieRrmAPIfThroughputGlobalConfig       INTEGER,
            ruijieRrmAPIfMobilesGlobalConfig          INTEGER
		}

ruijieRrmAPIfThresholdRadioType OBJECT-TYPE
    SYNTAX 			INTEGER {type80211a(0),type80211b(1)}
    MAX-ACCESS 	read-only
    STATUS 			current
    DESCRIPTION
      			"radio type, type 802.11b means 2.4GHz, type 802.11a means 5GHz,"
		::= {ruijieRrmAPIfProfileThresholdConfigEntry 1 }

ruijieRrmAPIfForeignInterferenceThreshold OBJECT-TYPE
    SYNTAX 			INTEGER(1..100)
    MAX-ACCESS 	read-write
    STATUS 			current
    DESCRIPTION
      			"foreign interference threshold between 1 and 100 percent."
		::= {ruijieRrmAPIfProfileThresholdConfigEntry 2 }

ruijieRrmAPIfForeignNoiseThreshold OBJECT-TYPE
    SYNTAX 			INTEGER(-127..0)
    MAX-ACCESS 	read-write
    STATUS 			current
    DESCRIPTION
     				"foreign noise threshold between -127 and 0 dBm."
   	::= {ruijieRrmAPIfProfileThresholdConfigEntry 3 }

ruijieRrmAPIfRFUtilizationThreshold OBJECT-TYPE
    SYNTAX 			INTEGER(1..100)
    MAX-ACCESS 	read-write
    STATUS 			current
    DESCRIPTION
    				"RF utlization threshold between 1 and 100 percent."
		::= {ruijieRrmAPIfProfileThresholdConfigEntry 4 }

ruijieRrmAPIfThroughputThreshold OBJECT-TYPE
    SYNTAX 			Unsigned32(1000..10000000)
    MAX-ACCESS 	read-write
    STATUS 			current
    DESCRIPTION
    				" Airespace AP data-rate threshold between 1000 and 1000000000."
		::= {ruijieRrmAPIfProfileThresholdConfigEntry 5 }

ruijieRrmAPIfMobilesThreshold OBJECT-TYPE
    SYNTAX 			INTEGER(1..128)
    MAX-ACCESS 	read-write
    STATUS 			current
    DESCRIPTION
    				" Airespace AP mobiles threshold between 1 and 128."
		::= {ruijieRrmAPIfProfileThresholdConfigEntry 6 }

ruijieRrmAPIfThresholdName OBJECT-TYPE
    SYNTAX 			DisplayString (SIZE(1..32))
    MAX-ACCESS 	read-only
    STATUS 			current
    DESCRIPTION
        		"The name of the AP"
    ::= { ruijieRrmAPIfProfileThresholdConfigEntry 7 }

ruijieRrmAPIfThresholdMacAddr OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The MAC address of radio on the AP"
    ::= { ruijieRrmAPIfProfileThresholdConfigEntry 8 }

ruijieRrmAPIfForeignGlobalConfig OBJECT-TYPE
    SYNTAX 			INTEGER(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Interference threshold inherited from global or not, 1: global config, 0: ap config"
    ::= { ruijieRrmAPIfProfileThresholdConfigEntry 9 }

ruijieRrmAPIfNoiseGlobalConfig OBJECT-TYPE
    SYNTAX 			INTEGER(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Noise threshold inherited from global or not, 1: global config, 0: ap config"
    ::= { ruijieRrmAPIfProfileThresholdConfigEntry 10 }

ruijieRrmAPIfRFUtilizationGlobalConfig OBJECT-TYPE
    SYNTAX 			INTEGER(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "RFUtilization threshold inherited from global or not, 1: global config, 0: ap config"
    ::= { ruijieRrmAPIfProfileThresholdConfigEntry 11 }

ruijieRrmAPIfThroughputGlobalConfig OBJECT-TYPE
    SYNTAX 			INTEGER(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Throughput threshold inherited from global or not, 1: global config, 0: ap config"
    ::= { ruijieRrmAPIfProfileThresholdConfigEntry 12 }

ruijieRrmAPIfMobilesGlobalConfig OBJECT-TYPE
    SYNTAX 			INTEGER(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Mobiles threshold inherited from global or not, 1: global config, 0: ap config"
    ::= { ruijieRrmAPIfProfileThresholdConfigEntry 13 }

--********************************************************************
-- * Begin of ruijieRrmAPIfLoadParametersTable
--********************************************************************
ruijieRrmAPIfLoadParametersTable OBJECT-TYPE
		SYNTAX 			SEQUENCE OF RuijieRrmAPIfLoadParametersEntry
		MAX-ACCESS 	not-accessible
		STATUS 			current
		DESCRIPTION
        		"These are RRM performance related read only parameters
        		per Airespace AP"
		::= { ruijieRrmObjectsAP 4 }

ruijieRrmAPIfLoadParametersEntry OBJECT-TYPE
		SYNTAX 			RuijieRrmAPIfLoadParametersEntry
		MAX-ACCESS 	not-accessible
		STATUS 			current
		DESCRIPTION
          	"An entry (conceptual row) in the Table.
          	Entries in this MIB are indexed by
         	ruijieRrmAPDot3MacAddress and ruijieRrmAPIfSlotId"
 		INDEX { ruijieRrmAPIfLoadMacAddr, ruijieRrmAPIfLoadSlotId }
 		::= { ruijieRrmAPIfLoadParametersTable 1 }

RuijieRrmAPIfLoadParametersEntry ::=
  	SEQUENCE {
  	    ruijieRrmAPIfLoadName DisplayString,
        ruijieRrmAPIfLoadMacAddr MacAddress,
        ruijieRrmAPIfLoadSlotId INTEGER,
     		ruijieRrmAPIfLoadRxUtilization        INTEGER,
   			ruijieRrmAPIfLoadTxUtilization        INTEGER,
  			ruijieRrmAPIfLoadChannelUtilization   INTEGER,
   			ruijieRrmAPIfLoadNumOfClients         Integer32,
   			ruijieRrmAPIfPoorSNRClients           Integer32,
   			ruijieRrmAPIfThroughput		      INTEGER
		}

ruijieRrmAPIfLoadRxUtilization OBJECT-TYPE
		SYNTAX 			INTEGER(0..100)
		MAX-ACCESS 	read-only
		STATUS 			current
		DESCRIPTION
      			"This is the percentage of time the Airespace AP
        		receiver is busy operating on packets. It is a number
       			from 0-100 representing a load from 0 to 1."
		::= { ruijieRrmAPIfLoadParametersEntry 1 }

ruijieRrmAPIfLoadTxUtilization OBJECT-TYPE
 		SYNTAX 			INTEGER(0..100)
		MAX-ACCESS 	read-only
		STATUS 			current
		DESCRIPTION
      			"This is the percentage of time the Airespace AP
         		transmitter is busy operating on packets. It is a
       			number from 0-100 representing a load from 0 to 1."
		::= { ruijieRrmAPIfLoadParametersEntry 2 }

ruijieRrmAPIfLoadChannelUtilization OBJECT-TYPE
    SYNTAX 			INTEGER(0..100)
    MAX-ACCESS 	read-only
    STATUS 			current
    DESCRIPTION
            "Channel Utilization"
    ::= { ruijieRrmAPIfLoadParametersEntry 3 }

ruijieRrmAPIfLoadNumOfClients OBJECT-TYPE
		SYNTAX 			Integer32
   	MAX-ACCESS 	read-only
   	STATUS 			current
   	DESCRIPTION
         		"This is the number of clients attached to this Airespace
        		AP at the last measurement interval(This comes from
        		APF)"
    ::= { ruijieRrmAPIfLoadParametersEntry 4 }

ruijieRrmAPIfPoorSNRClients OBJECT-TYPE
		SYNTAX 			Integer32
		MAX-ACCESS 	read-only
		STATUS 			current
		DESCRIPTION
           	"This is the number of clients with poor SNR attached to
          	this Airespace AP at the last measurement interval
          	( This comes from APF )."
		::= { ruijieRrmAPIfLoadParametersEntry 5 }

ruijieRrmAPIfLoadName OBJECT-TYPE
    SYNTAX 			DisplayString (SIZE(1..32))
    MAX-ACCESS 	read-only
    STATUS 			current
    DESCRIPTION
        		"The name of the AP"
    ::= { ruijieRrmAPIfLoadParametersEntry 6 }

ruijieRrmAPIfLoadMacAddr OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
            "The MAC address of radio on the AP"
    ::= { ruijieRrmAPIfLoadParametersEntry 7 }

ruijieRrmAPIfLoadSlotId OBJECT-TYPE
    SYNTAX 			INTEGER(1..32)
    MAX-ACCESS 	read-only
    STATUS 			current
    DESCRIPTION
        		"Number of Radio Interfaces on the Airespace AP.
         		Currently maximum two interfaces are supported.
         		One would be of type 802.11a and other of type
         		802.11b/g."
    ::= {  ruijieRrmAPIfLoadParametersEntry 8 }

ruijieRrmAPIfThroughput OBJECT-TYPE
    SYNTAX 			INTEGER
    MAX-ACCESS 	read-only
    STATUS 			current
    DESCRIPTION
        		"Throughput in ap's radio, it represent the performance of
        		ap"
    ::= {  ruijieRrmAPIfLoadParametersEntry 9 }


