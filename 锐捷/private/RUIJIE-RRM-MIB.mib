		MAX-ACCESS 	read-only
		STATUS 			current
		DESCRIPTION
						"Interference from other 802.11 networks on this
		        channel"
		::= { ruijieRrmAPIfChannelInterferenceInfoEntry 3}

ruijieRrmAPIfInterferenceName OBJECT-TYPE
    SYNTAX 			DisplayString (SIZE(1..32))
    MAX-ACCESS 	read-only
    STATUS 			current
    DESCRIPTION
        		"The name of the AP"
    ::= { ruijieRrmAPIfChannelInterferenceInfoEntry 4 }

ruijieRrmAPIfInterferenceMacAddr OBJECT-TYPE
    SYNTAX      MacAddress
