--********************************************************************
-- * Begin of ruijieRrmAPIfProfileStateTable
--********************************************************************

ruijieRrmAPIfProfileStateTable OBJECT-TYPE
		SYNTAX 			SEQUENCE OF RuijieRrmAPIfProfileStateEntry
		MAX-ACCESS 	not-accessible
		STATUS 			current
		DESCRIPTION
		        "This is a table of state of interference monitor
		         on each Airespace AP"
		::= { ruijieRrmObjectsAP 7 }

ruijieRrmAPIfProfileStateEntry OBJECT-TYPE
		SYNTAX 			RuijieRrmAPIfProfileStateEntry
		MAX-ACCESS 	not-accessible
		STATUS 			current
		DESCRIPTION
		       "An entry (conceptual row) in the Table.
		        Entries in this MIB are indexed by ruijieRrmAPDot3MacAddress,
		        ruijieRrmAPIfSlotId"
		INDEX { ruijieRrmAPIfProfileMacAddr, ruijieRrmAPIfProfileSlotId }
		::= { ruijieRrmAPIfProfileStateTable 1 }

RuijieRrmAPIfProfileStateEntry ::=
		SEQUENCE {
		    ruijieRrmAPIfProfileName DisplayString,
        ruijieRrmAPIfProfileMacAddr MacAddress,
        ruijieRrmAPIfProfileSlotId INTEGER,
        ruijieRrmAPIfLoadProfileState          	ProfileState,
        ruijieRrmAPIfInterferenceProfileState 	ProfileState,
        ruijieRrmAPIfNoiseProfileState         	ProfileState,
        ruijieRrmAPIfCoverageProfileState      	ProfileState,
        ruijieRrmAPIfPerformanceProfileState 		ProfileState
		}

ruijieRrmAPIfLoadProfileState OBJECT-TYPE
		SYNTAX 			ProfileState
		MAX-ACCESS 	read-only
		STATUS 			current
		DESCRIPTION
		        "This field represents the current state of the LOAD
		         monitor. This is a total measurement of the business of
		         this Airespace AP. PASS indicates that this Airespace
		         AP is performing adequately compared to the Airespace
		         AP profile. FAIL indicates the Airespace AP is not
		         performing adequately against the LOAD profile."
		::= { ruijieRrmAPIfProfileStateEntry 1 }


ruijieRrmAPIfInterferenceProfileState OBJECT-TYPE
		SYNTAX 			ProfileState
		MAX-ACCESS 	read-only
		STATUS 			current
		DESCRIPTION
		       "This field represents the current state of Interference
		        monitor. This is a total measurement of the interference
		        present at this Airespace AP. PASS indicates that this
		        Airespace AP is performing adequately compared to the
		        Interference profile.  FAIL indicates the Airespace AP
		        is not performing adequately against the Interference
		        profile."
		::= { ruijieRrmAPIfProfileStateEntry 2 }

ruijieRrmAPIfNoiseProfileState OBJECT-TYPE
		SYNTAX 			ProfileState
		MAX-ACCESS 	read-only
		STATUS 			current
		DESCRIPTION
						"This field represents the current state of Noise
						monitor. This is a total measurement of the noise
						present at this Airespace AP. PASS indicates that this
						Airespace AP is performing adequately compared to the
						noise profile.
						FAIL indicates the Airespace AP is not performing
						adequately against the noise profile."
		::= { ruijieRrmAPIfProfileStateEntry 3 }

ruijieRrmAPIfCoverageProfileState OBJECT-TYPE
		SYNTAX 			ProfileState
		MAX-ACCESS 	read-only
		STATUS 			current
		DESCRIPTION
		       "This field represents the current state of coverage
		        monitor. This is a total measurement of the client
		        coverage at this Airespace AP. PASS indicates that
		        this  Airespace AP is performing adequately compared
		        to the  coverage profile. FAIL indicates the Airespace
		        AP is not performing adequately against the coverage
		        profile."
		::= { ruijieRrmAPIfProfileStateEntry 4}

ruijieRrmAPIfPerformanceProfileState OBJECT-TYPE
		SYNTAX 			ProfileState
		MAX-ACCESS 	read-only
		STATUS 			current
		DESCRIPTION
		       ""
		::= { ruijieRrmAPIfProfileStateEntry 5}

ruijieRrmAPIfProfileName OBJECT-TYPE
    SYNTAX 			DisplayString (SIZE(1..32))
