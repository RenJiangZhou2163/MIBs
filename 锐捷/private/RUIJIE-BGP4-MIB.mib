RUIJIE-BGP4-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY, 
        OBJECT-TYPE, 
        Unsigned32,
        NOTIFICATION-TYPE,
        Counter32
              FROM SNMPv2-SMI
        TruthValue,
        TEXTUAL-CONVENTION
              FROM SNMPv2-TC
        InetAutonomousSystemNumber,
        InetAddressType,
        InetAddress 	
	      FROM INET-ADDRESS-MIB
        AddressFamilyNumbers
          FROM IANA-ADDRESS-FAMILY-NUMBERS-MIB        
        bgpPeerEntry,
        bgpPeerRemoteAddr
              FROM BGP4-MIB
        ruijieMgmt
              FROM RUIJIE-SMI;         

