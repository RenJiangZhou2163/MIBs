            ruijieIfQosWFQIfIndex
                Integer32,
            ruijieIfQosWFQIfName
                OCTET STRING,
            ruijieIfQosWFQMaxQueLen
                Integer32,
            ruijieIfQosWFQTotalQueNum
                INTEGER,
            ruijieIfQosWFQCurQueLen
                Integer32,
            ruijieIfQosWFQTotalDiscard
                Counter32,
            ruijieIfQosWFQActiveQueNum
