    ruijieHQoSIfAppTable  OBJECT-TYPE
        SYNTAX            SEQUENCE OF RuijieHQoSIfAppEntry
        MAX-ACCESS        not-accessible
        STATUS            current
        DESCRIPTION 
         "The HQoS config with this table."
         ::= { ruijieHQoSIfAppObjects 1 }
       
    ruijieHQoSIfAppEntry   OBJECT-TYPE
        SYNTAX        RuijieHQoSIfAppEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
          "A row in this table represents the interface HQoS config information."
        INDEX  { ruijieHQoSIfAppIndex }
        ::= { ruijieHQoSIfAppTable 1 }

  RuijieHQoSIfAppEntry ::= SEQUENCE {
        ruijieHQoSIfAppIndex             InterfaceIndex,
        ruijieHQoSIfAppInPolicyName      OCTET STRING,
        ruijieHQoSIfAppInPolicyIndex     Unsigned32,
        ruijieHQoSIfAppInPolicyLayer     RuijieLayerType,       
        ruijieHQoSIfAppOutPolicyName     OCTET STRING,
        ruijieHQoSIfAppOutPolicyIndex    Unsigned32,
        ruijieHQoSIfAppOutPolicyLayer    RuijieLayerType,  
        ruijieHQoSIfAppPortQueueName     OCTET STRING,
        ruijieHQoSIfAppPortQueueIndex    Unsigned32, 
        ruijieHQoSIfAppPortQueueShaping  INTEGER
        }

    ruijieHQoSIfAppIndex OBJECT-TYPE
        SYNTAX                InterfaceIndex
        MAX-ACCESS            not-accessible
        STATUS                current
        DESCRIPTION
