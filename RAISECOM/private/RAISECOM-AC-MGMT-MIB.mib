                FROM CAPWAP-BASE-MIB
        ruijieMgmt
                FROM RAISECOM-SMI; 
                
ruijieAcMgmtMIB MODULE-IDENTITY
    LAST-UPDATED "200909180000Z"
    ORGANIZATION "Raisecom Networks Co.,Ltd."
    CONTACT-INFO
        " 
    Tel: 4008-111-000

    E-mail: service@raisecom.com.cn"
    DESCRIPTION
        "This module defines ac management mibs."
    REVISION      "200909180000Z"
    DESCRIPTION
        "Initial version of this MIB module."
    ::= { ruijieMgmt 56}

--***************** define the AC root nodes *****************
ruijieAcMgmtAcMIBObjects OBJECT IDENTIFIER      ::= { ruijieAcMgmtMIB 1 }
ruijieAcMgmtAc     OBJECT IDENTIFIER ::= { ruijieAcMgmtAcMIBObjects 1 }
ruijieAcMgmtAcIf   OBJECT IDENTIFIER ::= { ruijieAcMgmtAcMIBObjects 2 }

--***************** define the AP root nodes *****************
ruijieAcMgmtApMIBObjects OBJECT IDENTIFIER      ::= { ruijieAcMgmtMIB 2 }
