		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.32
		ruijie3GLineDownCause OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The cause of line down."
			::= { ruijie3GEntry 32 }
	   
	       ruijie3GModemType OBJECT-TYPE
			SYNTAX INTEGER
				{
				modem-type-3G(1),
				modem-type-4G(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"the type of modem used in router."
			::= { ruijie3GEntry 33 }

		
-- ===============================================
-- Begin the table of ruijie3GstatTable.
-- ===============================================

    ruijie3GStatTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Ruijie3GStatEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The 3G connection infomation fot every user. There is one
             entry in this table for each active 3G connection."
        ::= { ruijie3GObjects 2 }

    ruijie3GStatEntry OBJECT-TYPE
        SYNTAX Ruijie3GStatEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about ruijie3GTunnelTable."
        INDEX { ruijie3GIPAddr }
        ::= { ruijie3GStatTable 1 }
 
    Ruijie3GStatEntry ::=
        SEQUENCE {
            ruijie3GInOctets
                Counter64,
            ruijie3GOutOctets
                Counter64,
            ruijie3GInSpeed
                Counter64,
            ruijie3GOutSpeed
                Counter64,
            ruijie3G2IMSI
                DisplayString,
            ruijie3G2ifIndex
                INTEGER
        }
        
    ruijie3GInOctets OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of octets received by this 3G user."
        ::= { ruijie3GStatEntry 1 }

    ruijie3GOutOctets OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of octets sent by this 3G user."
        ::= { ruijie3GStatEntry 2 } 
 
    ruijie3GInSpeed OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Speed(bps) of packets received by this 3G user."
        ::= { ruijie3GStatEntry 3 }

    ruijie3GOutSpeed OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Speed(bps) of packets snet by this 3G user."
        ::= { ruijie3GStatEntry 4 }
        
    ruijie3G2IMSI OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The IMSI  of 3G User."
        ::= { ruijie3GStatEntry 5 } 
    
    ruijie3G2ifIndex OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
            "The Index of 3G Interface."
    ::= { ruijie3GStatEntry 6 } 
 
-- ===============================================
-- Begin the ruijie3GTrapObject.
-- ===============================================

    ruijie3GTrap OBJECT IDENTIFIER ::= { ruijie3GObjects 3 }
    
    ruijie3GNotifications OBJECT IDENTIFIER ::= { ruijie3GTrap 1 }
    
    ruijie3GSignalThreshold NOTIFICATION-TYPE
        OBJECTS { 
                ruijie3GIPAddr, 
                ruijie3GSignalStrength,
                ruijie3GSignalStrengthPercent,
                ruijie3GIMSI 
        }
        STATUS current
        DESCRIPTION
            "This notification is generated when 3G signal is lower 
            than the specific Threshold value."
        ::= { ruijie3GNotifications 1 }
    
    ruijie3GUpLine NOTIFICATION-TYPE
        OBJECTS { 
                ruijie3GIPAddr,	
        	ruijie3gUsername,
        	ruijie3GIMSI,
                ruijie3GBackupInfo,
                ruijie3GSerialNumber,
                ruijie3GGatewayIPAddr							  
        }
        STATUS current
        DESCRIPTION
            "This notification is generated when 3G user up line."
        ::= { ruijie3GNotifications 2 }
    
    ruijie3GDownLine NOTIFICATION-TYPE
        OBJECTS { 
                ruijie3GLineDownCause,
		ruijie3GIPAddr,        		  
                ruijie3gUsername,
                ruijie3GIMSI 
        	}
        STATUS current
        DESCRIPTION
            "This notification is generated when 3G user down line."
        ::= { ruijie3GNotifications 3 }
        
      ruijie3GChangeAccessPoint NOTIFICATION-TYPE
        OBJECTS { 
                ruijie3GIPAddr,  
                ruijie3GApn,        		  
                ruijie3gUsername,
                ruijie3GIMSI 
                }
        STATUS current
        DESCRIPTION
            "This notification is generated when 3G change access point."
        ::= { ruijie3GNotifications 4 }
        
       ruijie3GBackupIntfSwitch NOTIFICATION-TYPE
        OBJECTS { 
                ruijie3GIPAddr,        		  
                ruijie3gUsername,
                ruijie3GIMSI,
                ruijie3GSerialNumber,
                ruijie3GBackupIMSI				  
                }
        STATUS current
        DESCRIPTION
            "This notification is generated when 3G switch to backup interface 
            in double card backup configuration."	
        ::= { ruijie3GNotifications 5 }
    
       ruijie3GBaseSationSwitch NOTIFICATION-TYPE
           OBJECTS {   
                   ruijie3GISP,
                   ruijie3GCellID,
                   ruijie3GLAC,
                   ruijie3GBSID,
                   ruijie3GSID,
                   ruijie3GNID,
                   ruijie3GIMSI,
                   ruijie3GPhoneNumber
                }
        STATUS current
        DESCRIPTION
            "This notification is generated when the access base station changes."	
        ::= { ruijie3GNotifications 6 }
		
	ruijie3GTrafficInformation NOTIFICATION-TYPE
        OBJECTS {    
                ruijie3GIPAddr,
		ruijie3GIMSI,
		ruijie3GSerialNumber,
		ruijie3GInOctets,
		ruijie3GOutOctets
	}
        STATUS current
        DESCRIPTION
            "This notification is used for reporting the traffic information of 3G user regularly. "
        ::= { ruijie3GNotifications 7 }

-- ===============================================
-- Begin the scalar parameters
-- ===============================================
    ruijie3GBsNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The number of Base Station available on the system"
        ::= { ruijie3GObjects 4 }

-- ===============================================
-- Begin the table of ruijie3GBsTable.
-- ===============================================

    ruijie3GBsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Ruijie3GBsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The Base Station Information."
        ::= { ruijie3GObjects 5 }

    ruijie3GBsEntry OBJECT-TYPE
        SYNTAX Ruijie3GBsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about Base Station."
        INDEX { ruijie3GBsSN }
        ::= { ruijie3GBsTable 1 }
 
    Ruijie3GBsEntry ::=
        SEQUENCE {
            ruijie3GBsSN
                Integer32,
            ruijie3GBsISP
