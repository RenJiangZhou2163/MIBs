            ruijie3GBsIMSI
                DisplayString,
 	        ruijie3GBsLAC
                Integer32,
            ruijie3GBsCellID
                Integer32,
            ruijie3GBsBSID
                Integer32,
            ruijie3GBsSID
                Integer32,
            ruijie3GBsNID 
                Integer32,
            ruijie3GBsRssi
                Integer32,
            ruijie3GBsBSLONG
                Integer32,
            ruijie3GBsBSLAT
                Integer32
        }

    ruijie3GBsSN OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "the Sequence Of The Base Station."
        ::= { ruijie3GBsEntry 1 }

    ruijie3GBsISP OBJECT-TYPE
        SYNTAX INTEGER
            {
            chinaUnicom(1),
            chinaTelecom(2),
            chinaMobile(3)
            }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "the ISP of 3G networsk."
        ::= { ruijie3GBsEntry 2 }
        
    ruijie3GBsMode OBJECT-TYPE
        SYNTAX INTEGER
            {
            sys2GMode(1),
            sys3GMode(2)
            }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "the sys mode of 3G networsk."
        ::= { ruijie3GBsEntry 3 }
        
     ruijie3GBsIMSI OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The IMSI of 3G User."
        ::= { ruijie3GBsEntry 4 }
        
    ruijie3GBsLAC OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
