-- Description: The MIB is designed to get 3G wireless safenet solution statistic information.
-- Reference: rujie Enterprise MIB
-- =====================================================================

RAISECOM-3G-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        DisplayString,TEXTUAL-CONVENTION,TimeStamp
            FROM SNMPv2-TC
        IpAddress, Integer32, Counter32, Counter64, OBJECT-TYPE,
        MODULE-IDENTITY, Gauge32, NOTIFICATION-TYPE, Unsigned32
            FROM SNMPv2-SMI
        MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
            FROM SNMPv2-CONF
        ruijieMgmt
            FROM RAISECOM-SMI;

    ruijie3GMonitor MODULE-IDENTITY
        LAST-UPDATED "201102220000Z"            
        ORGANIZATION
            "Raisecom Networks Co.,Ltd."
        CONTACT-INFO
            "Tel: 4008-111-000 
             E-mail: service@raisecom.com.cn"  
        DESCRIPTION
            "The MIB is designed to get statistic information of
             3G wireless safenet. With this MIB, we can get information of a certain
             3G user or all users."
        REVISION      "201102220000Z"
        DESCRIPTION
                "Initial version of this MIB module."
        ::= { ruijieMgmt 95 }       
                               
                       
-- ========================================================================
-- Node definitions
-- ========================================================================
--Begin the node of ruijie3GObjects.

    ruijie3GObjects OBJECT IDENTIFIER ::= { ruijie3GMonitor 1 }

-- ===============================================
-- Begin the table of ruijie3GTable.
-- ===============================================

    ruijie3GTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Ruijie3GEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The 3G connection infomation fot every user. There is one
             entry in this table for each active 3G connection."
        ::= { ruijie3GObjects 1 }

    ruijie3GEntry OBJECT-TYPE
        SYNTAX Ruijie3GEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about ruijie3GTunnelTable."
        INDEX { ruijie3GIPAddr }
        ::= { ruijie3GTable 1 }
    
    Ruijie3GEntry ::=
        SEQUENCE {
            ruijie3gUsername
                DisplayString,
            ruijie3GOnlineStatus
            	INTEGER, 
            ruijie3GIMEI
                DisplayString,
            ruijie3GIPAddrType
            	INTEGER,
            ruijie3GIPAddr
            	IpAddress,
            ruijie3GUplineTime
                TimeStamp,
            ruijie3GActiveTime
           	    Integer32,
            ruijie3GSignalStrength
           	    Integer32,
            ruijie3GISP
           	    INTEGER,
            ruijie3GSysMode
           	    INTEGER,
            ruijie3GServiceStatus        		
            	INTEGER,
            ruijie3GRoamingStatus
                INTEGER,
            ruijie3GDomain        
            	INTEGER,
            ruijie3GSIMStatus
                INTEGER,
   	    ruijie3GSignalStrengthPercent
                Integer32,
            ruijie3GApn
                DisplayString,
            ruijie3GCellID
                Integer32,
            ruijie3GLAC
                Integer32,
            ruijie3GBSID
                Integer32,
            ruijie3GSID
                Integer32,
            ruijie3GNID 
                Integer32,
            ruijie3GIMSI
                DisplayString,
            ruijie3GESN
                DisplayString,
            ruijie3GPhoneNumber
                DisplayString,
            ruijie3GifIndex
                INTEGER,
            ruijie3GBSLONG
                Integer32,
            ruijie3GBSLAT
                Integer32,
	    ruijie3GBackupInfo
	            INTEGER,
            ruijie3GSerialNumber
                DisplayString,
	    ruijie3GBackupIMSI
                DisplayString,
            ruijie3GGatewayIPAddr
            	IpAddress,
	    ruijie3GLineDownCause
	            Integer32,
	    ruijie3GModemType
	            INTEGER
        }
        
    ruijie3gUsername OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Username  of 3G User."
        ::= { ruijie3GEntry 1 }
   
    ruijie3GOnlineStatus OBJECT-TYPE
        SYNTAX INTEGER
            {
                lpm(0),
		online(1),
                offline(4),
		ftm(5),
		reset(6),
		rfOff(7)          
            }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "the status of 3G user. online or offline"
        ::= { ruijie3GEntry 2 }
    
    ruijie3GIMEI OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "the IMEI number of 3G user."
        ::= { ruijie3GEntry 3 }
    
    ruijie3GIPAddrType OBJECT-TYPE
         SYNTAX INTEGER {
            ipv4Addr(1),
            ipv6Addr(2)
         }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The IP Address type of remote user."
        ::= { ruijie3GEntry 4 }
    
    ruijie3GIPAddr OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The 3G remote users's IP address."
        ::= { ruijie3GEntry 5 }
    
    ruijie3GUplineTime OBJECT-TYPE
        SYNTAX TimeStamp
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The value of up line time for this 3G user."
        ::= { ruijie3GEntry 6 } 

		
        -- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.7
        ruijie3GActiveTime OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The duration of the 3G connection has been active for one user. 
				2147483647 is defined as invalid value."
			::= { ruijie3GEntry 7 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.8
		ruijie3GSignalStrength OBJECT-TYPE
			SYNTAX Integer32 (-150..0)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The signal strength of the 3G connection for this user."
			::= { ruijie3GEntry 8 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.9
		ruijie3GISP OBJECT-TYPE
			SYNTAX INTEGER
				{
				chinaUnicom(1),
				chinaTelecom(2),
				chinaMobile(3)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"the ISP of 3G networsk."
			::= { ruijie3GEntry 9 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.10
		ruijie3GSysMode OBJECT-TYPE
			SYNTAX INTEGER
				{
				noService(0),
				amps(1),
				cdma(2),
				gsmGprs(3),
				hdr(4),
				wcdma(5),
				gps(6),
				gsmCdma(7),
				cdmaHdrHybrid(8),
				tdscdma(15),
				td-1te(100),
				fdd-lte(101)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"the system mode of 3G networsk."
			::= { ruijie3GEntry 10 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.11
		ruijie3GServiceStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				noService(0),
				restricted(1),
				valid(2),
				restrictedRegional(3),
				powerSavingAndDeepSleepState(4)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"the sevice status of 3G user."
			::= { ruijie3GEntry 11 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.12
		ruijie3GRoamingStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				noRoaming(0),
				roaming(1)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"the roaming status of 3G  user."
			::= { ruijie3GEntry 12 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.13
		ruijie3GDomain OBJECT-TYPE
			SYNTAX INTEGER
				{
				noService(0),
				onlyCS(1),
				onlyPS(2),
				pSCS(3),
				pSCSnotRegistered(4),
				ePSService(100),
				cdmaNotSupport(255)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"the Domain status of 3G user ."
			::= { ruijie3GEntry 13 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.14
		ruijie3GSIMStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				invalidUsimCard(0),
				validUsimCard(1),
				invalidForCS(2),
				invalidForPS(3),
				invalidForCSPS(4),
				noUsimCard(255)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"the SIM card status of 3G user."
			::= { ruijie3GEntry 14 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.15
		ruijie3GSignalStrengthPercent OBJECT-TYPE
			SYNTAX Integer32 (0..100)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The percent of signal strength of the 3G connection for this user."
			::= { ruijie3GEntry 15 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.16
		ruijie3GApn OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The access-point  of 3G User(only used in TD-SCDMA and WCDMA)."
			::= { ruijie3GEntry 16 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.17
		ruijie3GCellID OBJECT-TYPE
			SYNTAX Integer32 (0..100)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The access Cell id  of 3G User(only used in TD-SCDMA and WCDMA)."
			::= { ruijie3GEntry 17 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.18
		ruijie3GLAC OBJECT-TYPE
			SYNTAX Integer32 (0..100)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The Location Area Code  of 3G User(only used in TD-SCDMA and WCDMA)."
			::= { ruijie3GEntry 18 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.19
		ruijie3GBSID OBJECT-TYPE
			SYNTAX Integer32 (0..100)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The Base Station ID  of 3G User(only used in CDMA2000)."
			::= { ruijie3GEntry 19 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.20
		ruijie3GNID OBJECT-TYPE
			SYNTAX Integer32 (0..100)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The Network ID  of 3G User(only used in CDMA2000)."
			::= { ruijie3GEntry 20 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.21
		ruijie3GSID OBJECT-TYPE
			SYNTAX Integer32 (0..100)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The System ID  of 3G User(only used in CDMA2000)."
			::= { ruijie3GEntry 21 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.22
		ruijie3GIMSI OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The IMSI  of 3G User."
			::= { ruijie3GEntry 22 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.23
		ruijie3GESN OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The ESN  of 3G User(only used in CDMA2000)."
			::= { ruijie3GEntry 23 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.24
		ruijie3GPhoneNumber OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The Phone Number of 3G User."
			::= { ruijie3GEntry 24 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.25
		ruijie3GifIndex OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The Index of 3G Interface."
			::= { ruijie3GEntry 25 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.26
		ruijie3GBSLONG OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The longitude of 3G User(only used in CDMA2000)."
			::= { ruijie3GEntry 26 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.27
		ruijie3GBSLAT OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The latitude of 3G User(only used in CDMA2000)."
			::= { ruijie3GEntry 27 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.28
		ruijie3GBackupInfo OBJECT-TYPE
			SYNTAX INTEGER
				{
				no-backup(0),
				myself(1),
				master(2),
				slave(3)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The backup info of 3G line."
			::= { ruijie3GEntry 28 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.29
		ruijie3GSerialNumber OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The serial number of 3G router."
			::= { ruijie3GEntry 29 }

		
		-- 1.3.6.1.4.1.4881.1.1.10.2.95.1.1.1.30
		ruijie3GBackupIMSI OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
