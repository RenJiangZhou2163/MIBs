    cItpXuaAspDAUDsRcvd Counter,
    cItpXuaAspDAUDsSent Counter,
    cItpXuaAspSCON0sRcvd Counter,
    cItpXuaAspSCON1sRcvd Counter,
    cItpXuaAspSCON2sRcvd Counter,
    cItpXuaAspSCON3sRcvd Counter,
    cItpXuaAspSCON0sSent Counter,
    cItpXuaAspSCON1sSent Counter,
    cItpXuaAspSCON2sSent Counter,
    cItpXuaAspSCON3sSent Counter,
    cItpXuaAspBytesFromAsp Counter,
    cItpXuaAspBytesToAsp Counter,
    cItpXuaAspBytesFromMtp3 Counter,
    cItpXuaAspBytesToMtp3 Counter
    }

cItpXuaAspPktsFromAsp OBJECT-TYPE
    SYNTAX Counter
--  Units
--    MSUs
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The number of MSUs received from this ASP."
    ::= { cItpXuaAspStatsTableEntry 1 }

cItpXuaAspPktsToAsp OBJECT-TYPE
    SYNTAX Counter
--  Units
--    MSUs
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The number of MSUs sent to this ASP."
    ::= { cItpXuaAspStatsTableEntry 2 }

cItpXuaAspPktsFromMtp3 OBJECT-TYPE
    SYNTAX Counter
