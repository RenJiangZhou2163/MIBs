srpTrapRingWrapped TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE srpTrapsNotificationsPrefix
    VARIABLES {
        ifIndex, srpIpsState, srpMACIpsAutoDetectMode, 
        srpMACIpsRemoteMode, srpMACIpsRemoteType, 
        srpMACIpsActiveMode, srpMACIpsWrapCounter, 
        srpMACIpsLastWrapTimeStamp, srpMACIpsLastUnWrapTimeStamp }
--  Status
--    mandatory
    DESCRIPTION
        "An srpTrapRingWrapped notification is sent
        when the value of an instance of
        srpMACIpsWrapCounter increments."
    ::= 1

srpTrapRingRestored TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE srpTrapsNotificationsPrefix
    VARIABLES {
        ifIndex, srpIpsState, srpMACIpsAutoDetectMode, 
        srpMACIpsRemoteMode, srpMACIpsRemoteType, 
        srpMACIpsActiveMode, srpMACIpsWrapCounter, 
        srpMACIpsLastWrapTimeStamp, srpMACIpsLastUnWrapTimeStamp }
--  Status
--    mandatory
    DESCRIPTION
        "An srpTrapRingRestored notification is sent
        when the value of an instance of
        rpMACIpsLastUnWrapTimeStamp is changed."
    ::= 2

