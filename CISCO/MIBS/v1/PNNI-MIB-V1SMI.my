    ::= { pnniSummaryAddressTable 1 }

PnniSummaryAddressEntry ::= SEQUENCE {
    pnniSummaryAddressType INTEGER,
    pnniSummaryAddressAddress AtmAddrPrefix,
    pnniSummaryAddressPrefixLength PnniPrefixLength,
