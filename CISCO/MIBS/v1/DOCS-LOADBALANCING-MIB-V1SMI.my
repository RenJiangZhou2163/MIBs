--    mandatory
--  Descr
--    A collection of objects providing policies.
--  objects
--    docsLoadBalPolicyRulePtr, docsLoadBalPolicyRowStatus

docsLoadBalBasicRuleGroup OBJECT IDENTIFIER ::= { docsLoadBalGroups 4 }
-- OBJECT-GROUP
--  Status
