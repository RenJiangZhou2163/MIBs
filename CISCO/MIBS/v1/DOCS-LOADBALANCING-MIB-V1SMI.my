--    docsLoadBalChnPairsRowStatus, docsLoadBalRestrictCmMACAddr, 
--    docsLoadBalRestrictCmMacAddrMask, docsLoadBalRestrictCmStatus

docsLoadBalPoliciesGroup OBJECT IDENTIFIER ::= { docsLoadBalGroups 3 }
-- OBJECT-GROUP
