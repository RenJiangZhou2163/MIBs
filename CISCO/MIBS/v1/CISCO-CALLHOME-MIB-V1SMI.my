ccmAlertGroupSeverity OBJECT-TYPE
    SYNTAX CallHomeMsgLevel
--    Rsyntax INTEGER {
--        debug(1),
--        normal(2),
--        notification(3),
--        warning(4),
--        minor(5),
--        major(6),
--        critical(7),
--        fatal(8),
--        disaster(9),
--        catastrophic(10)
--        }
