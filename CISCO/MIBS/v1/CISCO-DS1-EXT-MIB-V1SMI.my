    cds1AlarmThresholdGroupIndex INTEGER,
    cds1Current15MinBESsThreshold INTEGER,
    cds1Current24HrBESsThreshold INTEGER,
    cds1Current15MinCSSsThreshold INTEGER,
    cds1Current24HrCSSsThreshold INTEGER,
    cds1Current15MinDMsThreshold INTEGER,
    cds1Current24HrDMsThreshold INTEGER,
    cds1Current15MinESsThreshold INTEGER,
    cds1Current24HrESsThreshold INTEGER,
    cds1Current15MinLCVsThreshold INTEGER,
    cds1Current24HrLCVsThreshold INTEGER,
    cds1Current15MinLESsThreshold INTEGER,
    cds1Current24HrLESsThreshold INTEGER,
    cds1Current15MinLSESsThreshold INTEGER,
    cds1Current24HrLSESsThreshold INTEGER,
    cds1Current15MinPCVsThreshold INTEGER,
    cds1Current24HrPCVsThreshold INTEGER,
    cds1Current15MinPSASsThreshold INTEGER,
    cds1Current24HrPSASsThreshold INTEGER,
    cds1Current15MinSESsThreshold INTEGER,
    cds1Current24HrSESsThreshold INTEGER,
    cds1Current15MinSEFSsThreshold INTEGER,
    cds1Current24HrSEFSsThreshold INTEGER,
    cds1Current15MinUASsThreshold INTEGER,
    cds1Current24HrUASsThreshold INTEGER,
    cds1AlarmThresholdGroupRowStatus RowStatus
