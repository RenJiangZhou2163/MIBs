    fcmPortClass2TxFbsyFrames Counter,
    fcmPortClass2TxPbsyFrames Counter,
    fcmPortClass2TxFrjtFrames Counter,
    fcmPortClass2TxPrjtFrames Counter,
    fcmPortClass3RxFrames Counter,
    fcmPortClass3RxOctets Counter,
    fcmPortClass3TxFrames Counter,
    fcmPortClass3TxOctets Counter,
    fcmPortClass3Discards Counter,
    fcmPortClassFRxFrames Counter,
    fcmPortClassFRxOctets Counter,
    fcmPortClassFTxFrames Counter,
    fcmPortClassFTxOctets Counter,
    fcmPortClassFDiscards Counter
    }

