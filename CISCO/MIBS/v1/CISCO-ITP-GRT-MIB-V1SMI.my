    ::= { cgrtDestEntry 10 }

cgrtDestRestrictedMSUs OBJECT-TYPE
    SYNTAX Counter
--  Units
--    MSUs
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The number of MSUs transferred for this destination 
