--  Units
--    seconds
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Duration in seconds during which forwarding was prohibited
         for this route."
    ::= { cgrtRouteEntry 14 }

cgrtRouteDisplay OBJECT-TYPE
    SYNTAX CItpTcDisplayPC
--    Rsyntax OCTET STRING(SIZE(1..12))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The destination point code associated with the route in
