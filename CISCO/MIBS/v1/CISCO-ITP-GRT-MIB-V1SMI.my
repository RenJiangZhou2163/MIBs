    ::= { cgrtDestSITable 1 }

CgrtDestSIEntry ::= SEQUENCE {
    cgrtMtp3SI CItpTcServiceIndicator,
    cgrtDestSIMSUsOut Counter,
    cgrtDestSIOctetsOut Counter,
    cgrtDestSIMSUsIn Counter,
    cgrtDestSIOctetsIn Counter,
    cgrtDestSIDisplay CgrtDisplayPCSI
    }

