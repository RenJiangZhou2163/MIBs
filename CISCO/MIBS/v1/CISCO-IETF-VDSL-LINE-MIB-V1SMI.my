cvdslGroups     OBJECT IDENTIFIER ::= { cvdslConformance 1 }
cvdslCompliances OBJECT IDENTIFIER ::= { cvdslConformance 2 }

CVdslLineCodingType ::= INTEGER {
        other(1),
        mcm(2),
        scm(3)
        }
