cGtpPathStatisticsHistoryTable OBJECT-TYPE
    SYNTAX SEQUENCE OF CGtpPathStatisticsHistEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "This table indicates historical information for GTP paths
        which have been deleted."
    ::= { cGtpStatistics 5 }

cGtpPathStatisticsHistEntry OBJECT-TYPE
    SYNTAX CGtpPathStatisticsHistEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "An entry in this table.
        
        Creation of row(s) will happen when a PATH is removed.
        
        Deletion of row(s) will happen when the PATH entry limit,
        given by the object cGtpPathHistoryLimit, is reached.
        The oldest paths will be removed once the limit is
        reached."
    INDEX { cGtpHistIndex, cGtpHistRemoteAddrType, cGtpHistRemoteAddress, cGtpHistRemotePort }
    ::= { cGtpPathStatisticsHistoryTable 1 }

CGtpPathStatisticsHistEntry ::= SEQUENCE {
    cGtpHistIndex CGtpMaxHistNumber,
    cGtpHistRemoteAddrType InetAddressType,
    cGtpHistRemoteAddress InetAddress,
    cGtpHistRemotePort InetPortNumber,
    cGtpHistUnexpectedMsgs Counter,
    cGtpHistDroppedDataMsgs Counter,
    cGtpHistRcvdPDUMsgs Counter,
    cGtpHistSentPDUMsgs Counter,
    cGtpHistRcvdPDUOctets Counter,
    cGtpHistSentPDUOctets Counter,
    cGtpHistMsgTooShort Counter,
    cGtpHistUnknownMsgs Counter,
    cGtpHistUnexpectedSigMsgs Counter,
    cGtpHistRoamingPDUs Counter,
    cGtpHistSecurityViolations Counter,
    cGtpHistUnsupportedExtHdr Counter,
    cGtpHistPathFailures Counter,
    cGtpHistTotalDropped Counter,
    cGtpHistDroppedSigMsgs Counter,
    cGtpHistRcvdSigMsgs Counter,
    cGtpHistSentSigMsgs Counter,
    cGtpHistTotalCreatedPDPs Counter,
    cGtpHistTotalDeletedPDPs Counter,
    cGtpHistTotalCreatedPppPDPs Counter,
    cGtpHistTotalDeletedPppPDPs Counter,
    cGtpHistSinglePDPSessCleared Counter,
    cGtpHistPathFailLocalDelPDPs Counter,
    cGtpHistVerUpgradeLocalDel Counter,
    cGtpHistNoSgsnLocalDelPDPs Counter,
    cGtpHistVerFallbackLocalDel Counter,
    cGtpHistCreateCollideWithDel Counter,
    cGtpHistVersionChanges Counter,
    cGtpHistRetransCreatePDPReqs Counter,
    cGtpHistCreateAsUpdate Counter,
    cGtpHistIpv6PdpActRejects Counter,
    cGtpHistIpv6CreatedPDPs Counter,
    cGtpHistIpv6DeletedPDPs Counter,
    cGtpHistIpv6RcvdSigMsgs Counter,
    cGtpHistIpv6SentSigMsgs Counter,
    cGtpHistIpv6RcvdPDUs Counter,
    cGtpHistIpv6SentPDUs Counter,
    cGtpHistIpv6RcvdOctets Counter,
    cGtpHistIpv6SentOctets Counter,
    cGtpHistNoWaitSgsnLocalDelPDPs Counter,
    cGtpHistNoReqSgsnLocalDelPDPs Counter,
    cGtpHistRoamingTrustedPDPs Counter,
    cGtpHistRoamingNonTrustedPDPs Counter,
    cGtpHistNonRoamingPDPs Counter,
    cGtpHistSentPdpUpdateReqs Counter,
    cGtpHistRcvdPdpUpdateResponses Counter,
    cGtpHistTotalDtEnabled Counter,
    cGtpHistRemoteNode CGtpEntities,
    cGtpHistIpv4v6CreatedPDPs Counter,
    cGtpHistIpv4v6DeletedPDPs Counter
    }

