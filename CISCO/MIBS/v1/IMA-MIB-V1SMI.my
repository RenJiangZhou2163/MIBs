imaAlarmType OBJECT-TYPE
    SYNTAX ImaAlarmType
--    Rsyntax INTEGER {
--        imaAlarmLinkLif(1),
--        imaAlarmLinkLods(2),
--        imaAlarmLinkRfi(3),
--        imaAlarmLinkTxMisConnect(4),
--        imaAlarmLinkRxMisConnect(5),
--        imaAlarmLinkTxFault(6),
--        imaAlarmLinkRxFault(7),
--        imaAlarmLinkTxUnusableFe(8),
