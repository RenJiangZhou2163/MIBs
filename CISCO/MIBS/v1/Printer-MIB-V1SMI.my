prtMarkerCounterUnit OBJECT-TYPE
    SYNTAX PrtMarkerCounterUnitTC
--    Rsyntax INTEGER {
--        tenThousandthsOfInches(3),
--        micrometers(4),
--        characters(5),
--        lines(6),
--        impressions(7),
