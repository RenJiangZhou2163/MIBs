--        unknown(2),
--        removableBin(3),
--        unRemovableBin(4),
--        continuousRollDevice(5),
--        mailBox(6),
--        continuousFanFold(7)
--        }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The type of technology supported by this output sub-unit."
    ::= { prtOutputEntry 2 }

prtOutputCapacityUnit OBJECT-TYPE
    SYNTAX PrtCapacityUnitTC
