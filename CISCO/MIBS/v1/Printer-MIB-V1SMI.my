--        unknown(2),
--        tenThousandthsOfInches(3),
--        micrometers(4),
--        sheets(8),
--        feet(16),
--        meters(17),
--        items(18),
--        percent(19)
--        }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
