--******************************************************
--    he follow part defined some data types which will 
--    be used in this mib file later  
--****************************************************** 
MacAddress ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION  "The data type of mac address"
    SYNTAX       OCTET STRING (SIZE (6))
    
BitmapDscp  ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION  "The data type of Dscp"
    SYNTAX       OCTET STRING (SIZE(0..65535))

BitmapMplsexp  ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION  "The data type of MplsExp"
    SYNTAX       OCTET STRING (SIZE(0..65535))

BitmapPrec  ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION  "The data type of precedence"
    SYNTAX       OCTET STRING (SIZE(0..65535))

BitmapVlan  ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION  "The data type of vlan"
    SYNTAX       OCTET STRING (SIZE(0..65535))

Bitmap8021p  ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION  "The data type of 8021p"
    SYNTAX       OCTET STRING (SIZE(0..65535))

QosMatchSrvClass ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
            "server class of PHB info ,see below:
             be  Best effort
             af1 Assured forwarding class 1
             af2 Assured forwarding class 2
             af3 Assured forwarding class 3
             af4 Assured forwarding class 4
             ef  Expedited forward
             cs6 Internetwork control service class
             cs7 Network control service class"
    SYNTAX   INTEGER {be(1),af1(2),af2(3),af3(4),
                      af4(5),ef(6),cs6(7),cs7(8)}

QosMatchDscpExSrvClass ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
            "resserved words of DSCP info ,see below:
            af11    Match packets with AF11 dscp (001010)                                
            af12    Match packets with AF12 dscp (001100)                                
            af13    Match packets with AF13 dscp (001110)                                
            af21    Match packets with AF21 dscp (010010)                                
            af22    Match packets with AF22 dscp (010100)                                
            af23    Match packets with AF23 dscp (010110)                                
            af31    Match packets with AF31 dscp (011010)                                
            af32    Match packets with AF32 dscp (011100)                                
            af33    Match packets with AF33 dscp (011110)                                
            af41    Match packets with AF41 dscp (100010)                                
            af42    Match packets with AF42 dscp (100100)                                
            af43    Match packets with AF43 dscp (100110)                                
            cs1     Match packets with CS1(precedence 1) dscp (001000)                   
            cs2     Match packets with CS2(precedence 2) dscp (010000)                   
            cs3     Match packets with CS3(precedence 3) dscp (011000)                   
            cs4     Match packets with CS4(precedence 4) dscp (100000)                   
            cs5     Match packets with CS5(precedence 5) dscp (101000)                   
            cs6     Match packets with CS6(precedence 6) dscp (110000)                   
            cs7     Match packets with CS7(precedence 7) dscp (111000)                   
            default Match packets with default dscp (000000)                             
            ef      Match packets with EF dscp (101110)"
    SYNTAX   INTEGER {af11(10),af12(12),af13(14),af21(18),af22(20),af23(22),
                      af31(26),af32(28),af33(30),af41(34),af42(36),af43(38),
                      cs1(8),cs2(16),cs3(24),cs4(32),cs5(40),cs6(48),
                      cs7(56),default(0),ef(46)}

--*****************************************************************
--*****************************************************************
--zClassMapTable  
--*****************************************************************
zClassMapTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF ZClassMapEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
                "Class Map table"
        ::= { zClassMapObjects 1  }

zClassMapEntry OBJECT-TYPE
        SYNTAX   ZClassMapEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
                "H-QoS class map"
        INDEX {zClassMapStrcmapname }
        ::= { zClassMapTable 1 }

ZClassMapEntry ::=
        SEQUENCE {
                zClassMapStrcmapname                             
                    DisplayString,
                zClassMapUmatchrelattype                         
                    Unsigned32,
                zClassMapUiptype                                  
                    Unsigned32,
                zClassMapMatchtype                                
                    Unsigned32,
                zClassMapModeType
                    Unsigned32
        }

zClassMapStrcmapname OBJECT-TYPE
        SYNTAX   DisplayString(SIZE(0..31))
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
                "WORD Name of the class map"            
        ::= { zClassMapEntry  1  }

zClassMapUmatchrelattype OBJECT-TYPE
        SYNTAX   Unsigned32(0..2)
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
                "match-all Packets meet all of the match criteria
                 match-any Packets meet any of the match criteria"
        ::= { zClassMapEntry  2  }

zClassMapUiptype OBJECT-TYPE
        SYNTAX   Unsigned32(0..2)
        MAX-ACCESS  read-only
        STATUS  obsolete
        DESCRIPTION
                "ipv4 Packets based on IPv4
                 ipv6 Packets based on IPv6"
        ::= { zClassMapEntry  3  }
                                
zClassMapMatchtype OBJECT-TYPE
        SYNTAX   Unsigned32(0..4294967295)
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
                "The flag of configruing match type:
                bit0: dscp
                bit1: mplsexp
                bit2: precedence
                bit3: invlan
                bit4: outvlan
                bit5: in8021p
                bit6: out8021p
                bit7: interface
                bit8: qosgroup
                bit9: vrf
                bit10: unicast
                bit11: multicast
                bit12: mac address
                bit13: child
                bit14: ipv4 acl
                bit15: ipv6 acl
                bit16: vlss
                bit17: vpws
                bit18: vpls
                bit19: dpi
                bit20: serv class
                bit21: link acl
                bit22: dscp ex"
        ::= { zClassMapEntry  4  }
        
zClassMapModeType  OBJECT-TYPE
        SYNTAX   Unsigned32(0..3)
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION 
                  "The mode type of class-map.
                  0  Class map based on normal
                  1  Class map based on phb
                  2  Class map based on sub-interface
                  3  Class map based on switch-fabric"
        ::= { zClassMapEntry  5  }

