-- *******************************************************
--                Trace Control Table
-- *******************************************************

    zxr10TraceRouteCtlTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Zxr10TraceRouteCtlEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Defines the Remote Operations Traceroute Control Table for
            providing the capability of invoking traceroute from a remote
            host.  The results of traceroute operations can be stored in
            the zxr10TraceRouteResultsTable, and the zxr10TraceRouteHopsTable."
        ::= { zxr10TraceRouteObjects 1 }

    zxr10TraceRouteCtlEntry OBJECT-TYPE
        SYNTAX      Zxr10TraceRouteCtlEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Defines an entry in the zxr10TraceRouteCtlTable.  The index
            element, zxr10TraceRouteCtlSerial, is of type Integer32, limited
            to 100 entries."
       INDEX {
               zxr10TraceRouteCtlSerial
             }
       ::= { zxr10TraceRouteCtlTable 1 }

    Zxr10TraceRouteCtlEntry ::=
        SEQUENCE {
            zxr10TraceRouteCtlSerial        Integer32,
            zxr10TraceRouteCtlTraceType     INTEGER,
            zxr10TraceRouteCtlIpAddrType    InetAddressType,
            zxr10TraceRouteCtlTgtAddr       InetAddress,
            zxr10TraceRouteCtlDomainName    DisplayString,
            zxr10TraceRouteCtlVrfType       INTEGER,
            zxr10TraceRouteCtlVrfName       DisplayString,
            zxr10TraceRouteCtlMaxTtl        Unsigned32,
            zxr10TraceRouteCtlTimeOut       Unsigned32,
            zxr10TraceRouteCtlProbPerHop    Unsigned32,
            zxr10TraceRouteCtlSrcAddr       InetAddress,
            zxr10TraceRouteCtlGenHopEntry   TruthValue,
            zxr10TraceRouteCtlRowStatus     INTEGER,
            zxr10TraceRouteCtlDataSize      Unsigned32
       }

    zxr10TraceRouteCtlSerial OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
          "serial number"
        ::= { zxr10TraceRouteCtlEntry 1 }

     zxr10TraceRouteCtlTraceType OBJECT-TYPE
        SYNTAX      INTEGER { ip(1),domain(2) }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "The type of a traceroute destination.
             1: IP address, uses zxr10TraceRouteCtlTgtAddr
             2: domain name, uses zxr10TraceRouteCtlDomainName"
        DEFVAL      { 1 }
        ::= { zxr10TraceRouteCtlEntry 2 }

     zxr10TraceRouteCtlIpAddrType OBJECT-TYPE
        SYNTAX      InetAddressType
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "Specifies the IP address type for the traceroute
           request, including zxr10TraceRouteCtlDstAddr
           and zxr10TraceRouteCtlSrcAddr."
        ::= { zxr10TraceRouteCtlEntry 3 }

    zxr10TraceRouteCtlTgtAddr OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Specifies the host address used on the traceroute
            request at the remote host."
        ::= { zxr10TraceRouteCtlEntry 4 }

    zxr10TraceRouteCtlDomainName OBJECT-TYPE
        SYNTAX      DisplayString (SIZE(0..128))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The name of a traceroute domain. This is locally unique,
            within the scope of a zxr10TraceRouteCtlSerial."
        ::= { zxr10TraceRouteCtlEntry 5 }

    zxr10TraceRouteCtlVrfType OBJECT-TYPE
        SYNTAX      INTEGER { noneVrf(1), normalVrf(2), dcnVrf(3) }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Controls the vrf type of traceroute test. This is locally
            unique, within the scope of a zxr10TraceRouteCtlSerial.
              1: not vrf
              2: normal vrf, uses zxr10TraceRouteCtlVrfName
              3: dcn vrf, the vrf name is ?_dcn
            "
        DEFVAL { noneVrf }
        ::= { zxr10TraceRouteCtlEntry 6 }

    zxr10TraceRouteCtlVrfName OBJECT-TYPE
        SYNTAX      DisplayString (SIZE(0..32))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The name of a traceroute vrf. This is locally unique,
            within the scope of a zxr10TraceRouteCtlSerial."
        ::= { zxr10TraceRouteCtlEntry 7 }

    zxr10TraceRouteCtlMaxTtl OBJECT-TYPE
        SYNTAX      Unsigned32 (1..255)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Specifies the maximum time-to-live value."
        DEFVAL { 30 }
        ::= { zxr10TraceRouteCtlEntry 8 }

    zxr10TraceRouteCtlTimeOut OBJECT-TYPE
        SYNTAX      Unsigned32 (1..60)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Specifies the time-out value, in seconds, for
            a traceroute request."
        DEFVAL { 3 }
        ::= { zxr10TraceRouteCtlEntry 9 }

    zxr10TraceRouteCtlProbPerHop OBJECT-TYPE
       SYNTAX      Unsigned32 (1..10)
       MAX-ACCESS  read-create
       STATUS      current
       DESCRIPTION
           "Specifies the number of times to reissue a traceroute
           request with the same time-to-live (TTL) value."
       DEFVAL { 3 }
       ::= { zxr10TraceRouteCtlEntry 10 }

    zxr10TraceRouteCtlSrcAddr OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Use the specified IP address (which must be given as an
            IP number, not a hostname) as the source address in
            outgoing probe packets.  On hosts with more than one IP
            address, this option can be used to select the address
            to be used.  If the IP address is not one of this
            machine's interface addresses, an error is returned, and
            nothing is sent.  A zero-length octet string value for
            this object disables source address specification."
        ::= { zxr10TraceRouteCtlEntry 11 }

    zxr10TraceRouteCtlGenHopEntry OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The current path for a traceroute test is kept in the
            zxr10TraceRouteHopsTable on a per-hop basis when the value
            of this object is true(1)."
        DEFVAL { false }
        ::= { zxr10TraceRouteCtlEntry 12 }

    zxr10TraceRouteCtlRowStatus OBJECT-TYPE
        SYNTAX      INTEGER {
                        notActive(1),
                        startTrace(2),
                        traceProcessing(3),
                        traceCompleted(4),
                        destroy(5)
                    }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This object allows to start tracing or destroy entries
            in the zxr10TraceRouteCtlTable."
        ::= { zxr10TraceRouteCtlEntry 13 }

    zxr10TraceRouteCtlDataSize OBJECT-TYPE
       SYNTAX      Unsigned32 (36..8192)
       MAX-ACCESS  read-create
       STATUS      current
       DESCRIPTION
           "Specifies the size of the data portion of a traceroute
           request, in octets.
           The value for this object includes IP header size and 
           UDP header size."
       DEFVAL { 100 }
       ::= { zxr10TraceRouteCtlEntry 14 }

-- *******************************************************
--                Trace Result Table
-- *******************************************************

    zxr10TraceRouteResultsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Zxr10TraceRouteResultsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Defines the Remote Operations Traceroute Results Table for
            keeping track of the status of a zxr10TraceRouteCtlEntry.

            An entry is added to the zxr10TraceRouteResultsTable when an
            zxr10TraceRouteCtlEntry is started.

            An entry is removed from the zxr10TraceRouteResultsTable when
            its corresponding zxr10TraceRouteCtlEntry is deleted."
        ::= { zxr10TraceRouteObjects 2 }

    zxr10TraceRouteResultsEntry OBJECT-TYPE
        SYNTAX      Zxr10TraceRouteResultsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Defines an entry in the zxr10TraceRouteResultsTable.  The
            zxr10TraceRouteResultsTable has the same indexing as the
            zxr10TraceRouteCtlTable."
        INDEX {
                zxr10TraceRouteCtlSerial
              }
        ::= { zxr10TraceRouteResultsTable 1 }

    Zxr10TraceRouteResultsEntry ::=
        SEQUENCE {
          zxr10TraceRouteResCurHopCount      Unsigned32,
          zxr10TraceRouteResCurProbCount     Unsigned32,
          zxr10TraceRouteResIpTgtAddrType    InetAddressType,
          zxr10TraceRouteResIpTgtAddr        InetAddress,
          zxr10TraceRouteResTestAttempts     Unsigned32,
          zxr10TraceRouteResTestSuccesses    Unsigned32,
          zxr10TraceRouteResLastGoodPath     DateAndTime
        }

    zxr10TraceRouteResCurHopCount OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Reflects the current TTL value (from 1 to
            255) for a remote traceroute operation.
            Maximum TTL value is determined by
            zxr10TraceRouteCtlMaxTtl."
        ::= { zxr10TraceRouteResultsEntry 1 }

    zxr10TraceRouteResCurProbCount OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Reflects the current probe count (1..10) for
            a remote traceroute operation.  The maximum
            probe count is determined by
            zxr10TraceRouteCtlProbPerHop."
        ::= { zxr10TraceRouteResultsEntry 2 }

    zxr10TraceRouteResIpTgtAddrType OBJECT-TYPE
        SYNTAX      InetAddressType

        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This object indicates the type of address stored
            in the corresponding zxr10TraceRouteResIpTgtAddr
            object."
        ::= { zxr10TraceRouteResultsEntry 3 }

    zxr10TraceRouteResIpTgtAddr OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This object reports the IP address associated
            with a zxr10TraceRouteCtlTgtAddr value when the
            destination address is specified as a DNS name.
            The value of this object should be a zero-length
            octet string when a DNS name is not specified or
            when a specified DNS name fails to resolve."
        ::= { zxr10TraceRouteResultsEntry 4 }

    zxr10TraceRouteResTestAttempts OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The current number of attempts to determine a path
            to a target.  The value of this object MUST be started
            at 0."
        ::= { zxr10TraceRouteResultsEntry 5 }

    zxr10TraceRouteResTestSuccesses OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The current number of attempts to determine a path
            to a target that have succeeded.  The value of this
            object MUST be reported as 0 when no attempts have
            succeeded."
        ::= { zxr10TraceRouteResultsEntry 6 }

    zxr10TraceRouteResLastGoodPath OBJECT-TYPE
        SYNTAX      DateAndTime
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The date and time when the last complete path
            was determined.  A path is complete if responses
            were received or timeout occurred for each hop on
            the path."
        ::= { zxr10TraceRouteResultsEntry 7 }

-- *******************************************************
--                Trace Hop Result Table
-- *******************************************************

    zxr10TraceRouteHopsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Zxr10TraceRouteHopsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Defines the Remote Operations Traceroute Hop Table for
            keeping track of the results of traceroute tests on a
            per-hop basis."
        ::= { zxr10TraceRouteObjects 4 }

    zxr10TraceRouteHopsEntry OBJECT-TYPE
        SYNTAX      Zxr10TraceRouteHopsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Defines an entry in the zxr10TraceRouteHopsTable.
            The index elements identify the zxr10TraceRouteCtlEntry
            that a zxr10TraceRouteHopsEntry belongs to."
        INDEX {
                zxr10TraceRouteCtlSerial,
                zxr10TraceRouteHopsIndex
              }

        ::= { zxr10TraceRouteHopsTable 1 }

    Zxr10TraceRouteHopsEntry ::=
        SEQUENCE {
            zxr10TraceRouteHopsIndex            Integer32,
            zxr10TraceRouteHopsIpTgtAddrType    InetAddressType,
            zxr10TraceRouteHopsIpTgtAddr        InetAddress,
            zxr10TraceRouteHopsMinRtt           Unsigned32,
            zxr10TraceRouteHopsMaxRtt           Unsigned32,
            zxr10TraceRouteHopsAverageRtt       Unsigned32,
            zxr10TraceRouteHopsRttSumOfSquar    Unsigned32,
            zxr10TraceRouteHopsSentProbs        Unsigned32,
            zxr10TraceRouteHopsProbResponses    Unsigned32,
            zxr10TraceRouteHopsLastGoodProb     DateAndTime
          }

    zxr10TraceRouteHopsIndex OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
           "Specifies the hop index for a traceroute hop."
        ::= { zxr10TraceRouteHopsEntry 1 }

    zxr10TraceRouteHopsIpTgtAddrType OBJECT-TYPE
        SYNTAX      InetAddressType
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This object indicates the type of address stored
            in the corresponding zxr10TraceRouteHopsIpTgtAddr
            object."
        ::= { zxr10TraceRouteHopsEntry 2 }

    zxr10TraceRouteHopsIpTgtAddr OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This object reports the IP address associated with
            the hop.  A value for this object should be reported
            as a numeric IP address, not as a DNS name."
        ::= { zxr10TraceRouteHopsEntry 3 }

    zxr10TraceRouteHopsMinRtt OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The minimum traceroute round-trip-time (RTT) received for
            this hop.  A value of 0 for this object implies that no
            RTT has been received."
        ::= { zxr10TraceRouteHopsEntry 4 }

    zxr10TraceRouteHopsMaxRtt OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The maximum traceroute round-trip-time (RTT) received for
            this hop.  A value of 0 for this object implies that no
            RTT has been received."
        ::= { zxr10TraceRouteHopsEntry 5 }

    zxr10TraceRouteHopsAverageRtt OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The current average traceroute round-trip-time (RTT) for
            this hop."
        ::= { zxr10TraceRouteHopsEntry 6 }

    zxr10TraceRouteHopsRttSumOfSquar OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This object contains the sum of the squares of all
            round-trip-times received for this hop.  Its purpose is
            to enable standard deviation calculation."
        ::= { zxr10TraceRouteHopsEntry 7 }

    zxr10TraceRouteHopsSentProbs OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The value of this object reflects the number of probes sent
            for this hop during this traceroute test.  The value of this
            object should start at 0."
        ::= { zxr10TraceRouteHopsEntry 8 }

    zxr10TraceRouteHopsProbResponses OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Number of responses received for this hop during this
            traceroute test.  This value of this object should start
            at 0."
        ::= { zxr10TraceRouteHopsEntry 9 }

    zxr10TraceRouteHopsLastGoodProb OBJECT-TYPE
        SYNTAX      DateAndTime
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Date and time at which the last response was received for a
             probe for this hop during this traceroute test."
        ::= { zxr10TraceRouteHopsEntry 10 }

END

