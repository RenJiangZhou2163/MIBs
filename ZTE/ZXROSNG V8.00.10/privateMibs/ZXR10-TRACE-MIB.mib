ZXR10-TRACE-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE,
        Integer32,Unsigned32,IpAddress,
        enterprises,NOTIFICATION-TYPE
            FROM SNMPv2-SMI
        DisplayString,TruthValue,DateAndTime,
        TEXTUAL-CONVENTION
            FROM SNMPv2-TC
        InetAddressType, InetAddress
            FROM INET-ADDRESS-MIB
        Ipv6Address
            FROM IPV6-TC;

    trace MODULE-IDENTITY
        LAST-UPDATED "201408251939Z"
        ORGANIZATION "ZTE Corporation."
        CONTACT-INFO
            "       Zhao yanhua
            Tel:    +86-25-52870000
            E-Mail: zhao.yanhua3@zte.com.cn
            "
        DESCRIPTION
            "ZTE TRACE MIB
            "
        REVISION "201408251939Z"
        DESCRIPTION
            "Add     MactraceHopsResult;
             update  DESCRIPTION of zxr10LspTraceCtlTable;
                     DESCRIPTION of zxr10LspTraceCtlEntry;
                     DESCRIPTION of zxr10LspTraceResultsTabl;
                     DESCRIPTION of zxr10LspTraceHopsTable;
                     DESCRIPTION of zxr10MacTraceCtlTable;
                     DESCRIPTION of zxr10MacTraceHopsTable;
                     DESCRIPTION of zxr10TraceRouteCtlVrfType;
                     DESCRIPTION of lspTraceTrapResult;
                     DESCRIPTION of lspTraceTrapHops;
                     DESCRIPTION of macTraceTrapResult;
                     Zxr10MacTraceHopsEntry;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop0;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop1;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop2;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop3;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop4;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop5;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop6;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop7;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop8;
                     DESCRIPTION and SYNTAX of zxr10MacTraceHopsResultValidHop9.
            "
        REVISION "201403071004Z"
        DESCRIPTION
            "Add     zxr10TraceRouteCtlDataSize
            "
        REVISION "201309051924Z"
        DESCRIPTION
            "Add     zxr10LspTraceCtlReplyMode for pw ping ttl tlv;
                     zxr10LspTraceHopsV6Addr for ipv6 trace;
                     zxr10LspTraceHopsV6Addr in lspTraceTrapHops 
                     for ipv6 trace;
                     zxr10LspTraceHopsResultValid in lspTraceTrapHops 
                     because it's omitted before
            "
        REVISION "201308080930Z"
        DESCRIPTION
            "Add description for zxr10MacTraceHopsEntryOwner
            "
        REVISION "201305310930Z"
        DESCRIPTION
            "update  zxr10LspTraceCtlPwDestAddr to obsolete
                     zxr10LspTraceCtlPwVcId to obsolete
                     zxr10LspTraceCtlPwType to obsolete
                     zxr10LspTraceCtlTraceType add v6ldp(3)
                     zxr10LspTraceCtlLspType add tunnel(3)
             fix comling error and update descriptions.
            "
            
        REVISION "201303191130Z"
        DESCRIPTION
            "Add trace MIB (zte.zxr10.trace.zxr10TraceRoute)
             Add    zxr10LspTraceCtlMultiSegment
                    zxr10LspTraceCtlLdpV6Prefix
                    zxr10LspTraceCtlLdpV6PrefixLen
                    zxr10LspTraceCtlOutInterface
                    zxr10LspTraceCtlLspType
                    zxr10LspTraceCtlSrcIpAddr
                    zxr10LspTraceCtlV6SrcIpAddr
                    zxr10LspTraceCtlDownstreamMapType
             fix comling error and update descriptions.
            "
        REVISION "200811240000Z"
        DESCRIPTION
            "Initial version of this MIB module.
            "
        ::= { zxr10 318 }

    zxr10LspTrace OBJECT IDENTIFIER ::= { trace 1 }

    zxr10MacTrace OBJECT IDENTIFIER ::= { trace 2 }

    zxr10TraceRoute OBJECT IDENTIFIER    ::= { trace 3 }

    lspTraceNotifications OBJECT IDENTIFIER ::= { zxr10LspTrace 4 }

    macTraceNotifications OBJECT IDENTIFIER ::= { zxr10MacTrace 4 }

    zxr10TraceRouteObjects OBJECT IDENTIFIER ::= { zxr10TraceRoute 1 }

    zte OBJECT IDENTIFIER ::= { enterprises 3902 }

    zxr10 OBJECT IDENTIFIER ::= { zte 3 }

    LspTraceType ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION
            "lsp trace type"
        SYNTAX      INTEGER {
                        ldp(0),
                        rsvpTe(1),
                        vccv(2),
                        v6ldp(3)
                    }

    LspTraceIfOption ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION
            "lsp trace option"
        SYNTAX      INTEGER {
                        none(0),
                        option(1)
                    }

    LspTracePwType ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION
            "lsp trace pw type"
        SYNTAX      INTEGER {
                        etherVlan(0),
                        ether(1)
                    }    


    TraceRosStatus ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION
            "The type used to crontrol tracing"
        SYNTAX      INTEGER {
                        notActive(1),
                        startTrace(2),
                        traceProcessing(3),
                        traceCompleted(4)
                    }

    TraceHopsResult ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION
            "The type of trace hop result"
        SYNTAX      INTEGER {
                        valid(0),
                        success(1),
                        downstreamRouterButNotTarget(2),
                        requestNotTransmitted(3),
                        timeout(4),
                        unreachable(5)
                    }
    
    MactraceHopsResult ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION
            "The type of mac trace hop result"
        SYNTAX      INTEGER {
                        valid(0),
                        success(1)
                    }

    HopType ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION
            "Hop type"
        SYNTAX      INTEGER {
                        ingress(0),
                        downstream(1),
                        egress(2)
                    }

    MacTraceIfPeOption ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION
            "if PE option of mac trace"
        SYNTAX      INTEGER {
                        ce(0),
                        pe(1)
                    }

--  ******************************************************
--                Lsp Trace Control Table
--  ******************************************************
    zxr10LspTraceCtlTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10LspTraceCtlEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of lsp trace information."
               ::= { zxr10LspTrace 1 }

    zxr10LspTraceCtlEntry  OBJECT-TYPE
              SYNTAX  Zxr10LspTraceCtlEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "lsp trace control entry"
              INDEX  {
                        zxr10LspTraceCtlSerial
                     }
              ::= { zxr10LspTraceCtlTable 1 }

    Zxr10LspTraceCtlEntry ::=
              SEQUENCE {
                zxr10LspTraceCtlSerial
                    Integer32,
                zxr10LspTraceCtlTraceType
                    LspTraceType,
                zxr10LspTraceCtlLdpPrefix
                    IpAddress,
                zxr10LspTraceCtlLdpPrefixLength
                    Integer32,
                zxr10LspTraceCtlTeTunnelIfName
                    DisplayString,
                zxr10LspTraceCtlPwDestAddr
                    IpAddress,
                zxr10LspTraceCtlPwVcId
                    Unsigned32,
--                zxr10LspTraceCtlPwTTL 
--                    Integer32,
                zxr10LspTraceCtlIfOption
                    LspTraceIfOption,
                zxr10LspTraceCtlTimeOut
                    Integer32,
                zxr10LspTraceCtlTTL
                    Integer32,
                zxr10LspTraceCtlTrapOncompletion
                    TruthValue,
                zxr10LspTraceRosStatus
                    TraceRosStatus,
                zxr10LspTraceEntryOwner
                    DisplayString,
                zxr10LspTraceCtlPwType
                    LspTracePwType,
                zxr10LspTraceCtlPwName
                    DisplayString,
                zxr10LspTraceCtlMultiSegment
                    TruthValue,
                zxr10LspTraceCtlV6LdpPrefix
                    Ipv6Address,
                zxr10LspTraceCtlV6LdpPrefixLen
                    Integer32,
                zxr10LspTraceCtlOutInterface
                    DisplayString,
                zxr10LspTraceCtlLspType
                    INTEGER,
                zxr10LspTraceCtlSrcIpAddr
                    IpAddress,
                zxr10LspTraceCtlV6SrcIpAddr
                    Ipv6Address,
                zxr10LspTraceCtlDownstrMapType
                    INTEGER,
                zxr10LspTraceCtlReplyMode
                    INTEGER
              }

    zxr10LspTraceCtlSerial OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                  "lsp trace control serial number"
              ::= { zxr10LspTraceCtlEntry 1 }

    zxr10LspTraceCtlTraceType OBJECT-TYPE
              SYNTAX  LspTraceType
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                  "lsp trace type (0:ldp, 1:rsvpTe, 2:vccv, 3:v6ldp)"
              ::= { zxr10LspTraceCtlEntry 2 }

    zxr10LspTraceCtlLdpPrefix OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "ldp prefix"
              ::= { zxr10LspTraceCtlEntry 3 }

    zxr10LspTraceCtlLdpPrefixLength OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "ldp prefix length"
              ::= { zxr10LspTraceCtlEntry 4 }

     zxr10LspTraceCtlTeTunnelIfName OBJECT-TYPE
              SYNTAX  DisplayString(SIZE(0..32))
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "tunnel name "
              ::= { zxr10LspTraceCtlEntry 5 }
    zxr10LspTraceCtlPwDestAddr OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS  read-write
              STATUS  obsolete
              DESCRIPTION
                      "pw peer address"
              ::= { zxr10LspTraceCtlEntry 6 }
    zxr10LspTraceCtlPwVcId OBJECT-TYPE
              SYNTAX  Unsigned32 (1..4294967294)
              MAX-ACCESS  read-write
              STATUS  obsolete
              DESCRIPTION
                      "pw vcid"
              ::= { zxr10LspTraceCtlEntry 7 }

--    zxr10LspTraceCtlPwTTL OBJECT-TYPE
--              SYNTAX  Integer32 (1..255)
--              MAX-ACCESS  read-write
--              STATUS  current
--              DESCRIPTION
--                      "pw label ttl"   
--              DEFVAL  { 1 }
--              ::= { zxr10LspTraceCtlEntry 8 }   

    zxr10LspTraceCtlIfOption OBJECT-TYPE
              SYNTAX  LspTraceIfOption
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "if select option,default:none"
              ::= { zxr10LspTraceCtlEntry 9 }

    zxr10LspTraceCtlTimeOut OBJECT-TYPE
              SYNTAX  Integer32 (1..20)
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "timeout,default:3"
              ::= { zxr10LspTraceCtlEntry 10 }

    zxr10LspTraceCtlTTL OBJECT-TYPE
              SYNTAX  Integer32 (1..255)
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "ttl,default:30"
              ::= { zxr10LspTraceCtlEntry 11 }

    zxr10LspTraceCtlTrapOncompletion OBJECT-TYPE
              SYNTAX  TruthValue
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "if trap"
              ::= { zxr10LspTraceCtlEntry 12 }

    zxr10LspTraceRosStatus OBJECT-TYPE
              SYNTAX  TraceRosStatus
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "controls to start lsp trace"
              ::= { zxr10LspTraceCtlEntry 13 }

    zxr10LspTraceEntryOwner OBJECT-TYPE
              SYNTAX  DisplayString
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                  "entry owner"
              ::= { zxr10LspTraceCtlEntry 14 }

    zxr10LspTraceCtlPwType OBJECT-TYPE
              SYNTAX  LspTracePwType
              MAX-ACCESS  read-write
              STATUS  obsolete
              DESCRIPTION
                      "lsp pw type (0:etherVlan,1:ether)"
              DEFVAL       { 0 }  
              ::= { zxr10LspTraceCtlEntry 15 }

    zxr10LspTraceCtlPwName OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "pw name"
        ::= { zxr10LspTraceCtlEntry 16 }

    zxr10LspTraceCtlMultiSegment OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicate if the pw is multi-segment or not."
        ::= { zxr10LspTraceCtlEntry 17 }

    zxr10LspTraceCtlV6LdpPrefix OBJECT-TYPE
        SYNTAX      Ipv6Address
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "IPv6 ldp prefix"
        ::= { zxr10LspTraceCtlEntry 18 }

    zxr10LspTraceCtlV6LdpPrefixLen OBJECT-TYPE
        SYNTAX      Integer32 (0..128)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "IPv6 ldp prefix length"
        ::= { zxr10LspTraceCtlEntry 19 }

    zxr10LspTraceCtlOutInterface OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "out-interface for tracing packets"
        ::= { zxr10LspTraceCtlEntry 20 }

    zxr10LspTraceCtlLspType OBJECT-TYPE
        SYNTAX      INTEGER { master(1), slave(2), tunnel(3) }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "te-tunnel lsp type"
        ::= { zxr10LspTraceCtlEntry 21 }

    zxr10LspTraceCtlSrcIpAddr OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Source IPv4 address for tracing"
        ::= { zxr10LspTraceCtlEntry 22 }

    zxr10LspTraceCtlV6SrcIpAddr OBJECT-TYPE
        SYNTAX      Ipv6Address
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Source IPv6 address for tracing"
        ::= { zxr10LspTraceCtlEntry 23 }

    zxr10LspTraceCtlDownstrMapType OBJECT-TYPE
        SYNTAX      INTEGER (0..1)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Downstream mapping type
            - 0, Downstream Mapping in RFC4379
            - 1, Downstream Detailed Mapping in RFC6424"
        DEFVAL      { 0 }
        ::= { zxr10LspTraceCtlEntry 24 }

    zxr10LspTraceCtlReplyMode  OBJECT-TYPE
        SYNTAX      INTEGER  {
                       notReply(1),
                       ip(2),
                       routerAlert(3),
                       applicationChannel(4)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The reply mode by the receiving router receive an MPLS echo request"
        DEFVAL      { 2 }
        ::= { zxr10LspTraceCtlEntry 25 }

-- *******************************************************
--               Lsp Trace Results Table
-- *******************************************************
    zxr10LspTraceResultsTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10LspTraceResultsEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of lsp trace results information"
               ::= { zxr10LspTrace 2 }

    zxr10LspTraceResultsEntry  OBJECT-TYPE
              SYNTAX  Zxr10LspTraceResultsEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "lsp trace result entry"
              INDEX  {
                        zxr10LspTraceResultsSerial
                     }
              ::= { zxr10LspTraceResultsTable 1 }

    Zxr10LspTraceResultsEntry ::=
              SEQUENCE {
                zxr10LspTraceResultsSerial
                    Integer32,
                zxr10LspTraceResultsCurHopCount
                    Integer32,
                zxr10LspTraceResultsEntryOwner
                    DisplayString
              }

    zxr10LspTraceResultsSerial OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "lsp trace result serial number"
              ::= { zxr10LspTraceResultsEntry 1 }

    zxr10LspTraceResultsCurHopCount OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "valid hops count"
              ::= { zxr10LspTraceResultsEntry 2 }

    zxr10LspTraceResultsEntryOwner OBJECT-TYPE
              SYNTAX  DisplayString
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "entry owner"
              ::= { zxr10LspTraceResultsEntry 3 }

-- *******************************************************
--               Lsp Trace Hops Table
-- *******************************************************
    zxr10LspTraceHopsTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10LspTraceHopsEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of lsp trace hops information."
               ::= { zxr10LspTrace 3 }

    zxr10LspTraceHopsEntry  OBJECT-TYPE
              SYNTAX  Zxr10LspTraceHopsEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "lsp trace hops entry"
              INDEX  {
                        zxr10LspTraceHopsSerial,
                        zxr10LspTraceHopsHopIndex
                     }
              ::= { zxr10LspTraceHopsTable 1 }

    Zxr10LspTraceHopsEntry ::=
              SEQUENCE {
                zxr10LspTraceHopsSerial
                    Integer32,
                zxr10LspTraceHopsHopIndex
                    Integer32,
                zxr10LspTraceHopsResultValid
                    TraceHopsResult,
                zxr10LspTraceHopsType
                    HopType,
                zxr10LspTraceHopsAddr
                    IpAddress,
                zxr10LspTraceHopsLabel
                    Integer32,
                zxr10LspTraceHopsMtu
                    Integer32,
                zxr10LspTraceHopsRtt
                    Integer32,
                zxr10LspTraceHopsV6Addr
                    Ipv6Address
              }

    zxr10LspTraceHopsSerial OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "lsp trace hops serial number"
              ::= { zxr10LspTraceHopsEntry 1 }

    zxr10LspTraceHopsHopIndex OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "Hop index"
              ::= { zxr10LspTraceHopsEntry 2 }

    zxr10LspTraceHopsResultValid OBJECT-TYPE
              SYNTAX  TraceHopsResult
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "Hop Result (0:valid,1:success,2:downstreamRouterButNotTarget,
                      3:requestNotTransmitted,4:timeout,5:unreachable)"
              ::= { zxr10LspTraceHopsEntry 3 }

    zxr10LspTraceHopsType OBJECT-TYPE
              SYNTAX  HopType
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "Hop Type (0:Ingress,1:Downstream,2:Egress)"
              ::= { zxr10LspTraceHopsEntry 4 }

    zxr10LspTraceHopsAddr OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "Hop Address"
              ::= { zxr10LspTraceHopsEntry 5 }

    zxr10LspTraceHopsLabel OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "Hop Label"
              ::= { zxr10LspTraceHopsEntry 6 }

    zxr10LspTraceHopsMtu OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "Hop Mtu"
              ::= { zxr10LspTraceHopsEntry 7 }

    zxr10LspTraceHopsRtt OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "Hop Rtt"
              ::= { zxr10LspTraceHopsEntry 8 }

    zxr10LspTraceHopsV6Addr OBJECT-TYPE
              SYNTAX  Ipv6Address
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "IPv6 hop address"
              ::= { zxr10LspTraceHopsEntry 9 }


-- *******************************************************
--               Lsp Trace Result Trap Definitions
-- *******************************************************
    lspTraceTrapResult NOTIFICATION-TYPE
     OBJECTS  { zxr10LspTraceResultsSerial,
                zxr10LspTraceResultsCurHopCount,
                zxr10LspTraceResultsEntryOwner
              }
              STATUS  current
              DESCRIPTION
         "The object will return a value while trace finish."
     ::= { lspTraceNotifications 1 }

-- *******************************************************
--               Lsp Trace Hops Trap Definitions
-- *******************************************************
    lspTraceTrapHops NOTIFICATION-TYPE
     OBJECTS  { zxr10LspTraceHopsSerial,
                zxr10LspTraceHopsHopIndex,
                zxr10LspTraceHopsResultValid,
                zxr10LspTraceHopsType,
                zxr10LspTraceHopsAddr,
                zxr10LspTraceHopsLabel,
                zxr10LspTraceHopsMtu,
                zxr10LspTraceHopsRtt,
                zxr10LspTraceHopsV6Addr
              }
     STATUS  current
     DESCRIPTION
         "The object will return a value while trace finish."
     ::= { lspTraceNotifications 2 }

--  ******************************************************
--                Mac Trace Control Table
--  ******************************************************
    zxr10MacTraceCtlTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Zxr10MacTraceCtlEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "the description of mac trace information."
               ::= { zxr10MacTrace 1 }

    zxr10MacTraceCtlEntry  OBJECT-TYPE
              SYNTAX  Zxr10MacTraceCtlEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "mac trace control entry"
              INDEX  {
                        zxr10MacTraceCtlSerial
                     }
              ::= { zxr10MacTraceCtlTable 1 }

    Zxr10MacTraceCtlEntry ::=
              SEQUENCE {
                zxr10MacTraceCtlSerial
                    Integer32,
                zxr10MacTraceCtlDestMac
                    DisplayString,
                zxr10MacTraceCtlIfPeOption
                    MacTraceIfPeOption,
                zxr10MacTraceCtlOutEtherIfIndex
                    Integer32,
                zxr10MacTraceCtlVfiName
                    DisplayString,
                zxr10MacTraceCtlPeerAddress
                    IpAddress,
                zxr10MacTraceCtlVlanCount
                    Integer32,
                zxr10MacTraceCtlInternalVlanId
                    Integer32,
                zxr10MacTraceCtlExternalVlanId
                    Integer32,
                zxr10MacTraceCtlTrapOncompletion
                    TruthValue,
                zxr10MacTraceCtlRosStatus
                    TraceRosStatus,
                zxr10MacTraceCtlEntryOwner
                    DisplayString
              }

    zxr10MacTraceCtlSerial OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-only
              STATUS  current
              DESCRIPTION
                      "mac trace control serial number"
              ::= { zxr10MacTraceCtlEntry 1 }

    zxr10MacTraceCtlDestMac OBJECT-TYPE
              SYNTAX  DisplayString
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "destination mac address"
              ::= { zxr10MacTraceCtlEntry 2 }

    zxr10MacTraceCtlIfPeOption OBJECT-TYPE
              SYNTAX  MacTraceIfPeOption
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "if PE option (0:CE,1:PE)"
              ::= { zxr10MacTraceCtlEntry 3 }

    zxr10MacTraceCtlOutEtherIfIndex OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "index of out interface on CE"
              ::= { zxr10MacTraceCtlEntry 4 }

    zxr10MacTraceCtlVfiName OBJECT-TYPE
              SYNTAX  DisplayString
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "VFI name"
              ::= { zxr10MacTraceCtlEntry 5 }

    zxr10MacTraceCtlPeerAddress OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "Peer Address"
              ::= { zxr10MacTraceCtlEntry 6 }

    zxr10MacTraceCtlVlanCount OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "vlan count"
              ::= { zxr10MacTraceCtlEntry 7 }

    zxr10MacTraceCtlInternalVlanId OBJECT-TYPE
              SYNTAX  Integer32
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
