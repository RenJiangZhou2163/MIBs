
--  TEKTRONIX-GPID-MIB { iso(1) org(3) dod(6) internet(1) private(4)
--                       enterprises(1) tektronix(128) gpid (2) }
--
--  This is a MIB module for Tektronix network-connected printers.
--  It includes only the enterprise-specific objects; see the following
--  RFCs for Internet-standard MIB objects supported by the printer:
--
--  RFC 1155    Structure of Management Information
--  RFC 1212    Concise MIB Format
--  RFC 1213    MIB-II
--  RFC 1229    Generic Interface Extensions (as amended by RFC 1239)
--  RFC 1239    Reassignment of Experimental MIBs to Standard MIBs
--  RFC 1354    IP Forwarding Table MIB
--  RFC 1398    Ethernet/IEEE 802.3 MIB

TEKTRONIX-GPID-MIB DEFINITIONS ::= BEGIN

IMPORTS
	enterprises             FROM RFC1155-SMI
	OBJECT-TYPE             FROM RFC-1212
	DisplayString           FROM RFC1213-MIB;

--  Identifier 128 has been assigned to Tektronix, Inc. by the
--  IANA.  Each division within Tektronix manages its own
--  subtree below the Tektronix enterprise ID.

tektronix       OBJECT IDENTIFIER ::= { enterprises 128 }

-- Graphic Printing and Imaging Division

tekgpid         OBJECT IDENTIFIER ::= { tektronix 2 }

gpidmibs        OBJECT IDENTIFIER ::= { tekgpid 1 }
gpidproducts    OBJECT IDENTIFIER ::= { tekgpid 2 }

gpidprintermib  OBJECT IDENTIFIER ::= { gpidmibs 3 }

	  -- groups in PRINTERMIB

	  gpidGeneral    OBJECT IDENTIFIER ::= { gpidprintermib 1 }

	  gpidInput      OBJECT IDENTIFIER ::= { gpidprintermib 2 }

	  gpidOutput     OBJECT IDENTIFIER ::= { gpidprintermib 3 }

	  gpidMarker     OBJECT IDENTIFIER ::= { gpidprintermib 4 }

	  gpidMediaPath  OBJECT IDENTIFIER ::= { gpidprintermib 5 }

	  gpidChannel    OBJECT IDENTIFIER ::= { gpidprintermib 6 }

	  gpidAlerts     OBJECT IDENTIFIER ::= { gpidprintermib 7 }

	  gpidJobAccnt   OBJECT IDENTIFIER ::= { gpidprintermib 8 }

	  gpidSysSetup   OBJECT IDENTIFIER ::= { gpidprintermib 9 }


	  -- the gpidGeneral group

	  -- Implementation of the gpidGeneral group is mandatory for
	  -- all printers. This group contains variables that are 
	  -- provide status and information on the entire printer.
	  -- In the DMTF model this group usually is implemented on
	  -- the printer's system controller.

	  gpidGeneralManufacturer OBJECT-TYPE
	      SYNTAX  DisplayString (SIZE (0..20))
	      ACCESS  read-only 
	      STATUS  mandatory 
	      DESCRIPTION
		      "The name of the manufacturer of the printer.
		       Tektronix, Inc."
	      ::= { gpidGeneral 1 }

	  gpidGeneralProduct OBJECT-TYPE
	      SYNTAX  DisplayString (SIZE (0..80))
	      ACCESS  read-only 
	      STATUS  mandatory 
	      DESCRIPTION
		      "The product type name. For instance, Phaser 480."
	      ::= { gpidGeneral 2 }

	  gpidGeneralVersion OBJECT-TYPE
	      SYNTAX  DisplayString (SIZE (0..80))
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The Tektronix Version number as reported on the 
		       startpage. For instance, 1.1/01.22."
	      ::= { gpidGeneral 3 }

	  gpidGeneralStatus OBJECT-TYPE
	      SYNTAX  INTEGER {
			  other(1),          -- none of the following
			  unknown(2),
			  ready(3),
			  criticalState(4),
			  non-critical(5),
			  criticalandNonCritical(6),
			  onRequest(7),
			  unavailable(8),
			  busy(9),
			  waiting(10)
		      }                 
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The status indicator for the print engine. This
		       is an overall indication of the printer's state.
		       In addition, each of the sub-units can report their
		       state so this in essence reports the highest priority
		       state."
	      ::= { gpidGeneral 4 }

	  gpidGeneralPrinterBusy OBJECT-TYPE
	      SYNTAX  INTEGER {
			  initializing(1),
			  ready(2),
			  printing(3),
			  warmingUp(4),
			  cleaning(5),
			  standby(6),
			  energyStar(7),
			  engineFailure(8),
			  receivingData(9),
			  processingData(10),
			  printingStartPage(11),
			  frontPanelInUse(12),
			  copying(13),
			  waiting(14),
			  off-line(15),
			  printerError(16),
			  busy(17),
			  checkPrint(18),
			  coolingDown(19),
			  isCool(20),
			  cuttingMedia(21),
			  feedingMedia(22),
			  loadingMedia(23),
			  unloadingMedia(24),
			  loadingInk(25),
			  fuserWarmingUp(26),
			  fuserCoolingDown(27),
			  serviceMode(28),
			  testing(29),
			  waitingForCancel(30),
			  warmupManFeedError(31),
			  transientEngError(32),
			  engineSyncError(33),
			  mptMediaClassUnknownErr(34)
		      }
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "This status indicator for the print engine indicating
		       the current printer state."
	      ::= { gpidGeneral 5 }

	  gpidGeneralEngineFailure OBJECT-TYPE
	      SYNTAX  INTEGER 
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "This status indicator for a print engine failure
		       indicating which print engine failure occured.
		       This failure may be transient merely requiring the
		       printer to be turned off and on. Or could require
		       service support. Each printer will use its own
		       enumerations. For instance, Wren has specified:
			  enging-OK(-1),          
			  blackCartridgeMovementError(1),
			  cyanCartridgeMovementError(2),
			  magentaCartridgeMovementError(3),
			  yellowCartridgeMovementError(4),
			  tonerCartridgeCoolingFanError(5),
			  scannerMotorUnlocked(10),
			  laserPowerDownError(11),
			  lSyncOver(12),
			  lSyncShort(13),
			  chargerError(20),
			  asychronousOPC(21),
			  opcPositionError(22),
			  intermediateTransferRollerMovementError(30),
			  cleaningBladeMovementError(31),
			  paperTransferRollerMovementError(33),
			  fuserLowTemperature(40),
			  fuserHighTemperature(41),
			  fuserThermistorOpen(42),
			  powerSupplyCoolingFanError(50),
			  ozoneFanError(51),
			  sleeveMotorUnlocked(61),
			  transportMotorUnlocked(62),
			  mainMotorUnlocked(63),
			  ROMCheckSumError(70),
			  SRAMCheckError(71),
		      for its enumerations."
	      ::= { gpidGeneral 6 }

	  gpidGeneralEnergyStarOperState OBJECT-TYPE
	      SYNTAX  INTEGER {
			  operational(1),          
			  energy-star(2)
		      }
	      ACCESS  not-accessible
	      STATUS  obsolete
	      DESCRIPTION
		      "This variable is used to indicate the state
		       of the print engine. This variable should
		       not be confused with the energy star state
		       variable that lives on the I/O card. If the
		       printer is in standby then this variable
		       should be in energy-star. When this
		       variable is operational the printer is ready to
		       print."
	      ::= { gpidGeneral 7 }

	  gpidGeneralEnergyStarAdminState OBJECT-TYPE
	      SYNTAX  INTEGER {
			  operational(1),          
			  energy-star(2)
		      }
	      ACCESS  not-accessible
	      STATUS  obsolete
	      DESCRIPTION
		      "This variable is used to toggle the variable
		       gpidGeneralEnergyOperState. If the printer
		       is in the energy-star mode then writing to this
		       variable will wake the printer up. If the printer
		       is in operational mode then writing to this
		       variable will put the printer in energy-star mode.
		       This variable should be considered write only."
	      ::= { gpidGeneral 8 }

	  gpidGeneralTimeTillWarm OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "This variable is used to indicate the estimated time
		       until warmup is complete. This should indicate
		       a range of plus or minus 3 minutes."
	      ::= { gpidGeneral 9 }

	  gpidGeneralRAMInstalled OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "This variable is used to indicate the amount of
		       RAM installed in the printer. It is is MBytes."
	      ::= { gpidGeneral 10 }

	  gpidGeneralFontsInstalled OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "This variable is used to indicate the number of 
		       fonts installed in the printer."
	      ::= { gpidGeneral 11 }

	  gpidGeneralCopyStationInstalled OBJECT-TYPE
	      SYNTAX  INTEGER {
			  installed(1),          
			  notInstalled(2)
		      }
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "This variable is used to indicate if
		       there is a CopyStation attached to the printer."
	      ::= { gpidGeneral 12 }


	  -- the gpidInput group

	  -- Implementation of the gpidInput group is mandatory for
	  -- all printers. The input group is used for sub-units of
	  -- the printer that feed media into the printer. In most
	  -- Tektronix printers these units are removable trays. In
	  -- most Tektronix printers there is no way to seperately
	  -- select manual feed so it is not considered a seperate unit.

	  gpidInputNumber OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The number of input devices present on this system."
	      ::= { gpidInput 1 }

	  -- the Input table

	  -- The Input table contains information on the entity's
	  -- input trays.  Each tray is thought of as being a separate
	  -- input device. In most cases the word device and tray are
	  -- interchangable. Each device thus is a sub-sub-unit, i.e.
	  -- a device under the input sub-unit umbrella. This table will 
	  -- contain 1 or more entries. 

	  gpidInputTable OBJECT-TYPE
	      SYNTAX  SEQUENCE OF GpidInputEntry
	      ACCESS  not-accessible
	      STATUS  mandatory
	      DESCRIPTION
		      "A list of gpidInput entries.  The number of
		      entries is given by the value of gpidInputNumber."
	      ::= { gpidInput 2 }

	  gpidInputEntry OBJECT-TYPE
	      SYNTAX  GpidInputEntry
	      ACCESS  not-accessible
	      STATUS  mandatory
	      DESCRIPTION
		      "A gpidInput entry containing objects at 
		       the input device level for a particular input device."
	      INDEX   { gpidInputTrayIndex }
	      ::= { gpidInputTable 1 }

	  GpidInputEntry ::=
	      SEQUENCE {
		  gpidInputTrayIndex
		      INTEGER,
		  gpidInputTrayId
		      INTEGER,
		  gpidInputTrayType
		      INTEGER,
		  gpidInputCapacityUnit
		      INTEGER,
		  gpidInputMaxCapacity
		      INTEGER,
		  gpidInputCurrentLevel
		      INTEGER,
		  gpidInputStatus
		      INTEGER,
		  gpidInputMediaType
		      INTEGER,
		  gpidInputMediaSize
		      INTEGER
	      }

	  gpidInputTrayIndex OBJECT-TYPE
	      SYNTAX  INTEGER 
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "A unique value for each input device.  This is
		       index into the tray table."
	      ::= { gpidInputEntry 1 }

	  gpidInputTrayId OBJECT-TYPE
	      SYNTAX  INTEGER {
			  other(1),          -- none of the following
			  unknown(2),
			  manualFeed(3),
			  mainTray(4),
			  upperTray(5),
			  middleTray(6),
			  lowerTray(7),
			  trayOne(8),
			  trayTwo(9),
			  trayThree(10),
			  trayFour(11),
			  trayFive(12),
			  continuousRoll(13),
			  continousFanFold(14),
			  multipurposeTray(15),
			  autoDuplexUnit(16),
			  envelopeFeeder(17)
		      }         
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "A unique value for each input device.  This ID
		       indicates which instance of the tray is installed.
		       DO NOT CHANGE THIS ENUMERATION WITHOUT CHANGING
		       gpidInputDefaultSource."
	      ::= { gpidInputEntry 2 }

	  gpidInputTrayType OBJECT-TYPE
	      SYNTAX  INTEGER {
			  other(1),          -- none of the following
			  unknown(2),
			  removeableTray(3),
			  unremoveableTray(4),
			  sheetFeedManual(5),
			  continuousRoll(6),
			  continousFanFold(7)
		      }
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The kind of tray being used."
	      ::= { gpidInputEntry 3 }

	  gpidInputCapacityUnit OBJECT-TYPE
	      SYNTAX  INTEGER {
			  other(1),          -- none of the following
			  unknown(2),
			  thousandth-inch(3),
			  hundredth-millimeter(4),
			  sheets(8),
			  feet(16),
			  meters(17)
		      }
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The measurement unit used when by the variables
		      gpidInputmaxCapacity and gpidInputCurrentLevel. For
		      most Tektronix printers this will be sheets(8)."
	      ::= { gpidInputEntry 4 }

	  gpidInputMaxCapacity OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The number of sheets that can be placed in the tray."
	      ::= { gpidInputEntry 5 }
	  gpidInputCurrentLevel OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The number of sheets that are currently in 
		       the tray. This report may lie because the current  
		       printers only have a out, low and full indicators. Thus
		       if it possible to put a hundred sheets in the
		       tray, the level indicator could be 100, 25, or 0.
		       This would mean that when 100 is specified
		       the tray could have between 100 and 26 sheets.
		       When 25 is specified then the tray would have
		       between 25 and 1 sheets in the tray. By convention,
		       a low indication will be 10% or less of maximum."
	      ::= { gpidInputEntry 6 }

	  gpidInputStatus OBJECT-TYPE
	      SYNTAX  INTEGER {
			  other(1),          -- none of the following
			  unknown(2),
			  ready(3),
			  criticalState(4),
			  non-critical(5),
			  criticalandNonCritical(6),
			  onRequest(7),
			  unavailable(8),
			  busy(9)
		      }
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The current state of the particular input device."
	      ::= { gpidInputEntry 7 }

	  gpidInputMediaType OBJECT-TYPE
	      SYNTAX  INTEGER {
			  other(1),          -- none of the following
			  unknown(2),
			  paper(3),
			  heavyPaper(4),
			  transparency(5),
			  coatedPaper(6),
 			  cardStock(7),
			  labelStock(8),
			  fabricTransfer(9),
			  duplexSetting(10),
			  envelope(11),
			  thickCoverIndex(12),
			  thinCoverIndex(13),
			  heavyWeightCoverPaper(14),
			  letterhead(15),
                          colorPaper(16),
			  special(17),    
			  thinCardStock(18),
			  thickCardStock(19),
			  photoPaper(20),
			  businessCards(21),
