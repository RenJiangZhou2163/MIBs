	  -- the gpidOutput group
	  -- Implementation of the gpidOutput group is mandatory for
	  -- all printers. The output group is used for sub-units of
	  -- the printer that receive media that has been successfully
	  -- marked on. In most Tektronix printers there is a single unit 
	  -- which is removable tray that does not return any status. 
	  -- the Input table

	  gpidOutputNumber OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The number of output devices present on this system."
	      ::= { gpidOutput 1 }

	  -- the Output table

	  -- The Output table contains information on the entity's
	  -- output trays.  Each output tray is thought of as being a separate
	  -- output device. In most cases the word device and tray are
	  -- interchangable. Each device thus is a sub-sub-unit, i.e.
	  -- a device under the input sub-unit umbrella. This table will 
	  -- contain 1 or more entries. 

	  gpidOutputTable OBJECT-TYPE
	      SYNTAX  SEQUENCE OF GpidOutputEntry
	      ACCESS  not-accessible
	      STATUS  mandatory
	      DESCRIPTION
		      "A list of gpidOutput entries.  The number of
		      entries is given by the value of gpidOutputNumber."
	      ::= { gpidOutput 2 }

	  gpidOutputEntry OBJECT-TYPE
	      SYNTAX  GpidOutputEntry
	      ACCESS  not-accessible
	      STATUS  mandatory
	      DESCRIPTION
		      "A gpidOutput entry containing objects at 
		       the ouput device level for a particular output device."
	      INDEX   { gpidOutputTrayIndex }
	      ::= { gpidOutputTable 1 }

	  GpidOutputEntry ::=
	      SEQUENCE {
		  gpidOutputTrayIndex
		      INTEGER,
		  gpidOutputId
		      INTEGER,
		  gpidOutputType
		      INTEGER,
		  gpidOutputCapacityUnit
		      INTEGER,
		  gpidOutputMaxCapacity
		      INTEGER,
		  gpidOutputCurrentLevel
		      INTEGER,
		  gpidOutputStatus
		      INTEGER,
		  gpidOutputDeliveryOrientation
		      INTEGER,
		  gpidOutputMediaCutting
		      INTEGER,
		  gpidOutputMediaFusing
		      INTEGER,
		  gpidOutputAdvanceMedia
		      INTEGER,
		  gpidOutputAdvanceDistance
		      INTEGER
	      }
  
	 gpidOutputTrayIndex OBJECT-TYPE
	      SYNTAX  INTEGER 
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "A unique value for each output device.  This is
		       index into the output tray table."
	      ::= { gpidOutputEntry 1 }

	  gpidOutputId OBJECT-TYPE
	      SYNTAX  INTEGER (0..255)
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "A unique number for the output device sub-unit."
	      ::= { gpidOutputEntry 2 }

