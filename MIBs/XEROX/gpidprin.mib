	      ::= { gpidMarkerSuppliesEntry 2 }

	  gpidMarkerSuppliesMarkerId OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The marker ID for this marker supply. This 
		       is essentially unused in the Tektronix printers.
		       If multiple markers were possible then there
		       could be a marker supplies table for each
		       marker instance."
	      ::= { gpidMarkerSuppliesEntry 3 }


	  gpidMarkerSuppliesType OBJECT-TYPE
	      SYNTAX  INTEGER {
			  other(1),          -- none of the following
			  unknown(2),
			  toner(3),
			  wasteToner(4),
			  ink(5),
			  inkCartridge(6),
			  inkRibbon(7),
			  wasteInk(8),
			  organicPhotoConductor(9),
			  developer(10),
			  fuserOil(11),
			  wax-solid(12),
			  wax-ribbon(13),
			  wasteWax(14),
			  maintenanceTray(15),
			  feeder(16),
			  tensioner(17)
		      }
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The kind of supply type being used."
	      ::= { gpidMarkerSuppliesEntry 4 }

	  gpidMarkerSuppliesDescription OBJECT-TYPE
	      SYNTAX  DisplayString (SIZE (0..127))
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "A textual statement of the marker supply type. This
		       will match the wording in the printer and the
		       printer's user manual. This should make it easier
		       to connect a supply alert with the physical hardware."
	      ::= { gpidMarkerSuppliesEntry 5 }

	  gpidMarkerSuppliesMaxCapacity OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The maximum capacity of the marker supply element.
		       This will be the maximum amount of some supply unit.
		       The value (-2) means unknown."
	      ::= { gpidMarkerSuppliesEntry 6 }

	  gpidMarkerSuppliesCurrentLevel OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The current capacity of the marker supply element.
		       For instance, on printers with toner cartridges that
		       are replaced as an entire entity but report toner
		       low this will have a value in between 0 and 
		       gpidMarkerSuppliesMaxCapacity. By convention,
		       a low indication will be 10% or less of maximum.
		       The value (-2) means unknown.  A value of (-3) means 
		       that the printer knows that there is some supply
		       remaining."
	      ::= { gpidMarkerSuppliesEntry 7 }

	  gpidMarkerSuppliesMaxLifeCount OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-write
	      STATUS  mandatory
	      DESCRIPTION
		      "The number of sheets that can be expected to be run 
		       through the marking supply unit. When the 
		       gpidMarkerSuppliesLifeCount exceeds this number
		       the supply is expected to be exhausted. Your
		       mileage will vary. The value (-2) means unknown."
	      ::= { gpidMarkerSuppliesEntry 8 }

	  gpidMarkerSuppliesLifeCount OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-write
	      STATUS  mandatory
	      DESCRIPTION
		      "The number of sheets that have been run 
		       through the marking supply unit. With the
		       Phaser 540 writing to this variable will cause
		       the value to be set to 0. The value (-2) means 
		       unknown.  A value of (-3) means that the printer 
		       knows that it has not reached its max limit yet."
	      ::= { gpidMarkerSuppliesEntry 9 }

	  gpidMarkerSuppliesStatus OBJECT-TYPE
	      SYNTAX  INTEGER {
			  other(1),          -- none of the following
			  unknown(2),
			  ready(3),
			  criticalState(4),
			  non-critical(5),
			  criticalandNonCritical(6),
			  onRequest(7),
			  unavailable(8),
			  busy(9)
		      }
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The current state of the particular marker supply."
	      ::= { gpidMarkerSuppliesEntry 10 }

	  gpidMarkerSuppliesRemaining OBJECT-TYPE
	      SYNTAX  INTEGER
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The percent of the marker remaing.  The variable will
		       take the values (0, 100).  The value 0 indicates no 
		       marker is remaining and 100 indicates the marker is 
		       at full capacity.  This value should be displayed as
		       a percentage."
	      ::= { gpidMarkerSuppliesEntry 11 }


	  -- the gpidMediaPath group

	  -- Implementation of the gpidMediaPath group is mandatory for
	  -- all printers. The media path group is used for sub-units of
	  -- the printer that pull the media from the input sub-unit, 
	  -- through the marker and place the media in the output sub-unit.

