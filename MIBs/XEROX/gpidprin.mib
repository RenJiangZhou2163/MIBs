			  iso-a4(27),
			  iso-a5(28),
			  iso-a6(29),
			  iso-b4(34),
			  iso-b5(35),
			  iso-b6(36),
			  iso-c5(37),
			  iso-sra3(38),
			  tabloid-extra(101),
			  oversize-A(102),
			  tabloid-maximum(103),
			  tabloid-plus(104),
			  tabloid-xl(105),
                          jis-b5(107),
                          jis-b6(108),
                          oficio(112),
                          spFolio(113),
                          ra3(115),
			  jis-b1(124),
			  jis-b2(125),
			  jis-b3(126),
			  jis-b4(127),
                          envelopeISOC6 (129),
                          envelopeISOB4(130), 
                          envelopeISOB5(131), 
                          envelopeISOB6(132), 
			  regularC(134),
			  regularD(135),
			  regularE(136),
			  architectB(143),
			  architectC(144),
			  architectD(145),
