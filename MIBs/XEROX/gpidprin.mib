			  cdDVDLabels(23),
			  cdDVDInserts(24),
			  clearFilm(101),
			  whiteFilm(102),
			  transferFilm(103),
			  film(104),
			  photo(105),
			  mixed(-20)
		      }
	      ACCESS  read-only
	      STATUS  mandatory
	      DESCRIPTION
		      "The media type currently loaded in the tray. 
		       DO NOT CHANGE THIS ENTRY WITHOUT CHANGING
		       gpidJobAccntCurrentMediaType and gpidJobAccntMediaType."
	      ::= { gpidInputEntry 8 }

	  gpidInputMediaSize OBJECT-TYPE
	      SYNTAX  INTEGER {
			  other(1),          -- none of the following
			  unknown(2),
			  letter(3),
			  legal(4),
			  envelopeC6(5),
			  envelopeDL(6),
			  envelopeNo6(7),
			  envelopeNo10(8),
			  envelopeExec(9),
                          ukFoolscap(13),
                          usFolio(14),
			  tabloid(15),
                          bookletEnvelope9x12(16),
			  iso-a0(23),
			  iso-a1(24),
			  iso-a2(25),
