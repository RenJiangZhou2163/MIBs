    ::= { tVirtualSchedulerEntry 10 }

tVirtualSchedulerCIR OBJECT-TYPE
    SYNTAX       THPolVirtualScheCIRRate
    UNITS        "kbps"
    MAX-ACCESS   read-create
    STATUS       obsolete
    DESCRIPTION
        "The rate allowed for this scheduler. A value of -1 implies
         maximum.
      
