-- *******************************************************************
--  Conformance Information
-- *******************************************************************

 vrrpExtMIBCompliances  OBJECT IDENTIFIER ::= { vrrpExtConformance 1 }
 vrrpExtMIBGroups       OBJECT IDENTIFIER ::= { vrrpExtConformance 2 }

-- ...................................................................
-- Compliance Statements
-- ...................................................................

 vrrpExtMIBCompliance MODULE-COMPLIANCE
     STATUS current
     DESCRIPTION
        "The core compliance statement for all VRRP implementations."
     MODULE -- this module
     MANDATORY-GROUPS  {
         vrrpExtendTrackInterGroup,
         vrrpExtendTrackGroup,
         vrrpManageCfgGroup,
         vrrpMemberCfgGroup
     }

     ::= { vrrpExtMIBCompliances 1 }

-- ...................................................................
-- Conformance Groups
-- ...................................................................

 vrrpExtendTrackInterGroup  OBJECT-GROUP
     OBJECTS  {
         vrrpTrackInterState,
         vrrpTrackInterPriReduce,
         vrrpTrackInterOperRowStatus
         }
     STATUS current
     DESCRIPTION
        "Conformance group for VRRP track interface operations."
     ::= { vrrpExtMIBGroups 1 }

 vrrpExtendTrackGroup  OBJECT-GROUP
     OBJECTS  {         
         vrrpTrackType,
         vrrpDetectType,
         vrrpPolicyType,
         vrrpTrackPriReduce,
         vrrpTrackState,
         vrrpTrackOperRowStatus
         }
     STATUS current
     DESCRIPTION
        "Conformance group for VRRP tracking operations."
     ::= { vrrpExtMIBGroups 2 }

 vrrpManageCfgGroup  OBJECT-GROUP
     OBJECTS  {
         mCfgOperRowStatus         
         }
     STATUS current
     DESCRIPTION
        "Conformance group for VRRP manage operations."
     ::= { vrrpExtMIBGroups 3 }
 
 vrrpMemberCfgGroup  OBJECT-GROUP
     OBJECTS  {
         cfgOperRowStatus         
         }
     STATUS current
     DESCRIPTION
        "Conformance group for VRRP member operations."
     ::= { vrrpExtMIBGroups 4 }

-- *******************************************************************
--  VRRP Extend Operations Table
-- *******************************************************************

zxr10VrrpExtOperTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF Zxr10VrrpExtOperEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "Extend operations table for a VRRP router which consists
        of a sequence (i.e., one or more conceptual rows) of
        'zxr10VrrpExtOperEntry' items."

    ::= { zxr10VrrpExtOperObjects 1 }

zxr10VrrpExtOperEntry OBJECT-TYPE
    SYNTAX       Zxr10VrrpExtOperEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "An entry in the zxr10VrrpExtOperTable containing the private
        operational characteristics of a virtual router. On a VRRP
        router, a given virtual router is identified by a combination
        of the IF index and VRID."

     INDEX    { zxr10VrrpOperIfIndex, zxr10VrrpOperVrId }
    ::= { zxr10VrrpExtOperTable 1 }

Zxr10VrrpExtOperEntry ::=
     SEQUENCE {
         zxr10VrrpOperIfIndex
            Integer32,
         zxr10VrrpOperVrId
             VrId,
         zxr10VrrpOperTimerLearnMode
             TruthValue,
         zxr10VrrpOperPreemptMode
             TruthValue,
         zxr10VrrpOperPreemptDelay
             Integer32,
         zxr10VrrpOperPrivateAdvInterval
             Integer32,
         zxr10VrrpOperVersion
             INTEGER,
         zxr10VrrpOperCfgPriority
             Integer32,
         zxr10VrrpOperAcceptMode
             TruthValue,
         zxr10VrrpOperBackupRouteMode
             TruthValue,
         zxr10VrrpOperCheckTtlMode
             TruthValue,
         zxr10VrrpOperOutInterface1
             OCTET STRING,
         zxr10VrrpOperOutInterface2
             OCTET STRING,
         zxr10VrrpOperOutInterface3
             OCTET STRING,
         zxr10VrrpOperReloadDelay
             Integer32,
         zxr10VrrpOperPktSendMode
             INTEGER,
         zxr10VrrpOperStateChangeNum
             Integer32,
         zxr10VrrpOperStateLastChangeTime
             TimeStamp,
         zxr10VrrpOperMasterIpAddrType
             InetAddressType,
         zxr10VrrpOperMasterIpAddr
             InetAddress,
         zxr10VrrpOperMasterRunPriority
             Integer32,
         zxr10VrrpOperMasterAdverInterval
             Integer32,
         zxr10VrrpOperMasterDownInterval
             Integer32,
         zxr10VrrpOperState
             INTEGER,
         zxr10VrrpOperRunPriority
             Integer32,
         zxr10VrrpOperPrimaryVIPType
             InetAddressType,
         zxr10VrrpOperPrimaryVIPAddr
             InetAddress
 }

zxr10VrrpOperIfIndex OBJECT-TYPE
    SYNTAX       Integer32
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "This object contains the interface index of VRRP group."
    ::= { zxr10VrrpExtOperEntry 1 }

zxr10VrrpOperVrId OBJECT-TYPE
    SYNTAX       VrId
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "This object contains the Virtual Router Identifier (VRID)."
    ::= { zxr10VrrpExtOperEntry 2 }

zxr10VrrpOperTimerLearnMode OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The current learn mode state of the virtual router. This object has
        two defined values:

        - `true', which indicates that the virtual router is
         set the learn mode.

        - `false', which indicates the virtual router is
         not set the learn mode."
    DEFVAL       { false }
    ::= { zxr10VrrpExtOperEntry 3 }

zxr10VrrpOperPreemptMode OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The current preempt state of the virtual router. This object has
        two defined values:

        - `true', which indicates that the virtual router is
         set the preempt mode.

        - `false', which indicates the virtual router is
         not set the preempt mode."
    DEFVAL       { true }
    ::= { zxr10VrrpExtOperEntry 4 }

zxr10VrrpOperPreemptDelay OBJECT-TYPE
    SYNTAX       Integer32(0..3600)
    UNITS        "seconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The current set preempt delay time of the virtual router."
    DEFVAL       { 0 }
    ::= { zxr10VrrpExtOperEntry 5 }

zxr10VrrpOperPrivateAdvInterval OBJECT-TYPE
    SYNTAX       Integer32 (50..40000)
     UNITS        "milliseconds"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The time interval, in milliseconds, between sending
        advertisement messages. Only the master router sends
        VRRP advertisements."
    DEFVAL       { 1000 }
    ::= { zxr10VrrpExtOperEntry 6 }

zxr10VrrpOperVersion OBJECT-TYPE
    SYNTAX       INTEGER {
         v2(2),
         v3(3)
     }
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The version of VRRP protocol."
    ::= { zxr10VrrpExtOperEntry 7 }

zxr10VrrpOperCfgPriority OBJECT-TYPE
    SYNTAX       Integer32(1..254)
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The actual configured priority which was used for the
        virtual router master election process. Higher values imply
        higher priority."
    DEFVAL       { 100 }
    ::= { zxr10VrrpExtOperEntry 8 }

zxr10VrrpOperAcceptMode OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Controls whether a virtual router in Master state will accept
        packets addressed to the address owner's IPvX address as its
        own if it is not the IPvX address owner."
    DEFVAL       { true }
    ::= { zxr10VrrpExtOperEntry 9 }

zxr10VrrpOperBackupRouteMode OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Controls whether a virtual router in Backup state will forward
        packets addressed to virtual mac address."
    ::= { zxr10VrrpExtOperEntry 10 }

zxr10VrrpOperCheckTtlMode OBJECT-TYPE
    SYNTAX       TruthValue
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "Controls whether a virtual router verify that the IPv4 TTL or the
        IPv6 hop limit to 255."
    DEFVAL       { true }
    ::= { zxr10VrrpExtOperEntry 11 }

zxr10VrrpOperOutInterface1 OBJECT-TYPE
