-- *******************************************************************
--  Textual Conventions
-- *******************************************************************

 VrId ::= TEXTUAL-CONVENTION
     STATUS       current
     DESCRIPTION
         "A number which, along with an interface index (ifIndex),
         serves to uniquely identify a virtual router on a given VRRP
         router. A set of one or more associated addresses is assigned
         to a VRID."
     SYNTAX      Integer32 (1..255)

-- *******************************************************************
--  VRRP MIB Groups
-- *******************************************************************

 vrrpTrackManages         OBJECT IDENTIFIER ::= { vrrpExtMIB 1 }
 vrrpGroupManages         OBJECT IDENTIFIER ::= { vrrpExtMIB 2 }
 vrrpExtConformance       OBJECT IDENTIFIER ::= { vrrpExtMIB 3 }
 zxr10VrrpExtOperObjects  OBJECT IDENTIFIER ::= { vrrpExtMIB 4 }
 zxr10VrrpVFCfgObjects    OBJECT IDENTIFIER ::= { vrrpExtMIB 5 }
 zxr10VrrpVFOperObjects   OBJECT IDENTIFIER ::= { vrrpExtMIB 6 }
 zxr10VrrpVFTrackManages  OBJECT IDENTIFIER ::= { vrrpExtMIB 7 }

-- *******************************************************************
--  Start of MIB objects
-- *******************************************************************

-- *******************************************************************
--  VRRP Track Interface Table
-- *******************************************************************

 vrrpTrackInterTable OBJECT-TYPE
     SYNTAX       SEQUENCE OF VrrpTrackInterEntry
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "Tracking interface table for a VRRP router which consists of a
          sequence (i.e., one or more conceptual rows) of
          'VrrpTrackInterEntry' items."
     ::= { vrrpTrackManages 1 }

 vrrpTrackInterEntry OBJECT-TYPE
     SYNTAX       VrrpTrackInterEntry
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "An entry in the vrrpTrackInterTable containing the tracking
          characteristics of a virtual router. On a VRRP router,
          a given virtual router is identified by a combination
          of the IF index and VRID. Rows in the table cannot be modified."
     INDEX  {
                vrrpTrackInterStandByIfIdx,
                vrrpTrackInterVrid,
                vrrpTrackInterIfIdx
            }
     ::= { vrrpTrackInterTable 1 }

 VrrpTrackInterEntry ::=
     SEQUENCE {
         vrrpTrackInterState
             INTEGER,
         vrrpTrackInterPriReduce
             Integer32,         
         vrrpTrackInterOperRowStatus
             RowStatus,
         vrrpTrackInterStandByIfIdx
             Integer32,
         vrrpTrackInterVrid
             VrId,         
         vrrpTrackInterIfIdx
            Integer32,
        zxr10VrrpTrackInterPolicy
            INTEGER
 }
 
 vrrpTrackInterState OBJECT-TYPE
    SYNTAX          INTEGER
                    {
         up(1),
         down(2)
     }
     MAX-ACCESS   read-only
     STATUS       current

     DESCRIPTION
         "The current state of the tracked interface. This object has
         two defined values:
           - `up', which indicates the protocol status of the tracked interface.
           - `down', which indicates the protocol status of the tracked interface."
      ::= { vrrpTrackInterEntry 1 }
 
 vrrpTrackInterPriReduce OBJECT-TYPE
     SYNTAX       Integer32 (0..254)
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "This object specifies the priority to be reduced when 
          the tracked interface is down.
         A priority of '0', indicates not set the reduced priority."
     DEFVAL { 10 }
     ::= { vrrpTrackInterEntry 2 }

 vrrpTrackInterOperRowStatus OBJECT-TYPE
     SYNTAX       RowStatus
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "The row status variable, used in accordance to installation
         and removal conventions for conceptual rows. The rowstatus of
         a currently active row in the VrrpTrackInterTable is constrained
         by the tracking state of the corresponding virtual router.
         When `VrrpTrackInterOperRowStatus' is set to active(1), no other
         objects in the conceptual row. 

         To create a row in this table, a manager sets this object
         to either createAndGo(4) or createAndWait(5). Until instances
         of all corresponding columns are appropriately configured,
        the value of the corresponding instance of the
        `VrrpTrackInterOperRowStatus' column will be read as notReady(3).

         In particular, a newly created row cannot be made active(1)
        until (minimally) the corresponding instance of `VrrpTrackInterVrid'
        has been set and there is at least one active row in the
        `VrrpTrackInterTable' tracking a interface for the virtual router."
     ::= { vrrpTrackInterEntry 3 }

 vrrpTrackInterStandByIfIdx OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The interface index of VRRP groups."
     ::= { vrrpTrackInterEntry 4 }
     
 vrrpTrackInterVrid OBJECT-TYPE
     SYNTAX       VrId
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
        "This object contains the Virtual Router Identifier (VRID) of the
        tracked interface."
     ::= { vrrpTrackInterEntry 5 }

 vrrpTrackInterIfIdx OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The index of the tracked interface."
     ::= { vrrpTrackInterEntry 6 }  

zxr10VrrpTrackInterPolicy OBJECT-TYPE
    SYNTAX          INTEGER { priorityDecrement(1), rapidDown(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "The policy of tracking interface."
    ::= { vrrpTrackInterEntry 7 }

-- *******************************************************************
--  VRRP Track Table
-- *******************************************************************

 vrrpTrackTable OBJECT-TYPE
     SYNTAX       SEQUENCE OF VrrpTrackEntry
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The table of VRRP track associated with this virtual router."
     ::= { vrrpTrackManages 2 }

 vrrpTrackEntry OBJECT-TYPE
     SYNTAX       VrrpTrackEntry
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "An entry in the table contains an VRRP track that is
         associated with a virtual router. The number of rows for
         a given ifIndex and VrId will equal the number of VRRP track
         associated by the virtual router.

         Rows in the table cannot be modified."
     INDEX    { ifIndex, VrId, vrrpTrackName }
     ::= { vrrpTrackTable 1 }

 VrrpTrackEntry ::=
     SEQUENCE {         
         vrrpTrackType
             INTEGER,
         vrrpDetectType
             DisplayString,
         vrrpPolicyType
             INTEGER,
         vrrpTrackPriReduce
             Integer32,
         vrrpTrackState
             INTEGER,         
         vrrpTrackOperRowStatus
             RowStatus,
         vrrpTrackName
             DisplayString,
         vrrpTrackVrid
             VrId,
         vrrpTrackStandByIfIdx
             Integer32    
 }
      
 vrrpTrackType OBJECT-TYPE
     SYNTAX INTEGER {
         object(1),
         group(2)
     }
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "The type of tracking for a virtual router."
     ::= { vrrpTrackEntry 1 }
     
 vrrpDetectType OBJECT-TYPE
     SYNTAX       DisplayString
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "The type of VRRP track detect for a virtual router."
     ::= { vrrpTrackEntry 2 }
     
 vrrpPolicyType OBJECT-TYPE
     SYNTAX INTEGER {
         normal(1),
         link(2),
         peer(3)
     }
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "The type of VRRP track policy for a virtual router."
     ::= { vrrpTrackEntry 3 }
     
 vrrpTrackPriReduce OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "The assigned reduced priority when VRRP track down for a virtual
         router."
     ::= { vrrpTrackEntry 4 }
 
 vrrpTrackState OBJECT-TYPE
    SYNTAX INTEGER {
         up(1),
         localdown(2),
         transdown(3),
         localsd(4),
         transsd(5),
         unknown(6)
     }     
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "The VRRP track state for a virtual router."
     ::= { vrrpTrackEntry 5 }
     
 vrrpTrackOperRowStatus OBJECT-TYPE
     SYNTAX       RowStatus
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "The row status variable, used in accordance to installation
         and removal conventions for conceptual rows. The rowstatus of
         a currently active row in the vrrpTrackTable is constrained
         by the tracking state of the corresponding virtual router.
         When `vrrpTrackOperRowStatus' is set to active(1), no other
         objects in the conceptual row.

         To create a row in this table, a manager sets this object
         to either createAndGo(4) or createAndWait(5). Until instances
         of all corresponding columns are appropriately configured,
         the value of the corresponding instance of the `vrrpTrackOperRowStatus'
         column will be read as notReady(3).

         In particular, a newly created row cannot be made active(1)
         until (minimally) the corresponding instance of
         `VrrpTrackName' has been set and there is at least one active
         row in the `VrrpTrackTable'."
     ::= { vrrpTrackEntry 6 }
     
 vrrpTrackName OBJECT-TYPE
     SYNTAX       DisplayString
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The name of VRRP track for a virtual router."
     ::= { vrrpTrackEntry 7 }
     
 vrrpTrackVrid OBJECT-TYPE
     SYNTAX       VrId
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "This object contains the Virtual Router Identifier (VRID) of the
         VRRP track."
     ::= { vrrpTrackEntry 8 }

 vrrpTrackStandByIfIdx OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The interface index of VRRP groups."
     ::= { vrrpTrackEntry 9 } 

-- *******************************************************************
--  VRRP Group Manages
-- *******************************************************************

-- *******************************************************************
--  VRRP Group Manage Table
-- *******************************************************************

 vrrpManageCfgTable OBJECT-TYPE
     SYNTAX       SEQUENCE OF VrrpManageCfgEntry
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "Table of virtual router manage."
     ::= { vrrpGroupManages 1 }

 vrrpManageCfgEntry OBJECT-TYPE
     SYNTAX       VrrpManageCfgEntry
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "An entry in the table, containing manage information
         about a given virtual router."
     INDEX    { mVrrpCfgIfIndex, mVrrpCfgVRID }
     ::= { vrrpManageCfgTable 1 }

 VrrpManageCfgEntry ::=
     SEQUENCE {
         mCfgOperRowStatus
             RowStatus,
         mVrrpCfgIfIndex
             Integer32,
         mVrrpCfgVRID
             Integer32                  
     }
     
 mCfgOperRowStatus OBJECT-TYPE
     SYNTAX       RowStatus
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "The row status variable, used according to installation
         and removal conventions for conceptual rows. Setting this
         object to active(1) or createAndGo(4) results in the
         addition of an associated address for a virtual router.
         Destroying the entry or setting it to notInService(2)
         removes the manage info from the virtual router.
         The use of other values is implementation-dependent."
     ::= { vrrpManageCfgEntry 1 }
     
 mVrrpCfgIfIndex OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The interface index of manage VRRP groups."
     ::= { vrrpManageCfgEntry 2 }
     
 mVrrpCfgVRID OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The vrid of the manage VRRP groups."
     ::= { vrrpManageCfgEntry 3 } 
 
-- *******************************************************************
--  VRRP Group Member Table
-- *******************************************************************

 vrrpMemberCfgTable OBJECT-TYPE
     SYNTAX       SEQUENCE OF VrrpMemberCfgEntry
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The member table of the VRRP manage group."
     ::= { vrrpGroupManages 2 }

 vrrpMemberCfgEntry OBJECT-TYPE
     SYNTAX       VrrpMemberCfgEntry
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "An entry in the table, containing member information
         about a given virtual router manage group."
     INDEX    { memberIfIndex, memberVRID, mVrrpIfIndex, mVrrpVRID }
     ::= { vrrpMemberCfgTable 1 }

 VrrpMemberCfgEntry ::=
     SEQUENCE {
         cfgOperRowStatus
             RowStatus,
         memberIfIndex
             Integer32,
         memberVRID
             Integer32,
         mVrrpIfIndex
             Integer32,
         mVrrpVRID
             Integer32         
     }
     
 cfgOperRowStatus OBJECT-TYPE
     SYNTAX       RowStatus
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
         "The row status variable, used according to installation
         and removal conventions for conceptual rows. Setting this
         object to active(1) or createAndGo(4) results in the
         addition of a member group for a virtual router manage group.
         Destroying the entry or setting it to notInService(2)
         removes the member group from the virtual router manage group.
         The use of other values is implementation-dependent."
     ::= { vrrpMemberCfgEntry 1 }
     
 memberIfIndex OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The member interface index of virtual router manage group."
     ::= { vrrpMemberCfgEntry 2 }
     
 memberVRID OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The member vrid of virtual router manage group"
     ::= { vrrpMemberCfgEntry 3 }
     
 mVrrpIfIndex OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The interface index of the virtual router manage group."
     ::= { vrrpMemberCfgEntry 4 }   
     
 mVrrpVRID OBJECT-TYPE
     SYNTAX       Integer32
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
         "The vrid of the virtual router manage group."
     ::= { vrrpMemberCfgEntry 5 }    

