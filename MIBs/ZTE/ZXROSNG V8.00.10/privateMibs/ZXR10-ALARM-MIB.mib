    zxr10AlmConfirmEnable OBJECT-TYPE
        SYNTAX INTEGER
            {
                enable(1),
                disable(2)
            }                
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Enable or disable the alarm confirm."
        ::= { zxr10AlmTrapConfirmCfg 1 }
        
    zxr10AlmConfirmTimeout    OBJECT-TYPE 
        SYNTAX INTEGER
            {
                interval10(10),
                interval20(20),
                interval60(60),
                interval120(120),
                interval180(180)
            }

        UNITS        "seconds"
        MAX-ACCESS   read-write
        STATUS       current
        DESCRIPTION       
            "Specifies the time-out value, in seconds, for an event to be
             confirmed.It must be set with zxr10AlmResendingTimes together."
        DEFVAL { interval20 }
        ::= { zxr10AlmTrapConfirmCfg 2 }
    
    zxr10AlmResendingTimes OBJECT-TYPE 
        SYNTAX       Integer32(1..3)
        MAX-ACCESS   read-write
        STATUS       current
        DESCRIPTION       
            "The resending event times when device does not receive 
            the event confirm message from the snmp manager.
            It must be set with zxr10AlmConfirmTimeout together."
        DEFVAL { 1 }
        ::= { zxr10AlmTrapConfirmCfg 3 }

zxr10AlmTrapConfirmOpt OBJECT IDENTIFIER ::= { zxr10AlarmMIB 25  }

    zxr10AlmConfirmFlowId OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS   read-write
        STATUS      current
        DESCRIPTION
            "The cleard Flow Id to be confirmed by a snmp manager.
            When a manager receive a trap with confirmation information,
            then the manager should set this mib object to inform the agent
            that this manager has got this event.And then the agent should
            remove this event from a unconfirmed event queue. It must be 
            set with zxr10AlmConfirmAlarmStatus together."
        ::= { zxr10AlmTrapConfirmOpt 1 }
        
    zxr10AlmConfirmAlarmStatus OBJECT-TYPE
        SYNTAX INTEGER
            {
                appear(1),
                disappear(2),
                update(3),
                notify(4)
            }                
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION "The alarm status of the event to be confirmed."
        ::= { zxr10AlmTrapConfirmOpt 2 }


