ZXR10-ALARM-MIB DEFINITIONS ::= BEGIN
    
IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE,
        Integer32, NOTIFICATION-TYPE,enterprises, Unsigned32
                FROM SNMPv2-SMI
        DisplayString,TEXTUAL-CONVENTION,RowStatus
                FROM SNMPv2-TC
        OBJECT-GROUP, NOTIFICATION-GROUP
                FROM SNMPv2-CONF;
  
    zxr10AlarmMIB MODULE-IDENTITY
        LAST-UPDATED "201812120000Z"
        ORGANIZATION "ZTE Corp."
        CONTACT-INFO
               "ZTE"
        DESCRIPTION 
                "The MIB module to describe the Alarm."

        REVISION "201812120000Z"
        DESCRIPTION
                "Add enum value fiveMinutes to AlarmThAlmPeriod."

        REVISION "201809170000Z"
        DESCRIPTION
                "Add curAlarmNeedConfirm to curAlarmTable.
                Add notificationNeedConfirm to notificationTable.
                 Add zxr10AlmTrapConfirmCfg.
                 Add zxr10AlmTrapConfirmOpt."

        REVISION "201809050000Z"
        DESCRIPTION
               "add curAlarmOccuredMs, curAlarmUpdatedMs and curAlarmDisappearedMs to CurAlarmEntry,
                add hisAlarmOccuredMs, hisAlarmUpdatedMs and hisAlarmDisappearedMs to HisAlarmEntry,
                add notificationAlmOccuredMs to NotificationEntry,
                add globalAlarmOccuredMs, globalAlarmUpdatedMs and globalAlarmDisappearedMs to GlobalAlarmTrapEntry,
                add curAlarmOccuredMs, curAlarmUpdatedMs and curAlarmDisappearedMs to curAlarmTrap,
                add notificationAlmOccuredMs to notificationTrap."        
        REVISION "201808020000Z"
        DESCRIPTION
                "Modify length of curAlarmParaValue, hisAlarmParaValue, notificationAlmParaType and globalAlarmParaValue from 769 to 1537,
                 Modify length of curAlarmDescription, hisAlarmDescription, notificationAlmParaValue and globalAlarmDescription from 256 to 512."
        REVISION "201802080000Z"
        DESCRIPTION
                "Add zxr10AlmShdProfTable, zxr10AlmShdProfCodeTable and zxr10AlmShdProfApplyTable."
        REVISION "201709050000Z"
        DESCRIPTION
                "Add alarmLevelChangeRowStatus to AlarmLevelChangeEntry."
        REVISION "201705270000Z"
        DESCRIPTION
                "Add alarmHeartBeatGlobalObjects and alarmHeartBeatNotification."
        REVISION "201512310000Z"
        DESCRIPTION
                "Modify length of Tpid to 'from 62 to 128',
                add curAlarmTPIDLen, curAlarmTPIDLenSwitchFrom and curAlarmTPIDLenSwitchTo to CurAlarmEntry,
                add hisAlarmTPIDLen, hisAlarmTPIDLenSwitchFrom and hisAlarmTPIDLenSwitchTo to HisAlarmEntry,
                add notificationTPIDLen to NotificationEntry,
                add globalAlarmTPIDLen, globalAlarmTPIDLenSwitchFrom and globalAlarmTPIDLenSwitchTo to GlobalAlarmTrapEntry,
                add curAlarmTPIDLen, curAlarmTPIDLenSwitchFrom and curAlarmTPIDLenSwitchTo to curAlarmTrap,
                add notificationTPIDLen to notificationTrap,
                obsolete alarmMaskTable,
                add zxr10AlarmMaskTable,
                modify description of all node, capital first letter and add punctuation at end."
        REVISION "201511130000Z"
        DESCRIPTION
                "Modify length of curAlarmNetID, hisAlarmNetID and notificationAlmNetID from 32 to 64."
        REVISION "201510201033Z"
        DESCRIPTION
                "Add enum value of AlarmThAlmType."
        REVISION "201502270000Z"
        DESCRIPTION
                "Modify description of notificationAlmLevel, 
                globalAlarmOccuredTime, globalAlarmDisappearedTime
                and globalAlarmUpdatedTime."
        REVISION "201409040000Z"
        DESCRIPTION
                "Modify description of curAlarmTrap and notificationTrap."
        REVISION "201404150000Z"
        DESCRIPTION
                "Add alarmShieldTable."
        REVISION "201311260000Z"
        DESCRIPTION
                "Obsolete alarmLevelTable,
                add alarmLevelChangeTable." 
        REVISION "201307260000Z"
        DESCRIPTION
                "Add preload trap." 
        REVISION "201307100000Z"
        DESCRIPTION
                "Add alarm mask table and timestamp node, 
                modify alarmClearStatus node attribute."        
        REVISION "201304100000Z"
        DESCRIPTION
                "Add alarm node."
        REVISION "200905200000Z"
        DESCRIPTION
                "zxr10AlarmMIB."
        ::= { zxr10 302 }

