        ::= { zxr10PVlanCommunityPortEntry 1}
    
    zxr10PVlanCommunityPort OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..31))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The community port name, the max number is 16."
        ::= { zxr10PVlanCommunityPortEntry 2 } 
        
