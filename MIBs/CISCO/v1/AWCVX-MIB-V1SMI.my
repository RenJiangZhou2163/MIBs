    userMgrUserName DisplayString,
    userMgrPassword OCTET STRING,
    userMgrCapabilities Gauge,
    userMgrStatus RowStatus
    }

userMgrUserIndex OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The index of the user."
