    REFERENCE
        "ITU-T G.997.1, paragraph 7.3.2.1"
    ::= { adsl2ChConfProfileEntry 5 }

adsl2ChConfProfMaxDataRateDs OBJECT-TYPE
    SYNTAX Gauge(0..200000000)
