ciscoIkeConfigOperStateChanged TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoIkeConfigMIB
    VARIABLES {
        cicIkeEnabled }
--  Status
--    mandatory
    DESCRIPTION
        "
        The notification is generated when the operational
        state of IKE entity on the managed device has
        been changed."
    ::= 1

ciscoIkeConfigPskAdded TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoIkeConfigMIB
    VARIABLES {
        cicIkeCfgPskRemIdentType, cicIkeCfgPskRemIdentity }
--  Status
--    mandatory
    DESCRIPTION
        "
        This notification is generated when a new preshared
        key is configured on the managed device."
    ::= 2

ciscoIkeConfigPskDeleted TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoIkeConfigMIB
    VARIABLES {
        cicIkeCfgPskRemIdentType, cicIkeCfgPskRemIdentity }
--  Status
--    mandatory
    DESCRIPTION
        "
        This notification is generated when an existing
        preshared key is configured on the managed device is
        about to be deleted."
    ::= 3

ciscoIkeConfigPolicyAdded TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoIkeConfigMIB
    VARIABLES {
        cicIkeCfgPolicyEncr, cicIkeCfgPolicyHash, 
        cicIkeCfgPolicyAuth, cicIkeCfgPolicyDHGroup }
--  Status
--    mandatory
    DESCRIPTION
        "
        This notification is generated when a new ISAKMP
        policy is configured on the managed device."
    ::= 4

ciscoIkeConfigPolicyDeleted TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoIkeConfigMIB
    VARIABLES {
        cicIkeCfgPolicyEncr, cicIkeCfgPolicyHash, 
        cicIkeCfgPolicyAuth, cicIkeCfgPolicyDHGroup }
--  Status
--    mandatory
    DESCRIPTION
        "
        This notification is issued when an existing ISAKMP
        policy configured on the managed device is about
        to be deleted."
    ::= 5

cicIkeCfgOperGroup OBJECT IDENTIFIER ::= { cicIkeCfgMIBGroups 1 }
-- OBJECT-GROUP
--  Status
--    mandatory
--  Descr
--    
--    This group consists of objects that reflect the
--    operational state of the IKE entity on the
--    managed device.
--  objects
--    cicIkeEnabled, cicIkeAggressModeEnabled

cicIkeCfgIdentitiesGroup OBJECT IDENTIFIER ::= { cicIkeCfgMIBGroups 2 }
-- OBJECT-GROUP
--  Status
--    mandatory
--  Descr
--    
--    This group consists of objects that reflect the
--    Phase 1 ID used by the IKE entity on the
--    managed device.
--  objects
--    cicIkeCfgIdentityType, cicIkeCfgInitiatorNextAvailIndex, 
--    cicIkeCfgInitiatorPAddrType, cicIkeCfgInitiatorPAddr, 
--    cicIkeCfgInitiatorVer, cicIkeCfgInitiatorStatus

cicIkeCfgFailureRecoveryGroup OBJECT IDENTIFIER ::= { cicIkeCfgMIBGroups 3 }
-- OBJECT-GROUP
--  Status
--    mandatory
--  Descr
--    
--    This group consists of objects that define how the
--    local IKE entity is configured to respond to
--    common failures.
--  objects
--    cicIkeKeepAliveEnabled, cicIkeKeepAliveType, 
--    cicIkeKeepAliveInterval, cicIkeKeepAliveRetryInterval, 
--    cicIkeInvalidSpiNotify

cicIkeCfgPskAuthGroup OBJECT IDENTIFIER ::= { cicIkeCfgMIBGroups 4 }
-- OBJECT-GROUP
--  Status
--    mandatory
--  Descr
--    
--    This group consists of objects that are used to
--    view and configure the preshared keys configured on
--    the managed entity.
--  objects
--    cicIkeCfgPskNextAvailIndex, cicIkeCfgPskKey, 
--    cicIkeCfgPskRemIdentType, cicIkeCfgPskRemIdentTypeStand, 
--    cicIkeCfgPskRemIdentity, cicIkeCfgPskRemIdAddrOrRg1OrSn, 
--    cicIkeCfgPskRemIdAddrRange2, cicIkeCfgPskRemIdSubnetMask, 
--    cicIkeCfgPskStatus

cicIkeCfgPolicyGroup OBJECT IDENTIFIER ::= { cicIkeCfgMIBGroups 5 }
-- OBJECT-GROUP
--  Status
--    mandatory
--  Descr
--    
--    This group consists of objects that are used to
--    view and configure the ISAKMP policies configured on
--    the managed device.
--  objects
--    cicIkeCfgPolicyEncr, cicIkeCfgPolicyHash, cicIkeCfgPolicyPRF, 
--    cicIkeCfgPolicyAuth, cicIkeCfgPolicyDHGroup, 
--    cicIkeCfgPolicyLifetime, cicIkeCfgPolicyStatus

cicIkeCfgOptionalPolicyGroup OBJECT IDENTIFIER ::= { cicIkeCfgMIBGroups 6 }
