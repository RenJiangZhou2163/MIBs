CApsStatusEntry ::= SEQUENCE {
    cApsStatusK1K2Rcv CApsK1K2,
    cApsStatusK1K2Trans CApsK1K2,
    cApsStatusCurrent OCTET STRING,
    cApsStatusModeMismatches Counter,
    cApsStatusChannelMismatches Counter,
    cApsStatusPSBFs Counter,
