    ENTERPRISE ciscoTrustSecIfMIB
    VARIABLES {
        ifName, ctsiIfNotifMessage }
--  Status
--    mandatory
    DESCRIPTION
        "A ctsiIfSapNegotiationFailNotif is generated when a SAP
        negotiation error with the peer is detected for an interface."
