ctsiAuthorizationFailNotif TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoTrustSecIfMIB
    VARIABLES {
        ctsiAuthorizationPeerSgt }
--  Status
--    mandatory
    DESCRIPTION
        "A ctsiAuthorizationFailNotif is generated when the policy
        acquisition failed for the peer."
    ::= 1

ctsiIfAddSupplicantFailNotif TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoTrustSecIfMIB
    VARIABLES {
        ifName }
--  Status
--    mandatory
    DESCRIPTION
        "A ctsiIfAddSupplicantFailNotif is generated when the system
        fails to add dot1x supplicant for an interface."
    ::= 2

ctsiIfAuthenticationFailNotif TRAP-TYPE
