    REFERENCE
        "{ISIS.aoi areaAddresses (18)}"
    ::= { isisSystem 3 }

isisAreaAddrEntry OBJECT-TYPE
    SYNTAX IsisAreaAddrEntry
    ACCESS not-accessible
    STATUS mandatory
