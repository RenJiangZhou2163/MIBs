    DEFVAL { 20 }
    ::= { isisSummAddrEntry 5 }

isisSummAddrFullMetric OBJECT-TYPE
    SYNTAX IsisFullMetric
--    Rsyntax Gauge
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The wide metric value to announce this summary
