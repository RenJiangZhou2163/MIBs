ciscoCryAccelInserted TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoCryptoAcceleratorMIB
    VARIABLES {
        ccaAcclSlot }
--  Status
--    mandatory
    DESCRIPTION
        "A crypto accelerator module has been inserted into the
         managed device."
    ::= 1

ciscoCryAccelRemoved TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoCryptoAcceleratorMIB
    VARIABLES {
        ccaAcclSlot }
--  Status
--    mandatory
    DESCRIPTION
        "A crypto accelerator module has been removed from the
         managed device."
    ::= 2

ciscoCryAccelOperational TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoCryptoAcceleratorMIB
    VARIABLES {
        ccaAcclSlot }
--  Status
--    mandatory
    DESCRIPTION
        "A crypto accelerator module has become operational."
    ::= 3

ciscoCryAccelDisabled TRAP-TYPE
-- Reverse mappable trap
    ENTERPRISE ciscoCryptoAcceleratorMIB
    VARIABLES {
        ccaAcclSlot, ccaAcclStatus, ccaAcclActiveTime }
--  Status
--    mandatory
    DESCRIPTION
        "A crypto accelerator module has become non-operational."
    ::= 4

