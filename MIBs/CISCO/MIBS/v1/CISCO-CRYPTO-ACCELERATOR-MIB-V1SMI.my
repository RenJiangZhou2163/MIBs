    ::= { ccaProtocolStatsTable 1 }

CcaProtocolStatsEntry ::= SEQUENCE {
    ccaProtId CAProtocolType,
    ccaProtPktEncryptsReqs Counter,
    ccaProtPktDecryptsReqs Counter,
    ccaProtHmacCalcReqs Counter,
    ccaProtSaCreateReqs Counter,
    ccaProtSaRekeyReqs Counter,
    ccaProtSaDeleteReqs Counter,
    ccaProtPktEncapReqs Counter,
    ccaProtPktDecapReqs Counter,
    ccaProtNextPhaseKeyAllocReqs Counter,
    ccaProtRndGenReqs Counter,
    ccaProtFailedReqs Counter
    }

ccaProtId OBJECT-TYPE
    SYNTAX CAProtocolType
--    Rsyntax INTEGER {
--        other(1),
--        ikev1(2),
--        ikev2(3),
--        ipsec(4),
--        ssl(5),
--        ssh(6),
--        srtp(7)
--        }
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The index uniquely identifies the security protocol
        for which this row summarizes the statistics."
    ::= { ccaProtocolStatsEntry 1 }

