--        rsaRxPowerDiversityAnt(6),
--        rsaChDelaySpreadMainAnt(7),
--        rsaChDelaySpreadDiversityAnt(8),
--        rsaHeAmbientNoise(9),
--        rsaSuRxPowerDeltaMainAnt(10),
--        rsaSuRxPowerDeltaDiversityAnt(11),
--        rsaSuTotalTxPower(12)
--        }
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "This objects used to index this table.  It represents the
         attribute that is being histogrammed."
    ::= { p2mpHistCtrlEntry 2 }

p2mpHistSize OBJECT-TYPE
    SYNTAX INTEGER {
        fine(1),
        coarse(2)
        }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "This object lets the user select the granularity of the 
        histogram. The number of bins used for the capture in 
        each category is hardware dependent."
    ::= { p2mpHistCtrlEntry 3 }

p2mpHistSumScale OBJECT-TYPE
    SYNTAX CwrFixedPointScale
--    Rsyntax INTEGER {
--        yocto(1),
--        zepto(2),
--        atto(3),
--        femto(4),
--        pico(5),
