    InetAddressType, InetAddress, InetPortNumber
                                        FROM INET-ADDRESS-MIB
                                             -- [rfc4001]

    spdActions, SpdIPPacketLogging, spdEndGroupInterface
                                        FROM IPSEC-SPD-MIB
                                             -- [rfcZZZZ]
    ;

