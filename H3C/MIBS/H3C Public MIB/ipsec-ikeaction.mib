    MODULE-IDENTITY, OBJECT-TYPE, Integer32, Unsigned32
                                        FROM SNMPv2-SMI
                                             -- [rfc2578]

    TEXTUAL-CONVENTION, RowStatus, TruthValue,
    TimeStamp, StorageType, VariablePointer
                                        FROM SNMPv2-TC
                                             -- [rfc2579]

    MODULE-COMPLIANCE, OBJECT-GROUP
                                        FROM SNMPv2-CONF
                                             -- [rfc2580]

    SnmpAdminString
                                        FROM SNMP-FRAMEWORK-MIB
